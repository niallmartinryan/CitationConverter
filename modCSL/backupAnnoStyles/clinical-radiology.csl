<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" page-range-format="minimal" version="1.0">
  <info>
    <title>Clinical Radiology</title>
    <id>http://www.zotero.org/styles/clinical-radiology</id>
    <link href="http://www.zotero.org/styles/clinical-radiology" rel="self"/>
    <link href="http://www.zotero.org/styles/elsevier-vancouver" rel="template"/>
    <link href="https://www.rcr.ac.uk/sites/default/files/instructions_authors_cr.pdf" rel="documentation"/>
    <author>
      <name>Patrick O'Brien</name>
      <email>obrienpat86@gmail.com</email>
    </author>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <issn>0009-9260</issn>
    <eissn>1365-229X</eissn>
    <updated>2017-04-02T14:00:48+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <names variable="author">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <et-al font-style="italic"/>
      <label form="long" prefix=", "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names variable="editor">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="short" prefix=", " suffix="."/>
    </names>
  </macro>
  <macro name="year-date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </if>
      <else>
        <text form="short" term="no date"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <text suffix=": " text-case="title" variable="publisher-place"/>
    <text suffix="; " variable="publisher"/>
    <text macro="year-date"/>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="access">
    <choose>
      <if variable="DOI">
        <text prefix="https://doi.org/" variable="DOI"/>
      </if>
      <else-if match="any" type="webpage post-weblog">
        <choose>
          <if variable="URL">
            <text variable="URL"/>
            <group delimiter=" " prefix=" (" suffix=")">
              <text term="accessed"/>
              <date form="text" variable="accessed"/>
            </group>
          </if>
        </choose>
      </else-if>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
      <key variable="author"/>
    </sort>
    <layout delimiter=",">
      <text font-style="normal" variable="citation-number"/>
    </layout>
  </citation>
  <bibliography entry-spacing="0" et-al-min="7" et-al-use-first="3" second-field-align="flush">
    <layout suffix=".">
      <text prefix=" $$ " suffix=". $/$ " variable="citation-number"/>
      <text macro="author" prefix=" $author$ " suffix=".  $/author$ "/>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=". ">
            <text prefix=" $$ " suffix=" $/$ " variable="title"/>
            <text prefix=" $$ vol. " suffix=" $/$ " variable="volume"/>
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <group>
            <group>
              <text prefix=" $$ " suffix=".  $/$ " variable="title"/>
              <group>
                <text prefix=" $$ " suffix=":  $/$ " term="in" text-case="sentence"/>
                <text macro="editor" prefix=" $editor$ " suffix=".  $/editor$ "/>
              </group>
              <group delimiter=", " suffix=". ">
                <text form="short" prefix=" $$ " suffix=" $/$ " variable="container-title"/>
                <text prefix=" $$ vol. " suffix=" $/$ " variable="volume"/>
              </group>
              <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            </group>
            <text macro="publisher" prefix=" $publisher$ " suffix="; $/publisher$ "/>
            <text prefix=" $$ " suffix=" $/$ " variable="page"/>
          </group>
        </else-if>
        <else-if type="patent">
          <group delimiter=", ">
            <group delimiter=". ">
              <text prefix=" $$ " suffix=" $/$ " variable="title"/>
              <text prefix=" $$ " suffix=" $/$ " variable="number"/>
            </group>
            <text macro="year-date" prefix=" $year-date$ " suffix=" $/year-date$ "/>
          </group>
        </else-if>
        <else-if type="thesis">
          <group delimiter=". ">
            <text prefix=" $$ " suffix=" $/$ " variable="title"/>
            <text prefix=" $$ " suffix=" $/$ " variable="genre"/>
            <group delimiter=", ">
              <text prefix=" $$ " suffix=" $/$ " variable="publisher"/>
              <text macro="year-date" prefix=" $year-date$ " suffix=" $/year-date$ "/>
            </group>
          </group>
        </else-if>
        <else>
          <group delimiter=":">
            <group delimiter=" ">
              <group delimiter=". ">
                <text prefix=" $$ " suffix=" $/$ " variable="title"/>
                <text form="short" prefix=" $$ " strip-periods="true" suffix=" $/$ " text-case="title" variable="container-title"/>
              </group>
              <group delimiter=";">
                <text macro="year-date" prefix=" $year-date$ " suffix=" $/year-date$ "/>
                <group>
                  <text font-weight="bold" prefix=" $$ " suffix=" $/$ " variable="volume"/>
                  <text prefix=" $$ (" suffix=") $/$ " variable="issue"/>
                </group>
              </group>
            </group>
            <text form="short" prefix=" $$ " suffix=" $/$ " variable="page"/>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=" $access$ . " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>