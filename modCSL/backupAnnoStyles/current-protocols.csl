<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>Current Protocols journals</title>
    <id>http://www.zotero.org/styles/current-protocols</id>
    <link href="http://www.zotero.org/styles/current-protocols" rel="self"/>
    <link href="http://onlinelibrary.wiley.com/book/10.1002/0471250953/homepage/ForAuthors.html" rel="documentation"/>
    <author>
      <name>Sebastian Karcher</name>
    </author>
    <category citation-format="author-date"/>
    <category field="science"/>
    <summary>Style for the Wiley Series Current Protocols</summary>
    <updated>2012-09-27T22:06:38+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="editor">
    <names delimiter=", " variable="editor">
      <name and="text" delimiter=", " initialize-with=". "/>
      <label form="short" prefix=", "/>
    </names>
  </macro>
  <macro name="author">
    <names variable="author">
      <name and="text" delimiter=", " delimiter-precedes-last="always" initialize-with=". " name-as-sort-order="all" sort-separator=", "/>
      <label form="short" prefix=" "/>
      <substitute>
        <names variable="editor"/>
        <text variable="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="text" delimiter=", " form="short" initialize-with=". "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text variable="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <choose>
      <if variable="URL">
        <text suffix=" " value="Available at:"/>
        <text variable="URL"/>
        <group prefix=" [" suffix="]">
          <text suffix=" " term="accessed" text-case="capitalize-first"/>
          <date variable="accessed">
            <date-part name="month" suffix=" "/>
            <date-part name="day" suffix=", "/>
            <date-part name="year"/>
          </date>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="title">
    <text variable="title"/>
  </macro>
  <macro name="publisher">
    <group delimiter=", ">
      <text variable="publisher"/>
      <text variable="publisher-place"/>
    </group>
  </macro>
  <macro name="year-date">
    <date variable="issued">
      <date-part name="year"/>
    </date>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <citation collapse="year" disambiguate-add-year-suffix="true" et-al-min="3" et-al-use-first="1">
    <layout delimiter="; " prefix="(" suffix=")">
      <group delimiter=", ">
        <text macro="author-short"/>
        <text macro="year-date"/>
        <text variable="locator"/>
      </group>
    </layout>
  </citation>
  <bibliography et-al-min="11" et-al-use-first="10" hanging-indent="true">
    <sort>
      <key macro="author"/>
      <key variable="title"/>
    </sort>
    <layout suffix=".">
      <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
      <date prefix=" " suffix="." variable="issued">
        <date-part name="year"/>
      </date>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=" " prefix=" ">
            <group delimiter=", " suffix=".">
              <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
              <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            </group>
            <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <group delimiter=" " prefix=" ">
            <text macro="title" prefix=" $title$ " suffix=". $/title$ "/>
            <text font-style="italic" prefix=" $$ " suffix=" $/$ " term="in" text-case="capitalize-first"/>
            <group delimiter=", ">
              <text prefix=" $$ " suffix=" $/$ " variable="container-title"/>
              <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            </group>
            <text prefix=" $$ " suffix=". $/$ " variable="collection-title"/>
          </group>
          <text macro="editor" prefix=" $editor$  (" suffix=") $/editor$ "/>
          <group prefix=" " suffix=".">
            <label form="short" suffix=" " variable="page"/>
            <text prefix=" $$ " suffix=" $/$ " variable="page"/>
          </group>
          <text macro="publisher" prefix=" $publisher$  " suffix=" $/publisher$ "/>
        </else-if>
        <else>
          <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
          <group delimiter=":" prefix=" " suffix=".">
            <group delimiter=" ">
              <text font-style="italic" prefix=" $$ " suffix=" $/$ " variable="container-title"/>
              <text prefix=" $$ " suffix=" $/$ " variable="volume"/>
            </group>
            <text prefix=" $$ " suffix=" $/$ " variable="page"/>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=" $access$  " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>