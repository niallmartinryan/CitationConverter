<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>Entomologia Experimentalis et Applicata</title>
    <id>http://www.zotero.org/styles/entomologia-experimentalis-et-applicata</id>
    <link href="http://www.zotero.org/styles/entomologia-experimentalis-et-applicata" rel="self"/>
    <link href="http://onlinelibrary.wiley.com/journal/10.1111/%28ISSN%291570-7458/homepage/ForAuthors.html" rel="documentation"/>
    <author>
      <name>Adam Zeilinger</name>
      <email>arzeilinger@gmail.com</email>
    </author>
    <category citation-format="author-date"/>
    <category field="biology"/>
    <issn>0013-8703</issn>
    <eissn>1570-7458</eissn>
    <updated>2012-09-09T21:58:08+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <terms>
      <term form="verb-short" name="editor">ed. by</term>
      <term form="short" name="editor">
        <single>ed. by </single>
        <multiple>ed. by </multiple>
      </term>
    </terms>
  </locale>
  <macro name="editor-translator">
    <names delimiter=", " prefix="(" suffix=")" variable="editor translator">
      <name and="symbol" delimiter=", " initialize-with=""/>
      <label form="short" prefix=", " text-case="capitalize-first"/>
    </names>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" strip-periods="true" suffix="n." term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="author">
    <names variable="author">
      <name and="symbol" delimiter=", " delimiter-precedes-last="never" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="short" prefix=" (" strip-periods="true" suffix=")" text-case="capitalize-first"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text macro="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="symbol" delimiter=", " form="short" initialize-with=""/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
            <text form="short" variable="title"/>
          </if>
          <else>
            <text form="short" quotes="true" variable="title"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="title">
    <choose>
      <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
        <text variable="title"/>
      </if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=", ">
      <text variable="publisher"/>
      <text variable="publisher-place"/>
    </group>
  </macro>
  <citation collapse="year-suffix" disambiguate-add-names="true" disambiguate-add-year-suffix="true" et-al-min="3" et-al-use-first="1">
    <sort>
      <key variable="issued"/>
      <key variable="author"/>
    </sort>
    <layout delimiter="; " prefix="(" suffix=")">
      <group delimiter=", ">
        <text macro="author-short"/>
        <date variable="issued">
          <date-part name="year"/>
        </date>
        <group>
          <label form="short" variable="locator"/>
          <text prefix=" " variable="locator"/>
        </group>
      </group>
    </layout>
  </citation>
  <bibliography hanging-indent="true">
    <sort>
      <key macro="author-short"/>
      <key variable="issued"/>
    </sort>
    <layout>
      <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
      <date prefix=" (" suffix=")" variable="issued">
        <date-part name="year"/>
      </date>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group suffix=".">
            <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
            <text macro="editor-translator" prefix=" $editor-translator$  " suffix=" $/editor-translator$ "/>
          </group>
          <text macro="publisher" prefix=" $publisher$  " suffix=". $/publisher$ "/>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
          <group prefix=". ">
            <text prefix=" $$  " suffix=".  $/$ " variable="container-title"/>
            <names prefix="(" suffix=")" variable="editor translator">
              <label form="short" strip-periods="true"/>
              <name and="symbol" delimiter=", " delimiter-precedes-last="never" initialize-with="" sort-separator=" "/>
            </names>
            <text macro="edition" prefix=" $edition$  " suffix=" $/edition$ "/>
            <text prefix=" $$  " suffix=". $/$ " variable="collection-title"/>
            <group suffix=".">
              <text macro="publisher" prefix=" $publisher$  " suffix=", $/publisher$ "/>
              <group prefix=" ">
                <label form="short" strip-periods="true" suffix=" " variable="page"/>
                <text prefix=" $$ " suffix=" $/$ " variable="page"/>
              </group>
            </group>
          </group>
        </else-if>
        <else>
          <group suffix=".">
            <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
            <text macro="editor-translator" prefix=" $editor-translator$  " suffix=" $/editor-translator$ "/>
          </group>
          <group prefix=" " suffix=".">
            <text prefix=" $$ " suffix=" $/$ " variable="container-title"/>
            <group prefix=" ">
              <text prefix=" $$ " suffix=" $/$ " variable="volume"/>
            </group>
            <text prefix=" $$ :" suffix=" $/$ " variable="page"/>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>