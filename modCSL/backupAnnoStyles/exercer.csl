<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="fr-FR" demote-non-dropping-particle="sort-only" page-range-format="minimal" version="1.0">
  <info>
    <title>exercer (French)</title>
    <id>http://www.zotero.org/styles/exercer</id>
    <link href="http://www.zotero.org/styles/exercer" rel="self"/>
    <link href="http://www.zotero.org/styles/vancouver" rel="template"/>
    <link href="http://www.exercer.fr/recos_auteurs/" rel="documentation"/>
    <author>
      <name>Paul Frappe</name>
      <email>paul.frappe@univ-st-etienne.fr</email>
    </author>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <issn>0998-3953</issn>
    <updated>2014-10-16T10:34:03+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="fr">
    <date delimiter=" " form="text">
      <date-part name="day"/>
      <date-part form="short" name="month" strip-periods="true"/>
      <date-part name="year"/>
    </date>
    <terms>
      <term name="presented at">présenté à</term>
      <term name="retrieved">disponible</term>
      <term name="from">sur</term>
    </terms>
  </locale>
  <macro name="author">
    <names variable="author">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="long" prefix=", "/>
      <substitute>
        <names variable="editor"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <group delimiter=": ">
      <choose>
        <if match="any" type="chapter paper-conference">
          <text term="in" text-case="capitalize-first"/>
        </if>
      </choose>
      <names suffix="." variable="editor">
        <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
        <label form="long" prefix=", "/>
      </names>
    </group>
  </macro>
  <macro name="publisher">
    <group delimiter=": " suffix=",">
      <choose>
        <if type="thesis">
        </if>
        <else-if type="speech"/>
        <else>
          <text variable="publisher-place"/>
        </else>
      </choose>
      <text variable="publisher"/>
    </group>
  </macro>
  <macro name="access">
    <choose>
      <if variable="URL">
        <group delimiter=" : ">
          <group delimiter=" ">
            <text term="retrieved" text-case="capitalize-first"/>
            <text term="from"/>
          </group>
          <text suffix="." variable="URL"/>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="container-title">
    <choose>
      <if match="any" type="article-journal article-magazine chapter paper-conference article-newspaper">
        <group delimiter=" " suffix="">
          <choose>
            <if type="article-journal">
              <text form="short" strip-periods="true" variable="container-title"/>
            </if>
            <else-if match="any" type="chapter paper-conference">
              <text prefix="In: " strip-periods="true" variable="container-title"/>
            </else-if>
            <else>
              <text strip-periods="true" variable="container-title"/>
            </else>
          </choose>
          <choose>
            <if variable="URL">
              <text prefix="" suffix="" term="internet" text-case="capitalize-first"/>
            </if>
          </choose>
        </group>
        <text macro="edition" prefix=". "/>
      </if>
      <!--add event-name and event-place once they become available-->
      <else-if match="any" type="bill legislation">
        <group delimiter=", ">
          <group delimiter=". ">
            <group delimiter=" ">
              <text form="short" term="section" text-case="capitalize-first"/>
              <text variable="section"/>
            </group>
          </group>
          <text variable="number"/>
        </group>
      </else-if>
      <else-if type="speech">
        <group delimiter=": " suffix=";">
          <group delimiter=" ">
            <text text-case="capitalize-first" variable="genre"/>
            <text term="presented at"/>
          </group>
          <text variable="event"/>
        </group>
      </else-if>
      <else>
        <text suffix="." variable="container-title"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <text variable="title"/>
    <choose>
      <if match="none" type="article-journal article-magazine chapter paper-conference article-newspaper">
        <choose>
          <if variable="URL">
          </if>
        </choose>
        <text macro="edition" prefix=". "/>
      </if>
    </choose>
    <choose>
      <if type="thesis">
        <text prefix=". " suffix=" :" variable="genre"/>
      </if>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="date">
    <choose>
      <if match="any" type="article-journal article-magazine article-newspaper">
        <group delimiter=" " suffix=";">
          <date delimiter=" " variable="issued">
            <date-part name="year"/>
          </date>
        </group>
      </if>
      <else-if match="any" type="bill legislation">
      </else-if>
      <else-if type="report">
        <date delimiter=" " variable="issued">
          <date-part name="year"/>
          <date-part form="short" name="month" strip-periods="true"/>
        </date>
      </else-if>
      <else-if type="speech">
        <group delimiter="; ">
          <group delimiter=" ">
            <date delimiter=" " variable="issued">
              <date-part name="year"/>
              <date-part form="short" name="month" strip-periods="true"/>
              <date-part name="day"/>
            </date>
          </group>
          <text variable="event-place"/>
        </group>
      </else-if>
      <else-if type="chapter">
        <group suffix=":">
          <date variable="issued">
            <date-part name="year"/>
          </date>
        </group>
      </else-if>
      <else>
        <group suffix=".">
          <date variable="issued">
            <date-part name="year"/>
          </date>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="pages">
    <choose>
      <if match="any" type="article-journal article-magazine article-newspaper">
        <text prefix=":" variable="page"/>
      </if>
      <else>
        <group delimiter=" " prefix="">
          <text variable="page"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="journal-location">
    <choose>
      <if match="any" type="article-journal article-magazine">
        <text variable="volume"/>
      </if>
    </choose>
  </macro>
  <macro name="report-details">
    <choose>
      <if type="report">
        <text prefix="Report No.: " variable="number"/>
      </if>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="," vertical-align="sup">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography et-al-min="7" et-al-use-first="3" second-field-align="flush">
    <layout>
      <text prefix=" $$ " suffix=".  $/$ " variable="citation-number"/>
      <group delimiter=". " suffix=". ">
        <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
        <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
      </group>
      <group delimiter=" " suffix=". ">
        <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
        <text macro="container-title" prefix=" $container-title$ " suffix=" $/container-title$ "/>
        <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
        <group>
          <text macro="date" prefix=" $date$ " suffix=" $/date$ "/>
          <text macro="journal-location" prefix=" $journal-location$ " suffix=" $/journal-location$ "/>
          <text macro="pages" prefix=" $pages$ " suffix=" $/pages$ "/>
        </group>
      </group>
      <text macro="report-details" prefix=" $report-details$ " suffix=".  $/report-details$ "/>
      <text macro="access" prefix=" $access$ " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>