<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>Forest Science</title>
    <id>http://www.zotero.org/styles/forest-science</id>
    <link href="http://www.zotero.org/styles/forest-science" rel="self"/>
    <link href="http://www.zotero.org/styles/molecular-plant-microbe-interactions" rel="template"/>
    <link href="http://www.safnet.org/publications/forscience/guideforauthors.cfm" rel="documentation"/>
    <author>
      <name>Sebastian Karcher</name>
    </author>
    <category citation-format="author-date"/>
    <category field="biology"/>
    <issn>0015-749X</issn>
    <updated>2013-06-12T15:27:32+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="editor">
    <names delimiter=", " variable="editor">
      <name and="text" delimiter-precedes-last="always" initialize-with="." name-as-sort-order="first"/>
      <label form="short" prefix=" (" suffix=")"/>
    </names>
  </macro>
  <macro name="author">
    <names variable="author">
      <name and="text" delimiter=", " delimiter-precedes-last="always" initialize-with=". " name-as-sort-order="first" sort-separator=", "/>
      <label form="short" prefix=", "/>
      <substitute>
        <names variable="editor"/>
        <text variable="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="text" delimiter=", " form="short" initialize-with=". "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text variable="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <choose>
      <if variable="URL">
        <group>
          <text prefix="Available online at: " variable="URL"/>
          <group prefix=";">
            <text prefix=" last " term="accessed"/>
            <date prefix=" " variable="accessed">
              <date-part name="month" suffix=" "/>
              <date-part name="day" suffix=", "/>
              <date-part name="year"/>
            </date>
          </group>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
        <text font-style="italic" variable="title"/>
      </if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=", ">
      <text variable="genre"/>
      <text variable="publisher"/>
      <text variable="publisher-place"/>
    </group>
  </macro>
  <macro name="year-date">
    <group prefix=" ">
      <choose>
        <if variable="issued">
          <date variable="issued">
            <date-part name="year"/>
          </date>
        </if>
        <else>
          <text form="short" term="no date"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <citation collapse="year" disambiguate-add-year-suffix="true" et-al-min="3" et-al-use-first="1">
    <sort>
      <key macro="year-date"/>
      <key macro="author-short"/>
    </sort>
    <layout delimiter="; " prefix="(" suffix=")">
      <group delimiter=", ">
        <group delimiter=" ">
          <text macro="author-short"/>
          <text macro="year-date"/>
        </group>
        <text variable="locator"/>
      </group>
    </layout>
  </citation>
  <bibliography entry-spacing="0" et-al-min="11" et-al-use-first="7" hanging-indent="true">
    <sort>
      <key macro="author"/>
      <key variable="title"/>
    </sort>
    <layout suffix=".">
      <text macro="author" prefix=" $author$ " suffix=". $/author$ "/>
      <date prefix=" " suffix="." variable="issued">
        <date-part name="year"/>
      </date>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song thesis">
          <group delimiter=" " prefix=" ">
            <text macro="title" prefix=" $title$ " suffix=". $/title$ "/>
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
          </group>
          <text macro="publisher" prefix=" $publisher$  " suffix=". $/publisher$ "/>
          <text prefix=" $$  " suffix=" p. $/$ " variable="number-of-pages"/>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
          <group delimiter=" " prefix=" ">
            <group delimiter=" ">
              <label form="short" plural="never" text-case="capitalize-first" variable="page"/>
              <text prefix=" $$ " suffix=" $/$ " variable="page"/>
            </group>
            <text prefix=" $$ " suffix=" $/$ " term="in"/>
            <text font-style="italic" prefix=" $$ " suffix=", $/$ " variable="container-title"/>
            <text prefix=" $$ " suffix=". $/$ " variable="collection-title"/>
            <text macro="editor" prefix=" $editor$ " suffix=". $/editor$ "/>
            <group suffix=".">
              <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
            </group>
          </group>
        </else-if>
        <else>
          <group delimiter=" " prefix=" " suffix=".">
            <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
            <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
          </group>
          <group prefix=" " suffix=".">
            <text font-style="italic" form="short" prefix=" $$ " suffix=" $/$ " variable="container-title"/>
          </group>
          <group prefix=" ">
            <text prefix=" $$ " suffix=" $/$ " variable="volume"/>
            <text prefix=" $$ (" suffix=") $/$ " variable="issue"/>
            <text prefix=" $$ :" suffix=" $/$ " variable="page"/>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=" $access$  " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>