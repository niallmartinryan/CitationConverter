<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-GB" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>Genome Biology and Evolution</title>
    <id>http://www.zotero.org/styles/genome-biology-and-evolution</id>
    <link href="http://www.zotero.org/styles/genome-biology-and-evolution" rel="self"/>
    <link href="http://www.oxfordjournals.org/our_journals/gbe/for_authors/" rel="documentation"/>
    <author>
      <name>Jessica Leigh</name>
      <email>blackwednesday@gmail.com</email>
    </author>
    <category citation-format="author-date"/>
    <category field="biology"/>
    <eissn>1759-6653</eissn>
    <updated>2012-09-27T22:06:38+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="editor">
    <names variable="editor">
      <name and="symbol" delimiter=", " initialize-with="" name-as-sort-order="all" sort-separator=", ">
        <name-part name="family" text-case="capitalize-first"/>
        <name-part name="given" text-case="capitalize-first"/>
      </name>
      <label form="long" prefix=", "/>
    </names>
  </macro>
  <macro name="series-editor">
    <names variable="original-author">
      <label form="short" suffix=" " text-case="capitalize-first"/>
      <name and="symbol" delimiter=", "/>
    </names>
  </macro>
  <macro name="author">
    <names variable="author">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="short" prefix=", "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="symbol" delimiter=", " delimiter-precedes-last="never" form="short"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <choose>
      <if variable="DOI">
        <text prefix="doi: " variable="DOI"/>
      </if>
      <else>
        <choose>
          <if variable="URL">
            <text variable="URL"/>
            <group prefix=" (" suffix=")">
              <text suffix=" " term="accessed" text-case="capitalize-first"/>
              <date variable="accessed">
                <date-part name="month" suffix=" "/>
                <date-part name="day" suffix=", "/>
                <date-part name="year"/>
              </date>
            </group>
          </if>
        </choose>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
        <text font-style="italic" variable="title"/>
      </if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=": ">
      <text variable="publisher"/>
      <text variable="publisher-place"/>
    </group>
  </macro>
  <macro name="year-date">
    <date variable="issued">
      <date-part name="year"/>
    </date>
  </macro>
  <macro name="day-month">
    <date variable="issued">
      <date-part name="month"/>
      <date-part name="day" prefix=" "/>
    </date>
  </macro>
  <macro name="page">
    <label form="short" suffix=" " variable="page"/>
    <text variable="page"/>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <citation collapse="year" disambiguate-add-givenname="true" disambiguate-add-names="true" disambiguate-add-year-suffix="true" et-al-min="3" et-al-use-first="1">
    <layout delimiter="; " prefix="(" suffix=")">
      <group delimiter=", ">
        <group delimiter=" ">
          <text macro="author-short"/>
          <text macro="year-date"/>
        </group>
        <text variable="locator"/>
      </group>
    </layout>
  </citation>
  <bibliography et-al-min="6" et-al-use-first="1" hanging-indent="false">
    <sort>
      <key macro="author"/>
      <key variable="title"/>
    </sort>
    <layout suffix=".">
      <group delimiter=" ">
        <text macro="author" prefix=" $author$ " suffix=". $/author$ "/>
        <text macro="year-date" prefix=" $year-date$ " suffix=". $/year-date$ "/>
      </group>
      <choose>
        <if match="any" type="article-newspaper article-magazine">
          <group delimiter=" ">
            <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
          </group>
          <group delimiter=", " prefix=" ">
            <text prefix=" $$ " suffix=" $/$ " variable="container-title"/>
            <text macro="day-month" prefix=" $day-month$ " suffix=" $/day-month$ "/>
            <text prefix=" $$ " suffix=" $/$ " variable="edition"/>
          </group>
        </if>
        <else-if type="thesis">
          <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
          <group delimiter=", " prefix=" ">
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="editor" prefix=" $editor$ " suffix=". $/editor$ "/>
            <text prefix=" $$ " suffix=" $/$ " variable="genre"/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </else-if>
        <else-if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=" ">
            <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </else-if>
        <else-if match="any" type="chapter paper-conference">
          <group delimiter=" ">
            <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
            <group prefix="In: " suffix=".">
              <text prefix=" $$ " suffix=". $/$ " variable="container-title"/>
              <group delimiter=" ">
                <text macro="editor" prefix=" $editor$  " suffix=". $/editor$ "/>
                <text prefix=" $$ " suffix=" $/$ " variable="collection-title"/>
                <text prefix=" $$ Vol. " suffix=" $/$ " variable="volume"/>
                <text macro="series-editor" prefix=" $series-editor$ " suffix=" $/series-editor$ "/>
              </group>
              <text macro="publisher" prefix=" $publisher$  " suffix=" $/publisher$ "/>
              <text macro="page" prefix=" $page$  " suffix=" $/page$ "/>
            </group>
          </group>
        </else-if>
        <else>
          <group suffix=".">
            <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
            <text macro="editor" prefix=" $editor$  " suffix=" $/editor$ "/>
          </group>
          <group delimiter=" " prefix=" " suffix=".">
            <text form="short" prefix=" $$ " suffix=". $/$ " variable="container-title"/>
            <group delimiter=":">
              <text prefix=" $$ " suffix=" $/$ " variable="volume"/>
              <text prefix=" $$ " suffix=" $/$ " variable="page"/>
            </group>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=" $access$  " suffix=". $/access$ "/>
    </layout>
  </bibliography>
</style>