<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>Grasas y Aceites</title>
    <id>http://www.zotero.org/styles/grasas-y-aceites</id>
    <link href="http://www.zotero.org/styles/grasas-y-aceites" rel="self"/>
    <link href="http://www.zotero.org/styles/harvard-cite-them-right" rel="template"/>
    <link href="http://grasasyaceites.revistas.csic.es/index.php/grasasyaceites/about/submissions#authorGuidelines" rel="documentation"/>
    <author>
      <name>cleber barreto</name>
      <uri>http://www.mendeley.com/profiles/cleber-barreto/</uri>
    </author>
    <category citation-format="numeric"/>
    <category field="chemistry"/>
    <issn>0017-3495</issn>
    <eissn>1988-4214</eissn>
    <summary>Grasas y Aceites publishes, in English or in Spanish, original papers (neither previously published nor submitted for publication anywhere) on fats, oils, oleaginous seeds or fruits, and similar products, in the form of articles, notes, reviews, and informative papers</summary>
    <updated>2014-02-28T19:45:59+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="editor">
    <names delimiter=", " variable="editor">
      <name initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="short" plural="never" prefix=", (" suffix=")" text-case="capitalize-first"/>
    </names>
  </macro>
  <macro name="anon">
    <text form="short" strip-periods="true" term="anonymous" text-case="capitalize-first"/>
  </macro>
  <macro name="author">
    <names variable="author">
      <name delimiter-precedes-last="never" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="short" prefix=" " text-case="lowercase"/>
      <substitute>
        <names variable="editor"/>
        <text macro="anon"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="symbol" delimiter=", " delimiter-precedes-last="never" form="short" initialize-with=". "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text macro="anon"/>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <choose>
      <if match="all" type="webpage" variable="URL">
        <text suffix=" " value="Available at:"/>
        <text variable="URL"/>
        <group prefix=" [" suffix="]">
          <text suffix=" " term="accessed" text-case="capitalize-first"/>
          <date variable="accessed">
            <date-part name="month" suffix=" "/>
            <date-part name="day" suffix=", "/>
            <date-part name="year"/>
          </date>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if match="any" type="bill book graphic legal_case legislation motion_picture report song thesis">
        <text font-style="italic" variable="title"/>
      </if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=", ">
      <text variable="publisher"/>
      <text variable="publisher-place"/>
    </group>
  </macro>
  <macro name="year-date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </if>
      <else>
        <text form="short" term="no date"/>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="pages">
    <group>
      <label form="short" suffix=" " variable="page"/>
      <text variable="page"/>
    </group>
  </macro>
  <citation disambiguate-add-givenname="true" disambiguate-add-names="true" disambiguate-add-year-suffix="true" et-al-min="3" et-al-use-first="1">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="; " prefix="(" suffix=")">
      <group delimiter=", ">
        <group delimiter=" ">
          <text macro="author-short"/>
          <text macro="year-date"/>
        </group>
        <group>
          <label form="short" variable="locator"/>
          <text variable="locator"/>
        </group>
      </group>
    </layout>
  </citation>
  <bibliography et-al-min="4" et-al-use-first="1" hanging-indent="true">
    <sort>
      <key macro="author"/>
      <key variable="title"/>
    </sort>
    <layout>
      <text macro="author" prefix=" $author$ " suffix=". $/author$ "/>
      <date prefix=" " suffix="." variable="issued">
        <date-part name="year"/>
      </date>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=" " prefix=" " suffix=",">
            <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
          </group>
          <text macro="publisher" prefix=" $publisher$  " suffix=". $/publisher$ "/>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
          <group delimiter=" " prefix=" ">
            <text prefix=" $$ " suffix=" $/$ " term="in" text-case="lowercase"/>
            <text macro="editor" prefix=" $editor$ " quotes="false" suffix=" $/editor$ "/>
            <text font-style="italic" prefix=" $$ " suffix=". $/$ " variable="container-title"/>
            <text prefix=" $$ " suffix=". $/$ " variable="collection-title"/>
            <text prefix=" $$ " suffix=". $/$ " variable="event"/>
            <group delimiter=", " suffix=".">
              <text macro="publisher" prefix=" $publisher$  " suffix=" $/publisher$ "/>
              <text macro="pages" prefix=" $pages$ " suffix=" $/pages$ "/>
            </group>
          </group>
        </else-if>
        <else-if type="thesis">
          <group delimiter=". " prefix=" " suffix=".">
            <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
            <text prefix=" $$ " suffix=" $/$ " variable="genre"/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </else-if>
        <else-if match="any" type="patent">
          <group delimiter=". " suffix=".">
            <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
            <text prefix=" $$ " suffix=" $/$ " variable="number"/>
          </group>
        </else-if>
        <else>
          <group suffix=".">
            <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
            <text macro="editor" prefix=" $editor$  " suffix=" $/editor$ "/>
          </group>
          <group prefix=" " suffix=".">
            <text font-style="italic" form="short" prefix=" $$ " quotes="false" strip-periods="false" suffix=" $/$ " variable="container-title"/>
            <group font-weight="bold" prefix=" ">
              <text prefix=" $$ " suffix=" $/$ " variable="volume"/>
            </group>
            <group prefix=", ">
              <text prefix=" $$ " suffix=" $/$ " variable="page"/>
            </group>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=" $access$  " suffix=". $/access$ "/>
    </layout>
  </bibliography>
</style>