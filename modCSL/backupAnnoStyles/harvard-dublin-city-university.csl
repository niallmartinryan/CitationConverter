<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-GB" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>Dublin City University - Harvard</title>
    <title-short>Harvard (DCU)</title-short>
    <id>http://www.zotero.org/styles/harvard-dublin-city-university</id>
    <link href="http://www.zotero.org/styles/harvard-dublin-city-university" rel="self"/>
    <link href="http://www.zotero.org/styles/harvard-cite-them-right" rel="template"/>
    <link href="https://www101.dcu.ie/library/Citing&amp;ReferencingGuide/player.html" rel="documentation"/>
    <author>
      <name>Jack Hyland</name>
      <email>jack.hyland@dcu.ie</email>
    </author>
    <category citation-format="author-date"/>
    <category field="generic-base"/>
    <updated>2016-12-14T18:56:54+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="editor">
    <names delimiter=", " variable="editor">
      <name and="text" initialize-with=". "/>
      <label form="short" prefix=" (" suffix=")" text-case="lowercase"/>
    </names>
  </macro>
  <macro name="author">
    <names variable="author">
      <name and="text" delimiter-precedes-last="never" initialize-with="." name-as-sort-order="all"/>
      <label form="short" prefix=" (" suffix=")." text-case="lowercase"/>
      <substitute>
        <names prefix="(" suffix=")" variable="editor"/>
        <text macro="title" suffix="."/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="text" delimiter-precedes-last="never" form="short" initialize-with=". "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text macro="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <choose>
      <if match="any" type="article-magazine webpage report article-newspaper">
        <choose>
          <if variable="URL">
            <text suffix=" " value="Available from:"/>
            <text variable="URL"/>
            <group prefix=" [" suffix="]">
              <text suffix=" " term="accessed" text-case="capitalize-first"/>
              <date variable="accessed">
                <date-part name="month" suffix=" "/>
                <date-part name="day" suffix=", "/>
                <date-part name="year"/>
              </date>
            </group>
          </if>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if match="any" type="bill book graphic legal_case legislation motion_picture report song thesis">
        <text font-style="italic" suffix="." text-case="capitalize-first" variable="title"/>
      </if>
      <else>
        <text text-case="capitalize-first" variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=": ">
      <text variable="publisher-place"/>
      <text variable="publisher"/>
    </group>
  </macro>
  <macro name="year-date">
    <choose>
      <if variable="issued">
        <date font-variant="small-caps" variable="issued">
          <date-part name="year"/>
        </date>
      </if>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="pages">
    <group>
      <label form="short" suffix=" " variable="page"/>
      <text variable="page"/>
    </group>
  </macro>
  <citation disambiguate-add-givenname="true" disambiguate-add-names="true" disambiguate-add-year-suffix="true" et-al-min="4" et-al-use-first="1">
    <layout delimiter=", " prefix="(" suffix=")">
      <group delimiter=", ">
        <group delimiter=" ">
          <text macro="author-short"/>
          <text macro="year-date"/>
        </group>
        <group>
          <label form="short" variable="locator"/>
          <text variable="locator"/>
        </group>
      </group>
    </layout>
  </citation>
  <bibliography>
    <sort>
      <key macro="author"/>
      <key variable="title"/>
    </sort>
    <layout>
      <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
      <date prefix=" " suffix="." variable="issued">
        <date-part name="year"/>
      </date>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=" " prefix=" ">
            <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
          </group>
          <text macro="publisher" prefix=" $publisher$  " suffix=". $/publisher$ "/>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
          <group delimiter=" " prefix=" ">
            <text font-style="italic" prefix=" $$ " suffix=": $/$ " term="in" text-case="uppercase"/>
            <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
            <text font-style="italic" prefix=" $$ " suffix=". $/$ " variable="container-title"/>
            <text prefix=" $$ " suffix=". $/$ " variable="collection-title"/>
            <text prefix=" $$ " suffix=". $/$ " variable="event"/>
            <group delimiter=", " suffix=".">
              <text macro="publisher" prefix=" $publisher$  " suffix=" $/publisher$ "/>
              <text macro="pages" prefix=" $pages$ " suffix=" $/pages$ "/>
            </group>
          </group>
        </else-if>
        <else-if type="thesis">
          <group delimiter=". " prefix=" " suffix=".">
            <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
            <text prefix=" $$ " suffix=" $/$ " variable="genre"/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </else-if>
        <else>
          <group suffix=".">
            <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
            <text macro="editor" prefix=" $editor$  " suffix=" $/editor$ "/>
          </group>
          <group prefix=" " suffix=".">
            <text font-style="italic" prefix=" $$ " suffix=" $/$ " variable="container-title"/>
            <group prefix=", ">
              <text prefix=" $$ " suffix=" $/$ " variable="volume"/>
              <text prefix=" $$ (" suffix=") $/$ " variable="issue"/>
            </group>
            <group prefix=", ">
              <label form="short" variable="page"/>
              <text prefix=" $$ " suffix=" $/$ " variable="page"/>
            </group>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=" $access$  " suffix=". $/access$ "/>
    </layout>
  </bibliography>
</style>