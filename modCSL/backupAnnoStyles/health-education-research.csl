<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" page-range-format="minimal" version="1.0">
  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->
  <info>
    <title>Health Education Research</title>
    <id>http://www.zotero.org/styles/health-education-research</id>
    <link href="http://www.zotero.org/styles/health-education-research" rel="self"/>
    <link href="http://www.zotero.org/styles/elsevier-vancouver" rel="template"/>
    <link href="http://www.oxfordjournals.org/our_journals/healed/for_authors/index.html" rel="documentation"/>
    <author>
      <name>Center for Research on Media, Technology, and Health</name>
    </author>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <issn>0268-1153</issn>
    <eissn>1465-3648</eissn>
    <updated>2016-10-24T13:26:25+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <names variable="author">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="long" prefix=", "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names variable="editor">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="long" prefix=", " suffix="."/>
    </names>
  </macro>
  <macro name="year-date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </if>
      <else>
        <text form="short" term="no date"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <text suffix=": " text-case="title" variable="publisher-place"/>
    <text suffix="; " variable="publisher"/>
    <text macro="year-date"/>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text variable="edition"/>
      </else>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="," prefix="[" suffix="]">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography entry-spacing="0" et-al-min="4" et-al-use-first="3" second-field-align="flush">
    <layout suffix=".">
      <text prefix=" $$ " suffix=". $/$ " variable="citation-number"/>
      <text macro="author" prefix=" $author$ " suffix=".  $/author$ "/>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=". ">
            <text prefix=" $$ " suffix=" $/$ " variable="title"/>
            <text prefix=" $$ vol. " suffix=" $/$ " variable="volume"/>
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <group delimiter=", ">
            <group delimiter=". ">
              <text prefix=" $$ " suffix=" $/$ " variable="title"/>
              <group>
                <text prefix=" $$ " suffix=":  $/$ " term="in" text-case="sentence"/>
                <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
              </group>
              <group delimiter=", ">
                <text form="short" prefix=" $$ " suffix=" $/$ " variable="container-title"/>
                <text prefix=" $$ vol. " suffix=" $/$ " variable="volume"/>
              </group>
              <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            </group>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
            <group delimiter=" ">
              <label form="short" plural="never" variable="page"/>
              <text prefix=" $$ " suffix=" $/$ " variable="page"/>
            </group>
          </group>
        </else-if>
        <else-if type="patent">
          <group delimiter=", ">
            <group delimiter=". ">
              <text prefix=" $$ " suffix=" $/$ " variable="title"/>
              <text prefix=" $$ " suffix=" $/$ " variable="number"/>
            </group>
            <text macro="year-date" prefix=" $year-date$ " suffix=" $/year-date$ "/>
          </group>
        </else-if>
        <else-if type="thesis">
          <group delimiter=". ">
            <text prefix=" $$ " suffix=" $/$ " variable="title"/>
            <text prefix=" $$ " suffix=" $/$ " variable="genre"/>
            <group delimiter=", ">
              <text prefix=" $$ " suffix=" $/$ " variable="publisher"/>
              <text macro="year-date" prefix=" $year-date$ " suffix=" $/year-date$ "/>
            </group>
          </group>
        </else-if>
        <else-if type="webpage">
          <group delimiter=". ">
            <text prefix=" $$ " suffix=" $/$ " variable="title"/>
            <choose>
              <if variable="URL">
                <group delimiter=": ">
                  <text prefix=" $$ " suffix=" $/$ " term="available at" text-case="capitalize-first"/>
                  <text prefix=" $$ " suffix=" $/$ " variable="URL"/>
                </group>
                <group>
                  <text prefix=" $$ " suffix=" $/$ " term="accessed" text-case="capitalize-first"/>
                  <date form="text" variable="accessed"/>
                </group>
              </if>
            </choose>
          </group>
        </else-if>
        <else>
          <group delimiter=":">
            <group delimiter=" ">
              <group delimiter=". ">
                <text prefix=" $$ " suffix=" $/$ " variable="title"/>
                <text font-style="italic" form="short" prefix=" $$ " strip-periods="true" suffix=" $/$ " text-case="title" variable="container-title"/>
              </group>
              <group delimiter=";">
                <text macro="year-date" prefix=" $year-date$ " suffix=" $/year-date$ "/>
                <text font-weight="bold" prefix=" $$ " suffix=" $/$ " variable="volume"/>
              </group>
            </group>
            <text form="short" prefix=" $$ " suffix=" $/$ " variable="page"/>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>