<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="never" version="1.0">
  <info>
    <title>Journal of Management</title>
    <id>http://www.zotero.org/styles/journal-of-management</id>
    <link href="http://www.zotero.org/styles/journal-of-management" rel="self"/>
    <link href="http://www.zotero.org/styles/apa" rel="template"/>
    <link href="http://www.sagepub.com/upm-data/46742_JOM_Style_Guide_Revised_2011.pdf" rel="documentation"/>
    <author>
      <name>Charles Parnot</name>
      <uri>http://twitter.com/cparnot</uri>
      <email>charles.parnot@gmail.com</email>
    </author>
    <category citation-format="author-date"/>
    <category field="social_science"/>
    <issn>0149-2063</issn>
    <eissn>1557-1211</eissn>
    <updated>2012-09-27T22:06:38+00:00</updated>
