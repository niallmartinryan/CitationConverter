  </info>
  <locale xml:lang="en">
    <terms>
      <term form="verb-short" name="editor">ed. by</term>
      <term form="short" name="editor">
        <single>ed</single>
        <multiple>eds</multiple>
      </term>
    </terms>
  </locale>
  <macro name="editor-translator">
    <names delimiter=", " prefix="(" suffix=")" variable="editor translator">
      <name and="text" delimiter=", " initialize-with=""/>
      <label form="short" prefix=", " text-case="capitalize-first"/>
    </names>
  </macro>
  <macro name="author">
    <names variable="author">
      <name delimiter=", " delimiter-precedes-et-al="never" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="short" prefix=" (" strip-periods="true" suffix=")"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text macro="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="symbol" delimiter=", " form="short" initialize-with=". "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
            <text font-style="italic" form="short" variable="title"/>
          </if>
          <else>
            <text form="short" quotes="true" variable="title"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="title">
    <choose>
      <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
        <text font-style="italic" variable="title"/>
      </if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=", ">
      <text variable="publisher"/>
      <text variable="publisher-place"/>
    </group>
  </macro>
  <macro name="edition">
    <choose>
      <if match="any" type="bill book chapter graphic legal_case legislation motion_picture paper-conference report song">
        <choose>
          <if is-numeric="edition">
            <number form="ordinal" variable="edition"/>
            <text form="short" prefix=" " term="edition"/>
          </if>
          <else>
            <text suffix="." variable="edition"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <citation collapse="year-suffix" disambiguate-add-year-suffix="true" et-al-min="3" et-al-use-first="1">
    <sort>
      <key variable="issued"/>
      <key variable="author"/>
    </sort>
    <layout delimiter=", " prefix="(" suffix=")">
      <group delimiter=" ">
        <text macro="author-short"/>
        <date variable="issued">
          <date-part name="year"/>
        </date>
        <group>
          <label form="short" variable="locator"/>
          <text prefix=" " variable="locator"/>
        </group>
      </group>
    </layout>
  </citation>
  <bibliography et-al-min="10" et-al-use-first="6">
    <sort>
      <key macro="author-short"/>
      <key variable="issued"/>
    </sort>
    <layout>
      <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
      <date prefix=" (" suffix=")" variable="issued">
        <date-part name="year"/>
      </date>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=", " prefix=" " suffix=".">
            <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
          </group>
          <text macro="publisher" prefix=" $publisher$  " suffix=".  $/publisher$ "/>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
          <group prefix=" In: ">
            <names variable="editor translator">
              <name delimiter=", " delimiter-precedes-et-al="never" initialize-with="" name-as-sort-order="all" sort-separator=" " suffix=""/>
              <label form="short" prefix=" (" strip-periods="true" suffix=")"/>
            </names>
          </group>
          <group delimiter=", " prefix=" " suffix=".">
            <text font-style="italic" prefix=" $$ " suffix=" $/$ " variable="container-title"/>
            <text prefix=" $$ " suffix=" $/$ " variable="collection-title"/>
            <group delimiter=". ">
              <text prefix=" $$ " suffix=" $/$ " variable="page"/>
              <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
            </group>
          </group>
        </else-if>
        <else>
          <group prefix=" " suffix=".">
            <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
            <text macro="editor-translator" prefix=" $editor-translator$ " suffix=" $/editor-translator$ "/>
          </group>
          <group prefix=" " suffix=".">
            <text font-style="italic" prefix=" $$ " suffix=" $/$ " variable="container-title"/>
            <text prefix=" $$  " suffix=" $/$ " variable="volume"/>
            <text prefix=" $$ : " suffix=" $/$ " variable="page"/>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>