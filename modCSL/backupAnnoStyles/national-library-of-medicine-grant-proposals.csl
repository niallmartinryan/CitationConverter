<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>National Library of Medicine (grant proposals with PMCID/PMID)</title>
    <title-short>NLM</title-short>
    <id>http://www.zotero.org/styles/national-library-of-medicine-grant-proposals</id>
    <link href="http://www.zotero.org/styles/national-library-of-medicine-grant-proposals" rel="self"/>
    <link href="http://www.nlm.nih.gov/pubs/formats/recommendedformats1991-full.pdf" rel="documentation"/>
    <link href="http://publicaccess.nih.gov/citation_methods.htm" rel="documentation"/>
    <link href="http://grants.nih.gov/grants/funding/424/SF424_RR_Guide_General_Adobe_VerC.pdf" rel="documentation"/>
    <link href="http://grants.nih.gov/grants/funding/phs398/phs398.pdf" rel="documentation"/>
    <link href="http://www.ncbi.nlm.nih.gov/books/NBK7260/#A41037" rel="documentation"/>
    <author>
      <name>Michael Berkowitz</name>
      <email>mberkowi@gmu.edu</email>
    </author>
    <contributor>
      <name>Sebastian Karcher</name>
    </contributor>
    <contributor>
      <name>Matt Tracy</name>
    </contributor>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <summary>Adds required PMCID, or if absent adds optional PMID from extra field in bibliography</summary>
    <updated>2016-09-29T14:40:35+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <terms>
      <term name="presented at">presented at</term>
      <term name="retrieved">available</term>
      <term form="short" name="section">sect.</term>
    </terms>
  </locale>
  <macro name="author">
    <names variable="author">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="long" prefix=", "/>
      <substitute>
        <names variable="editor"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <group delimiter=": ">
      <choose>
        <if match="any" type="chapter paper-conference">
          <text term="in" text-case="capitalize-first"/>
        </if>
      </choose>
      <names suffix="." variable="editor">
        <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
        <label form="long" prefix=", "/>
      </names>
    </group>
  </macro>
  <macro name="publisher">
    <group delimiter=": " suffix=";">
      <choose>
        <if type="thesis">
          <text prefix="[" suffix="]" variable="publisher-place"/>
        </if>
        <else-if type="speech"/>
        <else>
          <text variable="publisher-place"/>
        </else>
      </choose>
      <text variable="publisher"/>
    </group>
  </macro>
  <macro name="access">
    <choose>
      <if variable="URL">
        <group delimiter=": ">
          <group delimiter=" ">
            <text term="retrieved" text-case="capitalize-first"/>
            <text term="from"/>
          </group>
          <text variable="URL"/>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="accessed-date">
    <choose>
      <if variable="URL">
        <group delimiter=" " prefix="[" suffix="]">
          <text term="cited"/>
          <date variable="accessed">
            <date-part name="year"/>
            <date-part form="short" name="month" prefix=" " strip-periods="true"/>
            <date-part name="day" prefix=" "/>
          </date>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="container-title">
    <choose>
      <if match="any" type="article-journal article-magazine chapter paper-conference article-newspaper">
        <group delimiter=" " suffix=".">
          <text form="short" strip-periods="true" variable="container-title"/>
          <choose>
            <if variable="URL">
              <text prefix="[" suffix="]" term="internet" text-case="capitalize-first"/>
            </if>
          </choose>
        </group>
        <text macro="edition" prefix=" "/>
      </if>
      <!--add event-name and event-place once they become available-->
      <else-if match="any" type="bill legislation">
        <group delimiter=", ">
          <group delimiter=". ">
            <text variable="container-title"/>
            <group delimiter=" ">
              <text form="short" term="section" text-case="capitalize-first"/>
              <text variable="section"/>
            </group>
          </group>
          <text variable="number"/>
        </group>
      </else-if>
      <else-if type="speech">
        <group delimiter=": " suffix=";">
          <group delimiter=" ">
            <text text-case="capitalize-first" variable="genre"/>
            <text term="presented at"/>
          </group>
          <text variable="event"/>
        </group>
      </else-if>
      <else>
        <text suffix="." variable="container-title"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <text variable="title"/>
    <choose>
      <if match="none" type="article-journal article-magazine chapter paper-conference article-newspaper">
        <choose>
          <if variable="URL">
            <text prefix=" [" suffix="]" term="internet" text-case="capitalize-first"/>
          </if>
        </choose>
        <text macro="edition" prefix=". "/>
      </if>
    </choose>
    <choose>
      <if type="thesis">
        <text prefix=" [" suffix="]" variable="genre"/>
      </if>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="date">
    <choose>
      <if match="any" type="article-journal article-magazine article-newspaper">
        <group delimiter=" " suffix=";">
          <date delimiter=" " variable="issued">
            <date-part name="year"/>
            <date-part form="short" name="month" strip-periods="true"/>
            <date-part name="day"/>
          </date>
          <text macro="accessed-date"/>
        </group>
      </if>
      <else-if match="any" type="bill legislation">
        <group delimiter=", ">
          <date delimiter=" " variable="issued">
            <date-part form="short" name="month" strip-periods="true"/>
            <date-part name="day"/>
          </date>
          <date variable="issued">
            <date-part name="year"/>
          </date>
        </group>
      </else-if>
      <else-if type="report">
        <date delimiter=" " variable="issued">
          <date-part name="year"/>
          <date-part form="short" name="month" strip-periods="true"/>
        </date>
      </else-if>
      <else-if type="patent">
        <group suffix=".">
          <group delimiter=", ">
            <text variable="number"/>
            <date variable="issued">
              <date-part name="year"/>
            </date>
          </group>
          <text macro="accessed-date" prefix=" "/>
        </group>
      </else-if>
      <else-if type="speech">
        <group delimiter="; ">
          <date delimiter=" " variable="issued">
            <date-part name="year"/>
            <date-part form="short" name="month" strip-periods="true"/>
            <date-part name="day"/>
          </date>
          <text variable="event-place"/>
        </group>
      </else-if>
      <else>
        <group suffix=".">
          <date variable="issued">
            <date-part name="year"/>
          </date>
          <text macro="accessed-date" prefix=" "/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="pages">
    <choose>
      <if match="any" type="article-journal article-magazine article-newspaper">
        <text prefix=":" variable="page"/>
      </if>
      <else>
        <text prefix=" p. " variable="page"/>
      </else>
    </choose>
  </macro>
  <macro name="pmcid">
    <text prefix=" PMCID: " variable="PMCID"/>
    <choose>
      <if match="none" variable="PMCID">
        <text prefix=" PMID: " variable="PMID"/>
      </if>
    </choose>
  </macro>
  <macro name="journal-location">
    <choose>
      <if match="any" type="article-journal article-magazine">
        <text variable="volume"/>
        <text prefix="(" suffix=")" variable="issue"/>
      </if>
    </choose>
  </macro>
  <macro name="report-details">
    <choose>
      <if type="report">
        <text prefix="Report No.: " variable="number"/>
      </if>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="," vertical-align="sup">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography second-field-align="flush">
    <layout>
      <text prefix=" $$ " suffix=".  $/$ " variable="citation-number"/>
      <group delimiter=". " suffix=". ">
        <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
        <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
      </group>
      <group delimiter=" " suffix=". ">
        <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
        <text macro="container-title" prefix=" $container-title$ " suffix=" $/container-title$ "/>
        <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
        <group>
          <text macro="date" prefix=" $date$ " suffix=" $/date$ "/>
          <text macro="journal-location" prefix=" $journal-location$ " suffix=" $/journal-location$ "/>
          <text macro="pages" prefix=" $pages$ " suffix=" $/pages$ "/>
        </group>
      </group>
      <text macro="report-details" prefix=" $report-details$ " suffix=".  $/report-details$ "/>
      <text macro="access" prefix=" $access$ " suffix=" $/access$ "/>
      <text macro="pmcid" prefix=" $pmcid$ " suffix=" $/pmcid$ "/>
    </layout>
  </bibliography>
</style>