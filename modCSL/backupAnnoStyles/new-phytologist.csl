<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-GB" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>New Phytologist</title>
    <id>http://www.zotero.org/styles/new-phytologist</id>
    <link href="http://www.zotero.org/styles/new-phytologist" rel="self"/>
    <link href="http://onlinelibrary.wiley.com/journal/10.1111/%28ISSN%291469-8137/homepage/ForAuthors.html" rel="documentation"/>
    <author>
      <name>Johan Asplund</name>
      <email>asplundj@gmail.com</email>
    </author>
    <category citation-format="author-date"/>
    <category field="biology"/>
    <issn>0028-646X</issn>
    <eissn>1469-8137</eissn>
    <updated>2017-04-29T19:45:13+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="editor-translator">
    <names delimiter=", " prefix="(" suffix=")" variable="editor translator">
      <name and="text" delimiter=", " initialize-with=""/>
      <et-al font-style="italic"/>
      <label form="short" prefix=", " text-case="capitalize-first"/>
    </names>
  </macro>
  <macro name="author">
    <names variable="author">
      <name delimiter=", " delimiter-precedes-last="always" font-weight="bold" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <et-al font-style="italic"/>
      <label form="short" prefix=" (" suffix=")" text-case="capitalize-first"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text macro="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="symbol" delimiter=", " form="short" initialize-with="."/>
      <et-al font-style="italic"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
            <text font-style="italic" form="short" variable="title"/>
          </if>
          <else>
            <text form="short" quotes="true" variable="title"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="title">
    <choose>
      <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
        <text font-style="italic" variable="title"/>
      </if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=": ">
      <text variable="publisher-place"/>
      <text variable="publisher"/>
    </group>
  </macro>
  <citation collapse="year-suffix" disambiguate-add-year-suffix="true" et-al-min="3" et-al-use-first="1" year-suffix-delimiter=",">
    <sort>
      <key variable="issued"/>
      <key variable="author"/>
    </sort>
    <layout delimiter="; " prefix="(" suffix=")">
      <group delimiter=", ">
        <text macro="author-short"/>
        <date variable="issued">
          <date-part name="year"/>
        </date>
        <group>
          <label form="short" variable="locator"/>
          <text prefix=" " variable="locator"/>
        </group>
      </group>
    </layout>
  </citation>
  <bibliography et-al-min="11" et-al-use-first="10" hanging-indent="false">
    <sort>
      <key macro="author-short"/>
      <key variable="issued"/>
    </sort>
    <layout suffix=".">
      <text font-weight="bold" macro="author" prefix=" $author$ " suffix=". $/author$ "/>
      <date font-weight="bold" prefix=" " suffix="." variable="issued">
        <date-part name="year"/>
      </date>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group suffix=".">
            <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
            <text macro="editor-translator" prefix=" $editor-translator$  " suffix=" $/editor-translator$ "/>
          </group>
          <text macro="publisher" prefix=" $publisher$  " suffix=". $/publisher$ "/>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
          <group delimiter=": " prefix=". ">
            <text prefix=" $$ " suffix=" $/$ " term="in" text-case="capitalize-first"/>
            <group delimiter=". ">
              <names variable="editor translator">
                <name delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
                <label form="short" prefix=", "/>
              </names>
              <text prefix=" $$ " suffix=" $/$ " variable="collection-title"/>
              <text prefix=" $$ " suffix=" $/$ " variable="container-title"/>
              <group delimiter=", ">
                <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
                <text prefix=" $$ " suffix=" $/$ " variable="page"/>
              </group>
            </group>
          </group>
        </else-if>
        <else>
          <group suffix=".">
            <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
            <text macro="editor-translator" prefix=" $editor-translator$  " suffix=" $/editor-translator$ "/>
          </group>
          <group prefix=" " suffix=".">
            <text font-style="italic" prefix=" $$ " suffix=" $/$ " variable="container-title"/>
            <group prefix=" ">
              <text font-weight="bold" prefix=" $$ " suffix=" $/$ " variable="volume"/>
            </group>
            <text prefix=" $$ : " suffix=" $/$ " variable="page"/>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>