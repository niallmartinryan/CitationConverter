<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="de-AT" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>Österreichische Zeitschrift für Politikwissenschaft (German - Austria)</title>
    <title-short>OEZP</title-short>
    <id>http://www.zotero.org/styles/osterreichische-zeitschrift-fur-politikwissenschaft</id>
    <link href="http://www.zotero.org/styles/osterreichische-zeitschrift-fur-politikwissenschaft" rel="self"/>
    <link href="http://www.oezp.at/authors_e.php" rel="documentation"/>
    <author>
      <name>Sebastian Karcher</name>
    </author>
    <category citation-format="author-date"/>
    <category field="social_science"/>
    <issn>1615-5548</issn>
    <summary>Stil fuer die Politikwissenschaftlichen Publikationen des Nomos Verlages</summary>
    <updated>2012-09-27T22:06:38+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="de">
    <terms>
      <term name="et-al">et al.</term>
    </terms>
  </locale>
  <macro name="editor">
    <names delimiter=", " variable="editor">
      <name delimiter="/" delimiter-precedes-last="always" name-as-sort-order="all" sort-separator=", ">
        <name-part font-style="italic" name="family"/>
      </name>
      <label form="short" prefix=" (" strip-periods="true" suffix=".):" text-case="capitalize-first"/>
    </names>
  </macro>
  <macro name="anon">
    <text form="short" strip-periods="true" term="anonymous" text-case="capitalize-first"/>
  </macro>
  <macro name="author">
    <names variable="author">
      <name delimiter="/" delimiter-precedes-last="always" name-as-sort-order="all" sort-separator=", ">
        <name-part font-style="italic" name="family"/>
      </name>
      <label form="short" prefix=" (" suffix=")" text-case="capitalize-first"/>
      <substitute>
        <names variable="editor"/>
        <text macro="anon"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name delimiter="/" delimiter-precedes-last="always" form="short" initialize-with=". "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text macro="anon"/>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <choose>
      <if variable="URL">
        <group>
          <text suffix=" " value="abrufbar unter:"/>
          <text variable="URL"/>
          <group prefix=" (" suffix=")">
            <text suffix=" " value="letzter Zugriff:"/>
            <date variable="accessed">
              <date-part name="day" suffix="."/>
              <date-part form="numeric" name="month" suffix="."/>
              <date-part name="year"/>
            </date>
          </group>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="title">
    <text variable="title"/>
  </macro>
  <macro name="publisher">
    <group delimiter=": ">
      <text variable="publisher-place"/>
      <text variable="publisher"/>
    </group>
  </macro>
  <macro name="year-date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </if>
      <else>
        <text form="short" term="no date"/>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="pages">
    <text variable="page"/>
  </macro>
  <citation disambiguate-add-givenname="true" disambiguate-add-names="true" disambiguate-add-year-suffix="true" et-al-min="3" et-al-use-first="1">
    <layout delimiter="; " prefix="(" suffix=")">
      <choose>
        <if position="ibid-with-locator">
          <group delimiter=", ">
            <text term="ibid"/>
            <text variable="locator"/>
          </group>
        </if>
        <else-if position="ibid">
          <text term="ibid"/>
        </else-if>
        <else>
          <group delimiter=", ">
            <group delimiter=" ">
              <text macro="author-short"/>
              <text macro="year-date"/>
            </group>
            <group>
              <text variable="locator"/>
            </group>
          </group>
        </else>
      </choose>
    </layout>
  </citation>
  <bibliography entry-spacing="0" et-al-min="5" et-al-use-first="4" hanging-indent="true" line-spacing="1">
    <sort>
      <key macro="author"/>
      <key variable="title"/>
    </sort>
    <layout>
      <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
      <date prefix=" (" suffix=")." variable="issued">
        <date-part name="year"/>
      </date>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=" " prefix=" " suffix=",">
            <group delimiter=", ">
              <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
              <text prefix=" $$ " suffix=" $/$ " variable="genre"/>
            </group>
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
          </group>
          <text macro="publisher" prefix=" $publisher$  " suffix=" $/publisher$ "/>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <text macro="title" prefix=" $title$  " suffix=", $/title$ "/>
          <group delimiter=" " prefix=" ">
            <text prefix=" $$ " suffix=": $/$ " term="in" text-case="lowercase"/>
            <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
            <text prefix=" $$ " suffix=", $/$ " variable="container-title"/>
            <text prefix=" $$ " suffix=". $/$ " variable="collection-title"/>
            <text prefix=" $$ " suffix=". $/$ " variable="event"/>
            <group delimiter=", ">
              <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
              <text macro="pages" prefix=" $pages$ " suffix=" $/pages$ "/>
            </group>
          </group>
        </else-if>
        <else-if type="thesis">
          <group delimiter=". " prefix=" " suffix=".">
            <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
            <text prefix=" $$ " suffix=" $/$ " variable="genre"/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </else-if>
        <else>
          <group suffix=",">
            <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
            <text macro="editor" prefix=" $editor$  " suffix=" $/editor$ "/>
          </group>
          <group prefix=" ">
            <text prefix=" $$ " suffix=":  $/$ " term="in" text-case="lowercase"/>
            <text font-style="italic" prefix=" $$ " suffix=" $/$ " variable="container-title"/>
            <group prefix=", ">
              <text prefix=" $$ Vol. " suffix=" $/$ " variable="volume"/>
              <text prefix=" $$ (" suffix=") $/$ " variable="issue"/>
            </group>
            <group prefix=", ">
              <text prefix=" $$ " suffix=" $/$ " variable="page"/>
            </group>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=" $access$ , " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>