<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-GB" demote-non-dropping-particle="never" page-range-format="minimal" version="1.0">
  <info>
    <title>The British Journal for the Philosophy of Science</title>
    <title-short>BJPS</title-short>
    <id>http://www.zotero.org/styles/the-british-journal-for-the-philosophy-of-science</id>
    <link href="http://www.zotero.org/styles/the-british-journal-for-the-philosophy-of-science" rel="self"/>
    <link href="http://www.zotero.org/styles/american-mineralogist" rel="template"/>
    <link href="https://github.com/citation-style-language/styles/files/887880/_.BJPS-House-Style-01-2016.pdf" rel="documentation"/>
    <author>
      <name>Patrick O'Brien</name>
      <email>obrienpat86@gmail.com</email>
    </author>
    <category citation-format="author-date"/>
    <category field="philosophy"/>
    <issn>0007-0882</issn>
    <eissn>1464-3537</eissn>
    <updated>2017-04-01T19:11:44+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale>
    <terms>
      <term form="short" name="editor">
        <single>ed.</single>
        <multiple>eds</multiple>
      </term>
    </terms>
  </locale>
  <macro name="container-contributors">
    <choose>
      <if match="any" type="chapter paper-conference">
        <names delimiter=", " variable="editor translator">
          <name and="text" delimiter=", " delimiter-precedes-last="never" initialize-with=". "/>
          <label font-style="italic" form="short" prefix=" (" suffix="), "/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="secondary-contributors">
    <choose>
      <if match="none" type="chapter paper-conference">
        <names delimiter=", " prefix=" (" suffix=")" variable="translator editor">
          <name and="text" delimiter=", " delimiter-precedes-last="never" initialize-with=". "/>
          <label form="short" prefix=", "/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="author">
    <names variable="author">
      <name and="text" delimiter=", " delimiter-precedes-last="never" initialize-with=". " name-as-sort-order="all" sort-separator=", "/>
      <et-al term="and others"/>
      <label font-style="italic" form="short" prefix=" (" suffix=") "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if type="report">
            <text variable="publisher"/>
            <text macro="title"/>
          </if>
          <else>
            <text macro="title"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="text" delimiter=", " form="short" initialize-with=". "/>
      <et-al font-style="italic"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if type="report">
            <text variable="publisher"/>
            <text form="short" variable="title"/>
          </if>
          <else-if match="any" type="bill book graphic legal_case legislation motion_picture song">
            <text form="short" variable="title"/>
          </else-if>
          <else>
            <text form="short" quotes="true" variable="title"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="title">
    <choose>
      <if match="any" type="book graphic  motion_picture report song manuscript speech">
        <group delimiter=", " suffix=",">
          <text quotes="true" text-case="title" variable="title"/>
          <text macro="edition"/>
        </group>
      </if>
      <else>
        <text quotes="true" suffix="," text-case="title" variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=", ">
      <choose>
        <if match="none" variable="event">
          <text variable="genre"/>
        </if>
      </choose>
      <choose>
        <if match="none" type="article-journal article-magazine">
          <group delimiter=", ">
            <group delimiter=": ">
              <text variable="publisher-place"/>
              <text variable="publisher"/>
            </group>
            <group delimiter=" ">
              <label form="short" variable="page"/>
              <text variable="page"/>
            </group>
          </group>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="event">
    <choose>
      <if variable="event">
        <choose>
          <if match="none" variable="genre">
            <text suffix=" " term="presented at" text-case="capitalize-first"/>
            <text variable="event"/>
          </if>
          <else>
            <group delimiter=" ">
              <text text-case="capitalize-first" variable="genre"/>
              <text term="presented at"/>
              <text variable="event"/>
            </group>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="issued">
    <choose>
      <if match="none" type="bill legal_case legislation">
        <choose>
          <if variable="issued">
            <group prefix=" [" suffix="]:">
              <date variable="issued">
                <date-part name="year"/>
              </date>
              <text variable="year-suffix"/>
              <choose>
                <if match="none" type="article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song">
                  <date variable="issued">
                    <date-part name="month" prefix=", "/>
                    <date-part name="day" prefix=" "/>
                  </date>
                </if>
              </choose>
            </group>
          </if>
          <else>
            <group prefix=" [" suffix="]:">
              <text form="short" term="no date"/>
              <text prefix="-" variable="year-suffix"/>
            </group>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="issued-sort">
    <choose>
      <if match="none" type="article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song">
        <date variable="issued">
          <date-part name="year"/>
          <date-part name="month"/>
          <date-part name="day"/>
        </date>
      </if>
      <else>
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </else>
    </choose>
  </macro>
  <macro name="issued-year">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year"/>
        </date>
        <text variable="year-suffix"/>
      </if>
      <else>
        <text form="short" term="no date"/>
        <text prefix="-" variable="year-suffix"/>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="locators">
    <choose>
      <if match="any" type="article-journal article-magazine">
        <group delimiter=", " prefix=", ">
          <text font-weight="bold" variable="volume"/>
          <group delimiter=" ">
            <label form="short" variable="page"/>
            <text variable="page"/>
          </group>
        </group>
      </if>
      <else-if type="article-newspaper">
        <group delimiter=" " prefix=", ">
          <label form="short" variable="page"/>
          <text variable="page"/>
        </group>
      </else-if>
      <else-if match="any" type="book graphic motion_picture report song chapter paper-conference">
        <group delimiter=", " prefix=" ">
          <group>
            <text form="short" plural="true" suffix=" " term="volume" text-case="capitalize-first"/>
            <number form="numeric" prefix="1-" variable="number-of-volumes"/>
          </group>
          <group>
            <text form="short" suffix=" " term="volume" text-case="capitalize-first"/>
            <number form="numeric" variable="volume"/>
          </group>
        </group>
      </else-if>
      <else-if type="legal_case">
        <group delimiter=" " prefix=" (" suffix=")">
          <text variable="authority"/>
          <date form="text" variable="issued"/>
        </group>
      </else-if>
      <else-if match="any" type="bill legislation">
        <date prefix=" (" suffix=")" variable="issued">
          <date-part name="year"/>
        </date>
      </else-if>
    </choose>
  </macro>
  <macro name="access">
    <text prefix="&lt;" suffix="&gt;" variable="URL"/>
  </macro>
  <macro name="citation-locator">
    <group>
      <choose>
        <if locator="chapter">
          <label form="long" text-case="capitalize-first" variable="locator"/>
        </if>
        <else>
          <label form="short" variable="locator"/>
        </else>
      </choose>
      <text prefix=" " variable="locator"/>
    </group>
  </macro>
  <macro name="container">
    <group>
      <choose>
        <if match="any" type="chapter paper-conference entry-encyclopedia">
          <text suffix=" " term="in"/>
        </if>
      </choose>
      <text macro="container-contributors"/>
      <text macro="secondary-contributors"/>
      <text macro="container-title"/>
    </group>
  </macro>
  <macro name="container-title">
    <choose>
      <if match="none" type="bill legal_case legislation">
        <text font-style="italic" suffix="," text-case="title" variable="container-title"/>
      </if>
      <else>
        <group delimiter=" " prefix=", ">
          <choose>
            <if variable="container-title">
              <text variable="volume"/>
              <text variable="container-title"/>
              <group delimiter=" ">
                <text form="symbol" term="section"/>
                <text variable="section"/>
              </group>
              <text variable="page"/>
            </if>
            <else>
              <choose>
                <if type="legal_case">
                  <text prefix="No. " variable="number"/>
                </if>
                <else>
                  <text prefix="Pub. L. No. " variable="number"/>
                  <group delimiter=" ">
                    <text form="symbol" term="section"/>
                    <text variable="section"/>
                  </group>
                </else>
              </choose>
            </else>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <citation collapse="year" disambiguate-add-givenname="true" disambiguate-add-year-suffix="true" et-al-min="3" et-al-use-first="1">
    <sort>
      <key macro="issued-sort" sort="ascending"/>
      <key macro="author"/>
    </sort>
    <layout delimiter="; " prefix="(" suffix=")">
      <group delimiter=", ">
        <group delimiter=" ">
          <text macro="author-short"/>
          <text macro="issued-year" prefix="[" suffix="]"/>
        </group>
        <text macro="citation-locator"/>
      </group>
    </layout>
  </citation>
  <bibliography hanging-indent="true" subsequent-author-substitute="———">
    <sort>
      <key macro="author" names-min="1" names-use-first="1"/>
      <key macro="issued-year" sort="ascending"/>
      <key variable="title"/>
    </sort>
    <layout suffix=".">
      <group delimiter=" ">
        <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
        <text macro="issued" prefix=" $issued$ " suffix=" $/issued$ "/>
        <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
        <text macro="container" prefix=" $container$ " suffix=" $/container$ "/>
      </group>
      <text macro="locators" prefix=" $locators$ " suffix=" $/locators$ "/>
      <group delimiter=", " prefix=" ">
        <text macro="event" prefix=" $event$ " suffix=" $/event$ "/>
        <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
      </group>
      <text macro="access" prefix=" $access$ , " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>