<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" page-range-format="minimal" version="1.0">
  <info>
    <title>Université de Picardie Jules Verne (Amiens) - Thèse de UFR de Médecine</title>
    <id>http://www.zotero.org/styles/universite-de-picardie-jules-verne-ufr-de-medecine</id>
    <link href="http://www.zotero.org/styles/universite-de-picardie-jules-verne-ufr-de-medecine" rel="self"/>
    <link href="http://forums.zotero.org/discussion/18705/new-csl-these-de-medecine-a-amiens/" rel="documentation"/>
    <link href="www.u-picardie.fr/medecine" rel="documentation"/>
    <author>
      <name>Gautier Metmer</name>
      <email>gautiermetmer@yahoo.fr</email>
    </author>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <updated>2012-09-27T22:06:38+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <names suffix=". " variable="author">
      <name delimiter=", " delimiter-precedes-last="always" font-weight="bold" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="long" prefix=", "/>
      <substitute>
        <names variable="editor"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names suffix=". " variable="editor">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="long" prefix=", "/>
    </names>
  </macro>
  <macro name="publisher">
    <text suffix=": " variable="publisher-place"/>
    <text suffix="; " variable="publisher"/>
    <group suffix=". ">
      <date variable="issued">
        <date-part name="year"/>
      </date>
      <text macro="accessed-date" prefix=" "/>
    </group>
  </macro>
  <macro name="access">
    <group>
      <text value="Available from: "/>
      <text variable="URL"/>
    </group>
  </macro>
  <macro name="accessed-date">
    <choose>
      <if variable="URL">
        <group delimiter=" " prefix="[" suffix="]">
          <text term="cited"/>
          <date variable="accessed">
            <date-part name="year"/>
            <date-part form="short" name="month" prefix=" " strip-periods="true"/>
            <date-part name="day" prefix=" "/>
          </date>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="journal-title">
    <choose>
      <if match="any" type="article-journal article-magazine">
        <group suffix=" ">
          <text font-style="italic" form="short" strip-periods="true" variable="container-title"/>
          <choose>
            <if variable="URL">
              <text prefix=" [" suffix="]" term="internet" text-case="capitalize-first"/>
            </if>
          </choose>
        </group>
      </if>
      <else>
        <text form="short" suffix=". " variable="container-title"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <group delimiter=" ">
      <text variable="title"/>
      <choose>
        <if match="none" type="article-journal article-magazine">
          <choose>
            <if variable="URL">
              <text prefix="[" suffix="]" term="internet" text-case="capitalize-first"/>
            </if>
          </choose>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="," vertical-align="sup">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography et-al-min="7" et-al-use-first="6" second-field-align="flush">
    <layout>
      <text prefix=" $$ [" suffix="] $/$ " variable="citation-number"/>
      <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
      <text macro="title" prefix=" $title$ " suffix=".  $/title$ "/>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=" " prefix=" ">
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <group prefix=" " suffix=". ">
            <text prefix=" $$ " suffix=":  $/$ " term="in" text-case="capitalize-first"/>
            <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
            <text prefix=" $$ " suffix=" $/$ " variable="container-title"/>
          </group>
          <text macro="publisher" prefix=" $publisher$  " suffix=" $/publisher$ "/>
          <text prefix=" $$  p. " suffix=". $/$ " variable="page"/>
        </else-if>
        <else>
          <text macro="journal-title" prefix=" $journal-title$ " suffix=" $/journal-title$ "/>
          <group suffix=";">
            <date variable="issued">
              <date-part name="year"/>
            </date>
            <text macro="accessed-date" prefix=" $accessed-date$  " suffix=" $/accessed-date$ "/>
          </group>
          <group suffix=". ">
            <text prefix=" $$ " suffix=" $/$ " variable="volume"/>
            <text prefix=" $$ :" suffix=" $/$ " variable="page"/>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=" $access$ " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>