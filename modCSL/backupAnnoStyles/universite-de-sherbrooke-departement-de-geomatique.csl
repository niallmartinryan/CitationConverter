  <macro name="title">
    <choose>
      <if match="any" type="report thesis">
        <text variable="title"/>
        <group prefix=" (" suffix=")">
          <text variable="genre"/>
          <text prefix=" No. " variable="number"/>
        </group>
      </if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <choose>
      <if match="any" type="report">
        <group delimiter=" : ">
          <text variable="publisher-place"/>
          <text variable="publisher"/>
          <text suffix=" p" variable="number-of-pages"/>
        </group>
      </if>
      <else-if match="any" type="thesis">
        <group delimiter=", ">
          <text prefix="Thèse de doctorat, " variable="publisher"/>
          <text variable="publisher-place"/>
          <text suffix=" p" variable="number-of-pages"/>
        </group>
      </else-if>
      <else>
        <group delimiter=", ">
          <choose>
            <if match="none" variable="event">
              <text variable="genre"/>
            </if>
          </choose>
          <choose>
            <if match="none" type="article-journal article-magazine">
              <group delimiter=" : ">
                <text variable="publisher-place"/>
                <text variable="publisher"/>
              </group>
            </if>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
