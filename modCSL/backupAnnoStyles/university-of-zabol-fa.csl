  <macro name="translator-author-editor">
    <names font-weight="bold" variable="translator">
      <name and="text" delimiter="، " delimiter-precedes-et-al="never" delimiter-precedes-last="never" et-al-min="6" et-al-subsequent-min="3" et-al-subsequent-use-first="1" et-al-use-first="1" initialize-with="." name-as-sort-order="all" sort-separator="، "/>
      <substitute>
        <names font-weight="bold" variable="author">
          <name and="text" delimiter="، " delimiter-precedes-et-al="never" delimiter-precedes-last="never" et-al-min="6" et-al-subsequent-min="3" et-al-subsequent-use-first="1" et-al-use-first="1" initialize-with="." name-as-sort-order="all" sort-separator="، "/>
          <substitute>
            <names font-weight="bold" variable="editor">
              <name and="text" delimiter-precedes-et-al="never" delimiter-precedes-last="never" et-al-min="6" et-al-subsequent-min="3" et-al-subsequent-use-first="1" et-al-use-first="1" initialize-with="." name-as-sort-order="all"/>
              <label prefix=" (" suffix=")"/>
            </names>
          </substitute>
        </names>
      </substitute>
    </names>
  </macro>
  <macro name="translator-author-editor-short">
    <names font-weight="normal" variable="translator">
      <name and="text" delimiter="، " delimiter-precedes-et-al="never" delimiter-precedes-last="never" et-al-min="3" et-al-use-first="1" form="short" sort-separator="، "/>
      <substitute>
        <names variable="author">
          <name and="text" delimiter="، " delimiter-precedes-et-al="never" delimiter-precedes-last="never" et-al-min="3" et-al-use-first="1" form="short" sort-separator="، "/>
          <substitute>
            <names variable="editor">
              <name and="text" delimiter="، " delimiter-precedes-et-al="never" delimiter-precedes-last="never" et-al-min="3" et-al-use-first="1" form="short" sort-separator="، "/>
            </names>
          </substitute>
        </names>
      </substitute>
    </names>
  </macro>
  <macro name="translator-short">
    <names variable="translator">
