  <locale xml:lang="tr-TR">
    <terms>
      <term name="no date">t.y.</term>
      <term name="et-al">ve diğerleri</term>
      <term name="and">ve</term>
      <term name="in">içinde</term>
      <term name="retrieved">erişildi</term>
      <term name="from">adresinden</term>
      <term name="presented at">sunulmuş bildiri</term>
      <term name="edition">
        <single>basım</single>
        <multiple>basım</multiple>
      </term>
      <term form="short" name="edition">bs.</term>
      <term name="page">
        <single>page</single>
        <multiple>pages</multiple>
      </term>
      <term form="short" name="page">
        <single>s.</single>
        <multiple>ss.</multiple>
      </term>
      <term form="short" name="volume">
        <single>c.</single>
        <multiple>c.</multiple>
      </term>
      <term form="short" name="translator">
        <single>çev.</single>
        <multiple>çev.</multiple>
      </term>
      <term form="short" name="editor">
        <single>ed.</single>
        <multiple>ed.</multiple>
      </term>
      <!-- ORDINALS -->
      <term name="ordinal-01">.</term>
      <term name="ordinal-02">.</term>
      <term name="ordinal-03">.</term>
      <term name="ordinal-04">.</term>
      <!-- LONG MONTH FORMS -->
      <term name="month-01">Ocak</term>
      <term name="month-02">Şubat</term>
      <term name="month-03">Mart</term>
      <term name="month-04">Nisan</term>
      <term name="month-05">Mayıs</term>
      <term name="month-06">Haziran</term>
      <term name="month-07">Temmuz</term>
      <term name="month-08">Ağustos</term>
      <term name="month-09">Eylül</term>
      <term name="month-10">Ekim</term>
      <term name="month-11">Kasım</term>
      <term name="month-12">Aralık</term>
    </terms>
  </locale>
  <macro name="container-contributors">
    <choose>
      <if match="any" type="chapter paper-conference">
        <names delimiter=", " suffix=", " variable="editor">
          <name and="text" delimiter=", " delimiter-precedes-last="never" initialize-with=". "/>
          <label form="short" prefix=" (" suffix=")" text-case="capitalize-first"/>
          <substitute>
            <names variable="translator"/>
          </substitute>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="secondary-contributors">
    <choose>
      <if match="none" type="article-journal chapter paper-conference">
        <names delimiter=", " prefix=" (" suffix=")" variable="translator">
          <name and="text" delimiter=", " delimiter-precedes-last="never" initialize-with=". "/>
          <label form="short" prefix=", " text-case="capitalize-first"/>
          <substitute>
            <names variable="editor"/>
          </substitute>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="author">
    <names variable="author">
      <name and="text" delimiter=", " delimiter-precedes-last="never" initialize-with=". " name-as-sort-order="all" sort-separator=", "/>
      <label form="short" prefix=" (" suffix=")" text-case="capitalize-first"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if type="report">
            <text variable="publisher"/>
            <text macro="title"/>
          </if>
          <else>
            <text macro="title"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="text" delimiter=", " delimiter-precedes-last="never" form="short" initialize-with=". "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if type="report">
            <text variable="publisher"/>
            <text font-style="italic" form="short" variable="title"/>
          </if>
          <else-if match="any" type="bill book graphic legal_case legislation motion_picture song">
            <text font-style="italic" form="short" variable="title"/>
          </else-if>
          <else>
            <text form="short" quotes="true" variable="title"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <choose>
      <if type="thesis">
        <choose>
          <if match="any" variable="archive">
            <group>
              <text variable="archive"/>
              <text prefix=" (" suffix=")" variable="archive_location"/>
              <text prefix=" veri tabanından " suffix="." term="retrieved"/>
            </group>
          </if>
          <else>
            <group>
              <text variable="URL"/>
              <text prefix=" " term="from"/>
              <text prefix=" " suffix="." term="retrieved"/>
            </group>
          </else>
        </choose>
      </if>
      <else>
        <choose>
          <if variable="DOI">
            <text prefix="doi:" variable="DOI"/>
          </if>
          <else>
            <choose>
              <if type="webpage">
                <group delimiter=" " suffix=".">
                  <group>
                    <date suffix=" " variable="accessed">
                      <date-part name="day" suffix=" "/>
                      <date-part name="month" suffix=" "/>
                      <date-part name="year" suffix=" tarihinde "/>
                    </date>
                  </group>
                  <text variable="URL"/>
                  <text term="from"/>
                  <text term="retrieved"/>
                </group>
              </if>
              <else>
                <group delimiter=" " suffix=".">
                  <text variable="URL"/>
                  <text term="from"/>
                  <text term="retrieved"/>
                </group>
              </else>
            </choose>
          </else>
        </choose>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if type="report">
        <text font-style="italic" variable="title"/>
        <group prefix=" (" suffix=")">
          <text variable="genre"/>
          <text prefix=" No: " variable="number"/>
        </group>
      </if>
      <else-if match="any" type="book graphic motion_picture report song manuscript speech thesis">
        <text font-style="italic" variable="title"/>
      </else-if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <choose>
      <if type="report">
        <group delimiter=": ">
          <text variable="publisher-place"/>
          <text variable="publisher"/>
        </group>
      </if>
      <else-if type="thesis">
        <choose>
          <if match="any" variable="archive URL">
            <text prefix=" (" suffix=")" text-case="capitalize-first" variable="genre"/>
          </if>
          <else>
            <text prefix=" (Yayımlanmamış " suffix="). " text-case="lowercase" variable="genre"/>
            <group delimiter=", ">
              <text variable="publisher"/>
              <text variable="publisher-place"/>
            </group>
          </else>
        </choose>
      </else-if>
      <else>
        <group delimiter=", ">
          <choose>
            <if match="none" variable="event">
              <text variable="genre"/>
            </if>
          </choose>
          <choose>
            <if match="none" type="article-journal article-magazine">
              <group delimiter=": ">
                <text variable="publisher-place"/>
                <text variable="publisher"/>
              </group>
            </if>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="event">
    <choose>
      <if variable="event">
        <choose>
          <if match="none" variable="genre">
            <text variable="event"/>
            <text prefix=", " term="presented at"/>
          </if>
          <else>
            <group delimiter=" ">
              <text variable="event"/>
              <text variable="genre"/>
            </group>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="issued">
    <choose>
      <if match="none" type="bill legal_case legislation">
        <choose>
          <if variable="issued">
            <group prefix=" (" suffix=").">
              <date variable="issued">
                <date-part name="year"/>
              </date>
              <text variable="year-suffix"/>
              <choose>
                <if match="none" type="bill book graphic legal_case legislation motion_picture report song article-journal chapter paper-conference">
                  <date prefix="," variable="issued">
                    <date-part name="day" prefix=" "/>
                    <date-part name="month" prefix=" "/>
                  </date>
                </if>
              </choose>
            </group>
          </if>
          <else>
            <group prefix=" (" suffix=").">
              <text form="short" term="no date"/>
              <text prefix="-" variable="year-suffix"/>
            </group>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="issued-sort">
    <choose>
      <if match="none" type="article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song">
        <date variable="issued">
          <date-part name="year"/>
          <date-part name="day" prefix=", "/>
          <date-part name="month" prefix=" "/>
        </date>
      </if>
      <else>
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </else>
    </choose>
  </macro>
  <macro name="issued-year">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year"/>
        </date>
        <text variable="year-suffix"/>
      </if>
      <else>
        <text form="short" term="no date"/>
        <text prefix="-" variable="year-suffix"/>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="locators">
    <choose>
      <if match="any" type="article-journal article-magazine">
        <group delimiter=", " prefix=", ">
          <group>
            <text font-style="italic" variable="volume"/>
            <text prefix="(" suffix=")" variable="issue"/>
          </group>
          <text variable="page"/>
        </group>
      </if>
      <else-if type="article-newspaper">
        <group delimiter=" " prefix=", ">
          <label form="short" variable="page"/>
          <text variable="page"/>
        </group>
      </else-if>
      <else-if match="any" type="book graphic motion_picture report song chapter paper-conference">
        <group delimiter=", " prefix=" (" suffix=")">
          <text macro="edition"/>
          <group>
            <text form="short" plural="true" suffix=" " term="volume" text-case="capitalize-first"/>
            <number form="numeric" prefix="1-" variable="number-of-volumes"/>
          </group>
          <group>
            <text form="short" suffix=" " term="volume" text-case="capitalize-first"/>
            <number form="numeric" variable="volume"/>
          </group>
          <group>
            <label form="short" suffix=" " variable="page"/>
            <text variable="page"/>
          </group>
        </group>
      </else-if>
      <else-if type="legal_case">
        <group delimiter=" " prefix=" (" suffix=")">
          <text variable="authority"/>
          <date delimiter=" " variable="issued">
            <date-part form="short" name="month"/>
            <date-part name="day" suffix=","/>
            <date-part name="year"/>
          </date>
        </group>
      </else-if>
      <else-if match="any" type="bill legislation">
        <date prefix=" (" suffix=")" variable="issued">
          <date-part name="year"/>
        </date>
      </else-if>
    </choose>
  </macro>
  <macro name="citation-locator">
    <group>
      <label form="short" variable="locator"/>
      <text prefix=" " variable="locator"/>
    </group>
  </macro>
  <macro name="container">
    <choose>
      <if match="none" type="bill legal_case legislation">
        <text font-style="italic" variable="container-title"/>
        <choose>
          <if match="any" type="chapter paper-conference">
            <text prefix=" " suffix=" " term="in"/>
          </if>
        </choose>
      </if>
      <else>
        <group delimiter=" " prefix=" ">
          <choose>
            <if variable="container-title">
              <text variable="volume"/>
              <text variable="container-title"/>
              <group delimiter=" ">
                <!--change to label variable="section" as that becomes available -->
                <text form="symbol" term="section"/>
                <text variable="section"/>
              </group>
              <text variable="page"/>
            </if>
            <else>
              <choose>
                <if type="legal_case">
                  <text prefix="No. " variable="number"/>
                </if>
                <else>
                  <text prefix="Pub. L. No. " variable="number"/>
                  <group delimiter=" ">
                    <!--change to label variable="section" as that becomes available -->
                    <text form="symbol" term="section"/>
                    <text variable="section"/>
                  </group>
                </else>
              </choose>
            </else>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <citation collapse="year" disambiguate-add-givenname="true" disambiguate-add-names="true" disambiguate-add-year-suffix="true" et-al-min="6" et-al-subsequent-min="3" et-al-subsequent-use-first="1" et-al-use-first="1" givenname-disambiguation-rule="primary-name">
    <sort>
      <key macro="author"/>
      <key macro="issued-sort"/>
    </sort>
    <layout delimiter="; " prefix="(" suffix=")">
      <group delimiter=", ">
        <choose>
          <if match="any" type="personal_communication interview">
            <names variable="author">
              <name initialize-with=". "/>
            </names>
            <text value="kişisel iletişim"/>
            <date variable="issued">
              <date-part name="day" prefix=" "/>
              <date-part name="month" prefix=" "/>
              <date-part name="year" prefix=" "/>
            </date>
          </if>
          <else>
            <text macro="author-short"/>
            <text macro="issued-year"/>
          </else>
        </choose>
        <text macro="citation-locator"/>
      </group>
    </layout>
  </citation>
  <bibliography entry-spacing="0" et-al-min="8" et-al-use-first="6" et-al-use-last="true" hanging-indent="true" line-spacing="2">
    <sort>
      <key macro="author"/>
      <key macro="issued-sort" sort="ascending"/>
    </sort>
    <layout>
      <group suffix=".">
        <text macro="author" prefix=" $author$ " suffix=". $/author$ "/>
        <text macro="issued" prefix=" $issued$ " suffix=" $/issued$ "/>
        <group delimiter=". ">
          <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
          <group>
            <text macro="container-contributors" prefix=" $container-contributors$ " suffix=" $/container-contributors$ "/>
            <text macro="secondary-contributors" prefix=" $secondary-contributors$ " suffix=" $/secondary-contributors$ "/>
            <group delimiter=", ">
              <text macro="container" prefix=" $container$ " suffix=" $/container$ "/>
              <text prefix=" $collection-title$ " suffix=" $collection-title$ " variable="collection-title"/>
            </group>
          </group>
        </group>
        <text macro="locators" prefix=" $locators$ " suffix=" $/locators$ "/>
        <group delimiter=", " prefix=". ">
          <text macro="event" prefix=" $event$ " suffix=" $/event$ "/>
          <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
        </group>
      </group>
      <text macro="access" prefix=" $access$  " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>