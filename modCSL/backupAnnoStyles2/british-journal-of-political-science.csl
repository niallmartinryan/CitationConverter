<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="note" default-locale="en-GB" demote-non-dropping-particle="sort-only" page-range-format="minimal" version="1.0">
  <info>
    <title>British Journal of Political Science</title>
    <title-short>BJPS</title-short>
    <id>http://www.zotero.org/styles/british-journal-of-political-science</id>
    <link href="http://www.zotero.org/styles/british-journal-of-political-science" rel="self"/>
    <link href="http://www.zotero.org/styles/american-sociological-association" rel="template"/>
    <link href="http://journals.cambridge.org/images/fileUpload/documents/BJPOLS_style_sheet.pdf" rel="documentation"/>
    <author>
      <name>Sebastian Karcher</name>
    </author>
    <category citation-format="note"/>
    <category field="social_science"/>
    <issn>0007-1234</issn>
    <eissn>1469-2112</eissn>
    <updated>2012-10-25T21:15:26+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="editor">
    <names variable="editor">
      <label form="verb" suffix=" "/>
      <name and="text" delimiter=", "/>
    </names>
  </macro>
  <macro name="anon">
    <text form="short" strip-periods="true" term="anonymous" text-case="capitalize-first"/>
  </macro>
  <macro name="author">
    <names variable="author">
      <name and="text" delimiter=", " delimiter-precedes-last="always" initialize="false" initialize-with=". " name-as-sort-order="first" sort-separator=", "/>
      <label form="short" prefix=", "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if type="report">
            <text variable="publisher"/>
          </if>
        </choose>
        <text macro="anon"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="text" delimiter=", " form="short" initialize-with=". "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if type="report">
            <text variable="publisher"/>
          </if>
        </choose>
        <text macro="anon"/>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <group>
      <choose>
        <if variable="URL">
          <group delimiter=" " prefix=" ">
            <text term="available at" text-case="capitalize-first"/>
            <text suffix="," variable="URL"/>
            <text term="accessed"/>
            <date delimiter=" " variable="accessed">
              <date-part name="day"/>
              <date-part name="month"/>
              <date-part name="year"/>
            </date>
          </group>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="title">
    <choose>
      <if type="thesis">
        <text text-case="title" variable="title"/>
      </if>
      <else-if match="any" type="bill book graphic legal_case legislation motion_picture report song">
        <text font-style="italic" text-case="title" variable="title"/>
      </else-if>
      <else>
        <text text-case="title" variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=": ">
      <text variable="publisher-place"/>
      <text variable="publisher"/>
    </group>
  </macro>
  <macro name="year-date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </if>
      <else>
        <text form="short" term="no date"/>
      </else>
    </choose>
  </macro>
  <macro name="day-month">
    <date variable="issued">
      <date-part name="month"/>
      <date-part name="day" prefix=" "/>
    </date>
  </macro>
  <macro name="pages">
    <label form="short" suffix=" " text-case="capitalize-first" variable="page"/>
    <text variable="page"/>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <citation collapse="year" disambiguate-add-givenname="true" disambiguate-add-names="true" disambiguate-add-year-suffix="true" et-al-min="4" et-al-use-first="1">
    <sort>
      <key macro="author-short"/>
      <key macro="year-date"/>
    </sort>
    <layout delimiter="; " suffix=".">
      <group delimiter=", ">
        <group delimiter=" ">
          <text macro="author-short"/>
          <text macro="year-date"/>
        </group>
        <text variable="locator"/>
      </group>
    </layout>
  </citation>
  <bibliography et-al-min="10" et-al-use-first="6" hanging-indent="true">
    <sort>
      <key macro="author"/>
      <key macro="year-date"/>
      <key variable="title"/>
    </sort>
    <layout suffix=".">
      <group delimiter=" ">
        <text macro="author" prefix=" $author$ " suffix=". $/author$ "/>
        <text macro="year-date" prefix=" $year-date$ " suffix=". $/year-date$ "/>
      </group>
      <choose>
        <if match="any" type="article-newspaper article-magazine">
          <group delimiter=" ">
            <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
          </group>
          <group delimiter=", " prefix=" ">
            <text font-style="italic" prefix=" $container-title$ " suffix=" $container-title$ " text-case="title" variable="container-title"/>
            <text macro="day-month" prefix=" $day-month$ " suffix=" $/day-month$ "/>
            <text prefix=" $edition$ " suffix=" $edition$ " variable="edition"/>
            <text prefix=" $page$ " suffix=" $page$ " variable="page"/>
          </group>
        </if>
        <else-if type="thesis">
          <text macro="title" prefix=" $title$  " quotes="true" suffix=". $/title$ "/>
          <group delimiter=", " prefix=" ">
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="editor" prefix=" $editor$ " suffix=". $/editor$ "/>
            <text prefix=" $genre$ " suffix=" $genre$ " variable="genre"/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </else-if>
        <else-if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=", " prefix=" " suffix=". ">
            <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ " text-case="title"/>
          </group>
          <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
        </else-if>
        <else-if match="any" type="chapter paper-conference">
          <group delimiter=" ">
            <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
            <group delimiter=", " suffix=".">
              <group delimiter=" ">
                <text macro="pages" prefix=" $pages$ " suffix=" $/pages$ "/>
                <text prefix=" $$ " suffix=" $/$ " term="in"/>
                <group delimiter=", ">
                  <text font-style="italic" prefix=" $container-title$ " suffix=" $container-title$ " text-case="title" variable="container-title"/>
                  <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
                </group>
              </group>
              <text prefix=" $volume$ vol. " suffix=" $volume$ " variable="volume"/>
              <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
            </group>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </else-if>
        <else-if type="speech">
          <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
          <group delimiter=", ">
            <group delimiter=" " prefix=". ">
              <text prefix=" $genre$ " suffix=" $genre$ " variable="genre"/>
              <text prefix=" $$ " suffix=" $/$ " term="presented at" text-case="capitalize-first"/>
              <text prefix=" $event$ " suffix=" $event$ " variable="event"/>
            </group>
            <text prefix=" $event-place$ " suffix=" $event-place$ " variable="event-place"/>
            <date delimiter=" " variable="issued">
              <date-part name="day"/>
              <date-part name="month"/>
            </date>
          </group>
        </else-if>
        <else>
          <group suffix=".">
            <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
            <text macro="editor" prefix=" $editor$  " suffix=" $/editor$ "/>
          </group>
          <group delimiter=" " prefix=" " suffix=".">
            <text font-style="italic" prefix=" $container-title$ " suffix=" $container-title$ " variable="container-title"/>
            <group delimiter=":">
              <text prefix=" $volume$ " suffix=" $volume$ " variable="volume"/>
              <text prefix=" $page$ " suffix=" $page$ " variable="page"/>
            </group>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=" $access$ . " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>