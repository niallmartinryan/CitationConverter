        <multiple>argitaratzaile eta itzultzaileak</multiple>
      </term>
      <!-- SHORT ROLE FORMS -->
      <term form="short" name="author">
        <single/>
        <multiple/>
      </term>
      <term form="short" name="editor">
        <single>arg.</single>
        <multiple>arg.</multiple>
      </term>
      <term form="short" name="editorial-director">
