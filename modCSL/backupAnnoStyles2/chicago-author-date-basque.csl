      <term form="short" name="month-02">ots.</term>
      <term form="short" name="month-03">martx.</term>
      <term form="short" name="month-04">apr.</term>
      <term form="short" name="month-05">mai.</term>
      <term form="short" name="month-06">eka.</term>
      <term form="short" name="month-07">uzt.</term>
      <term form="short" name="month-08">abz.</term>
