<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>Ecology Letters</title>
    <id>http://www.zotero.org/styles/ecology-letters</id>
    <link href="http://www.zotero.org/styles/ecology-letters" rel="self"/>
    <link href="http://www.zotero.org/styles/apa" rel="template"/>
    <link href="http://onlinelibrary.wiley.com/journal/10.1111/%28ISSN%291461-0248/homepage/ForAuthors.html" rel="documentation"/>
    <author>
      <name>David Kaplan</name>
      <email>david.kaplan@ird.fr</email>
    </author>
    <contributor>
      <name>Sebastian Karcher</name>
    </contributor>
    <category citation-format="author-date"/>
    <category field="biology"/>
    <issn>1461-023X</issn>
    <eissn>1461-0248</eissn>
    <updated>2017-09-25T10:45:32+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="container">
    <choose>
      <if match="any" type="chapter paper-conference">
        <text suffix=": " term="in" text-case="capitalize-first"/>
        <text font-style="italic" variable="container-title"/>
        <text prefix=", " variable="collection-title"/>
        <names delimiter=", " prefix=" (" suffix=")" variable="editor translator">
          <label form="short" suffix=" "/>
          <name and="symbol" delimiter=", " delimiter-precedes-last="never" initialize-with="." name-as-sort-order="all" sort-separator=", "/>
        </names>
      </if>
      <else>
        <group delimiter=", ">
          <text font-style="italic" form="short" variable="container-title"/>
          <text variable="collection-title"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="author">
    <names variable="author">
      <name and="symbol" delimiter=", " delimiter-precedes-last="never" initialize-with="." name-as-sort-order="all" sort-separator=", "/>
      <label form="short" prefix=" (" suffix=")" text-case="capitalize-first"/>
      <et-al font-style="italic"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text macro="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="symbol" delimiter=", " form="short" initialize-with=". "/>
      <et-al font-style="italic"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
            <text font-style="italic" form="short" variable="title"/>
          </if>
          <else>
            <text form="short" quotes="true" variable="title"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <choose>
      <if type="webpage">
        <group>
          <text suffix=": " term="available at" text-case="capitalize-first"/>
          <text suffix="." variable="URL"/>
        </group>
        <text prefix=" " suffix=" " value="Last accessed"/>
        <date variable="accessed">
          <date-part name="day" suffix=" "/>
          <date-part name="month" suffix=" "/>
          <date-part name="year"/>
        </date>
      </if>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if match="any" type="report">
        <text font-style="italic" variable="title"/>
        <group prefix=" (" suffix=")">
          <text variable="genre"/>
          <text prefix=" No. " variable="number"/>
        </group>
      </if>
      <else-if match="any" type="bill book graphic legal_case legislation motion_picture report song speech">
        <text font-style="italic" variable="title"/>
      </else-if>
      <else-if type="webpage">
        <text font-style="italic" variable="title"/>
      </else-if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <choose>
      <if match="any" type="report">
        <group delimiter=", ">
          <text variable="publisher"/>
          <text variable="publisher-place"/>
        </group>
      </if>
      <else>
        <text suffix=". " variable="genre"/>
        <group delimiter=", ">
          <text variable="publisher"/>
          <text variable="publisher-place"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="event">
    <choose>
      <if variable="event">
        <text suffix=" " term="presented at" text-case="capitalize-first"/>
        <text variable="event"/>
      </if>
    </choose>
  </macro>
  <macro name="issued">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </if>
      <else-if variable="accessed">
        <choose>
          <if type="webpage">
            <date variable="accessed">
              <date-part name="year"/>
            </date>
          </if>
          <else>
            <text form="short" term="no date"/>
          </else>
        </choose>
      </else-if>
      <else>
        <text form="short" term="no date"/>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text value="edn"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="locators">
    <choose>
      <if match="any" type="article-journal article-magazine article-newspaper">
        <group delimiter=", " prefix=", ">
          <group>
            <text variable="volume"/>
          </group>
          <text variable="page"/>
        </group>
      </if>
      <else-if match="any" type="bill book graphic legal_case legislation motion_picture report song thesis">
        <group delimiter=". " prefix=". ">
          <text macro="edition"/>
          <text macro="event"/>
          <text macro="publisher"/>
        </group>
      </else-if>
      <else-if match="any" type="chapter paper-conference">
        <group delimiter=", " prefix=". ">
          <text macro="event"/>
          <text macro="publisher"/>
          <group>
            <label form="short" suffix=" " variable="page"/>
            <text variable="page"/>
          </group>
        </group>
      </else-if>
    </choose>
  </macro>
  <citation collapse="year-suffix" disambiguate-add-year-suffix="true" et-al-min="3" et-al-use-first="1" year-suffix-delimiter=", ">
    <sort>
      <key macro="issued"/>
      <key macro="author"/>
    </sort>
    <layout delimiter="; " prefix="(" suffix=")">
      <group delimiter=" ">
        <text macro="author-short"/>
        <text macro="issued"/>
      </group>
    </layout>
  </citation>
  <bibliography entry-spacing="0" et-al-min="7" et-al-use-first="6" hanging-indent="true">
    <sort>
      <key macro="author"/>
      <key macro="issued" sort="ascending"/>
      <key macro="title"/>
    </sort>
    <layout>
      <group suffix=".">
        <text macro="author" prefix=" $author$ " suffix=". $/author$ "/>
        <text macro="issued" prefix=" $issued$  (" suffix=").  $/issued$ "/>
        <group delimiter=". ">
          <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
          <text macro="container" prefix=" $container$ " suffix=" $/container$ "/>
        </group>
        <text macro="locators" prefix=" $locators$ " suffix=" $/locators$ "/>
        <text macro="access" prefix=" $access$ . " suffix=" $/access$ "/>
      </group>
    </layout>
  </bibliography>
</style>