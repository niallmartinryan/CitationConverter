<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>International Journal of Radiation Oncology, Biology, Physics</title>
    <id>http://www.zotero.org/styles/international-journal-of-radiation-oncology-biology-physics</id>
    <link href="http://www.zotero.org/styles/international-journal-of-radiation-oncology-biology-physics" rel="self"/>
    <link href="http://www.elsevier.com/journals/international-journal-of-radiation-oncology-biology-physics/0360-3016/guide-for-authors#manu" rel="documentation"/>
    <author>
      <name>Julian Onions</name>
      <email>julian.onions@gmail.com</email>
    </author>
    <author>
      <name>Santam Chakraborty</name>
      <email>pgiresident@gmail.com </email>
    </author>
    <contributor>
      <name>Charles Parnot</name>
      <uri>http://twitter.com/cparnot</uri>
      <email>charles.parnot@gmail.com</email>
    </contributor>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <issn>0360-3016</issn>
    <eissn>1879-355X</eissn>
    <summary>The American Medical Association style as used in International Journal of Radiation Oncology Biology Physics.</summary>
    <updated>2012-09-27T22:06:38+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="editor">
    <names variable="editor">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <et-al font-style="italic"/>
      <label form="short" prefix=", "/>
    </names>
  </macro>
  <macro name="anon">
    <text form="short" strip-periods="true" term="anonymous" text-case="capitalize-first"/>
  </macro>
  <macro name="author">
    <group suffix=".">
      <names variable="author">
        <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
        <et-al font-style="italic"/>
        <label form="short" prefix=" " strip-periods="true"/>
        <substitute>
          <names variable="editor"/>
          <text macro="anon"/>
        </substitute>
      </names>
    </group>
  </macro>
  <macro name="title">
    <choose>
      <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
        <text font-style="italic" variable="title"/>
      </if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=": ">
      <text variable="publisher-place"/>
      <text variable="publisher"/>
    </group>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter=", " prefix="(" suffix=")">
      <text variable="citation-number"/>
      <group>
        <label form="short" strip-periods="true" variable="locator"/>
        <text variable="locator"/>
      </group>
    </layout>
  </citation>
  <bibliography et-al-min="4" et-al-use-first="3" hanging-indent="false">
    <layout>
      <text prefix=" $citation-number$ " suffix=".  $citation-number$ " variable="citation-number"/>
      <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group suffix=".">
            <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
            <text macro="edition" prefix=" $edition$  " suffix=" $/edition$ "/>
            <text macro="editor" prefix=" $editor$  (" suffix=") $/editor$ "/>
          </group>
          <text macro="publisher" prefix=" $publisher$  " suffix=" $/publisher$ "/>
          <group prefix="; " suffix=".">
            <date variable="issued">
              <date-part name="year"/>
            </date>
            <text prefix=" $page$ :" suffix=" $page$ " variable="page"/>
          </group>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
          <group prefix=" ">
            <text prefix=" $$ " suffix=":  $/$ " term="in" text-case="capitalize-first"/>
            <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
            <text font-style="italic" prefix=" $container-title$  " suffix=". $container-title$ " variable="container-title"/>
            <text prefix=" $volume$ Vol " suffix=". $volume$ " variable="volume"/>
            <text macro="edition" prefix=" $edition$  " suffix=" $/edition$ "/>
            <text prefix=" $collection-title$  " suffix=". $collection-title$ " variable="collection-title"/>
            <group suffix=".">
              <text macro="publisher" prefix=" $publisher$  " suffix=" $/publisher$ "/>
              <group prefix="; " suffix=".">
                <date variable="issued">
                  <date-part name="year"/>
                </date>
                <text prefix=" $page$ :" suffix=" $page$ " variable="page"/>
              </group>
            </group>
          </group>
        </else-if>
        <else>
          <group suffix=".">
            <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
            <text macro="editor" prefix=" $editor$  " suffix=" $/editor$ "/>
          </group>
          <group prefix=" " suffix=".">
            <text font-style="italic" form="short" prefix=" $container-title$ " suffix=". $container-title$ " variable="container-title"/>
            <group delimiter=";" prefix=" ">
              <date variable="issued">
                <date-part name="year"/>
              </date>
              <group>
                <text prefix=" $volume$ " suffix=" $volume$ " variable="volume"/>
              </group>
            </group>
            <text prefix=" $page$ :" suffix=" $page$ " variable="page"/>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>