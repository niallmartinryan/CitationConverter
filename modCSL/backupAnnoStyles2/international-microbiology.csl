<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" version="1.0">
  <!-- This style was edited with the Visual CSL Editor (http://steveridout.com/csl/visualEditor/) -->
  <info>
    <title>International Microbiology</title>
    <id>http://www.zotero.org/styles/international-microbiology</id>
    <link href="http://www.zotero.org/styles/international-microbiology" rel="self"/>
    <link href="http://www.zotero.org/styles/acta-neurochirurgica" rel="template"/>
    <link href="http://www.springer.com/life+sciences/microbiology/journal/10123" rel="documentation"/>
    <author>
      <name>Sebastian Karcher</name>
    </author>
    <category citation-format="numeric"/>
    <category field="biology"/>
    <issn>1139-6709</issn>
    <eissn>1618-1905</eissn>
    <summary>Springer Vancouver style - alphabetical, no et al, alphabetical, no issue numbers</summary>
    <updated>2012-10-18T22:38:43+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author-short">
    <names variable="author">
      <name and="text" form="short"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
      </substitute>
    </names>
  </macro>
  <macro name="author">
    <names variable="author">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="short" prefix=" (" strip-periods="true" suffix=")"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <text suffix=": " term="in" text-case="capitalize-first"/>
    <names variable="editor">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="short" prefix=" (" strip-periods="true" suffix=")"/>
    </names>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" strip-periods="true" term="edition"/>
        </group>
      </if>
      <else>
        <text variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="year">
    <date variable="issued">
      <date-part name="year"/>
    </date>
  </macro>
  <macro name="title">
    <choose>
      <if type="book">
        <group delimiter=", " suffix=". ">
          <text variable="title"/>
          <text macro="edition"/>
        </group>
      </if>
      <else>
        <text suffix=". " variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="year-parenth">
    <date prefix="(" suffix=")" variable="issued">
      <date-part name="year"/>
    </date>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter=", " prefix="[" suffix="]">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography second-field-align="flush">
    <sort>
      <key macro="author-short"/>
      <key macro="year"/>
    </sort>
    <layout>
      <text prefix=" $citation-number$ " suffix=".  $citation-number$ " variable="citation-number"/>
      <group delimiter=" ">
        <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
        <text macro="year-parenth" prefix=" $year-parenth$ " suffix=" $/year-parenth$ "/>
        <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
      </group>
      <choose>
        <if variable="page">
          <choose>
            <if match="any" type="chapter paper-conference">
              <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
              <group delimiter=", " prefix=" " suffix=". ">
                <text form="short" prefix=" $container-title$ " suffix=" $container-title$ " variable="container-title"/>
                <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
              </group>
              <text prefix=" $publisher$ " suffix=",  $publisher$ " variable="publisher"/>
              <text prefix=" $publisher-place$ " suffix=",  $publisher-place$ " variable="publisher-place"/>
              <group delimiter=" ">
                <label form="short" strip-periods="true" variable="page"/>
                <text prefix=" $page$ " suffix=" $page$ " variable="page"/>
              </group>
            </if>
            <else>
              <group>
                <text form="short" prefix=" $container-title$ " strip-periods="true" suffix="  $container-title$ " variable="container-title"/>
                <text prefix=" $volume$ " suffix=": $volume$ " variable="volume"/>
                <text prefix=" $page$ " suffix=" $page$ " variable="page"/>
              </group>
            </else>
          </choose>
        </if>
        <else-if variable="DOI">
          <group delimiter=". ">
            <text form="short" prefix=" $container-title$ " strip-periods="true" suffix=" $container-title$ " variable="container-title"/>
            <text prefix=" $DOI$ doi: " suffix=" $DOI$ " variable="DOI"/>
          </group>
        </else-if>
        <else-if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=", ">
            <text prefix=" $publisher$ " suffix=" $publisher$ " variable="publisher"/>
            <text prefix=" $publisher-place$ " suffix=" $publisher-place$ " variable="publisher-place"/>
          </group>
        </else-if>
        <else-if type="webpage">
          <group>
            <text form="short" prefix=" $container-title$ In: " suffix=".  $container-title$ " variable="container-title"/>
            <text prefix=" $URL$ " suffix=".  $URL$ " variable="URL"/>
            <date variable="accessed">
              <date-part name="day" prefix="Accessed " suffix=" "/>
              <date-part form="short" name="month" strip-periods="true" suffix=" "/>
              <date-part name="year"/>
            </date>
          </group>
        </else-if>
        <else-if type="thesis">
          <group delimiter=", ">
            <text prefix=" $genre$ " suffix=" $genre$ " variable="genre"/>
            <text prefix=" $publisher$ " suffix=" $publisher$ " variable="publisher"/>
          </group>
        </else-if>
        <else>
          <group>
            <text form="short" prefix=" $container-title$ " suffix="  $container-title$ " variable="container-title"/>
            <text prefix=" $volume$ " suffix=": $volume$ " variable="volume"/>
            <text prefix=" $page$ " suffix=". $page$ " variable="page"/>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>