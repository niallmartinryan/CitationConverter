  <macro name="editor-translator">
    <names delimiter=", " prefix="(" suffix=")" variable="editor translator">
      <name and="symbol" delimiter=", " initialize-with=". "/>
      <label form="short" prefix=", " text-case="capitalize-first"/>
    </names>
  </macro>
  <macro name="author">
    <names variable="author">
      <name and="symbol" delimiter-precedes-last="always" initialize-with="." name-as-sort-order="all"/>
      <label form="short" prefix=" (" suffix=")" text-case="capitalize-first"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text macro="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="symbol" delimiter=", " form="short" initialize-with=". "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
            <text font-style="italic" form="short" variable="title"/>
          </if>
          <else>
            <text form="short" quotes="true" variable="title"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="title">
    <choose>
      <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
        <text font-style="italic" variable="title"/>
      </if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=", ">
      <text variable="publisher"/>
      <text variable="publisher-place"/>
    </group>
  </macro>
  <citation disambiguate-add-names="true" disambiguate-add-year-suffix="true" et-al-min="3" et-al-use-first="1">
    <sort>
      <key variable="issued"/>
      <key variable="author"/>
    </sort>
    <layout delimiter="; " prefix="(" suffix=")">
      <group delimiter=" ">
        <text macro="author-short"/>
        <date variable="issued">
          <date-part name="year"/>
        </date>
        <group>
          <label form="short" variable="locator"/>
          <text prefix=" " variable="locator"/>
        </group>
      </group>
    </layout>
  </citation>
  <bibliography hanging-indent="true">
    <sort>
      <key macro="author-short"/>
      <key macro="title"/>
    </sort>
    <layout>
      <text macro="author" prefix=" $author$ " suffix=". $/author$ "/>
      <date prefix=" " suffix="." variable="issued">
        <date-part name="year"/>
      </date>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group suffix=".">
            <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
            <text macro="editor-translator" prefix=" $editor-translator$  " suffix=" $/editor-translator$ "/>
          </group>
          <text macro="publisher" prefix=" $publisher$  " suffix=". $/publisher$ "/>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
          <group prefix=". ">
            <text prefix=" $$ " suffix=" $/$ " term="in" text-case="capitalize-first"/>
            <names delimiter=", " prefix=" " suffix="," variable="editor translator">
              <name and="symbol" initialize-with="." name-as-sort-order="all"/>
              <label form="short" prefix=" (" suffix=")"/>
            </names>
            <text font-style="italic" prefix=" $container-title$  " suffix=", $container-title$ " variable="container-title"/>
            <group prefix=" " suffix=".">
              <label form="short" suffix=" " variable="page"/>
              <text prefix=" $page$ " suffix=" $page$ " variable="page"/>
            </group>
            <text prefix=" $collection-title$  " suffix=". $collection-title$ " variable="collection-title"/>
            <group suffix=".">
              <text macro="publisher" prefix=" $publisher$  " suffix=" $/publisher$ "/>
            </group>
          </group>
        </else-if>
        <else-if type="article-journal">
          <group delimiter=" " prefix=" " suffix=". ">
            <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
            <text macro="editor-translator" prefix=" $editor-translator$ " suffix=" $/editor-translator$ "/>
          </group>
          <choose>
            <if match="any" variable="issue volume">
              <group delimiter=" " suffix=".">
                <text font-style="italic" prefix=" $container-title$ " suffix=" $container-title$ " variable="container-title"/>
                <text font-style="normal" prefix=" $volume$ " suffix=": $volume$ " variable="volume"/>
                <text prefix=" $page$ " suffix=" $page$ " variable="page"/>
              </group>
            </if>
            <else>
              <text font-style="italic" prefix=" $container-title$ " suffix=" $container-title$ " variable="container-title"/>
              <!-- Online version before inclusion in an issue -->
              <text prefix=" $DOI$ . doi: " suffix=" $DOI$ " variable="DOI"/>
            </else>
          </choose>
        </else-if>
        <else>
          <group delimiter=" " prefix=" " suffix=". ">
            <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
            <text macro="editor-translator" prefix=" $editor-translator$ " suffix=" $/editor-translator$ "/>
          </group>
          <group delimiter=" " suffix=".">
            <text font-style="italic" prefix=" $container-title$ " suffix=" $container-title$ " variable="container-title"/>
            <text font-style="normal" prefix=" $volume$ " suffix=": $volume$ " variable="volume"/>
            <text prefix=" $page$ " suffix=" $page$ " variable="page"/>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>