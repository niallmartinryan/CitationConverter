<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-GB" demote-non-dropping-particle="sort-only" page-range-format="minimal" version="1.0">
  <info>
    <title>Molecular Psychiatry (letters to the editor)</title>
    <id>http://www.zotero.org/styles/molecular-psychiatry-letters</id>
    <link href="http://www.zotero.org/styles/molecular-psychiatry-letters" rel="self"/>
    <link href="http://mts-mp.nature.com/cgi-bin/main.plex?form_type=display_auth_instructions#style" rel="documentation"/>
    <author>
      <name>Michael Berkowitz</name>
      <email>mberkowi@gmu.edu</email>
    </author>
    <contributor>
      <name>Sean Takats</name>
      <email>stakats@gmu.edu</email>
    </contributor>
    <contributor>
      <name>Sebastian Karcher</name>
    </contributor>
    <contributor>
      <name>Daniel Stjepanovic</name>
      <email>daniel.stjepanovic@gmail.com</email>
    </contributor>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <issn>1359-4184</issn>
    <eissn>1476-5578</eissn>
    <summary>Vancouver style w/o titles for use in Letters to the Editor to the Nature journal Molecular Psychiatry</summary>
    <updated>2013-01-18T11:02:00+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <names suffix=". " variable="author">
      <name delimiter=", " delimiter-precedes-et-al="never" delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <et-al font-style="italic"/>
      <label form="long" prefix=", "/>
      <substitute>
        <names variable="editor"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names suffix=". " variable="editor">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="long" prefix=", "/>
    </names>
  </macro>
  <macro name="publisher">
    <choose>
      <if type="thesis">
        <text prefix="[" suffix="]: " variable="publisher-place"/>
      </if>
      <else>
        <text suffix=": " variable="publisher-place"/>
      </else>
    </choose>
    <text suffix="; " variable="publisher"/>
    <group suffix=". ">
      <date variable="issued">
        <date-part name="year"/>
      </date>
      <text macro="accessed-date" prefix=" "/>
    </group>
  </macro>
  <macro name="access">
    <group>
      <text value="Available from: "/>
      <text variable="URL"/>
    </group>
  </macro>
  <macro name="accessed-date">
    <choose>
      <if variable="URL">
        <group delimiter=" " prefix="[" suffix="]">
          <text term="cited"/>
          <date variable="accessed">
            <date-part name="year"/>
            <date-part form="short" name="month" prefix=" " strip-periods="true"/>
            <date-part name="day" prefix=" "/>
          </date>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="journal-title">
    <choose>
      <if match="any" type="article-journal article-magazine">
        <group suffix=" ">
          <text font-style="italic" form="short" strip-periods="true" variable="container-title"/>
          <choose>
            <if variable="URL">
              <text prefix=" [" suffix="]" term="internet" text-case="capitalize-first"/>
            </if>
          </choose>
        </group>
      </if>
      <else>
        <text font-style="italic" form="short" suffix=". " variable="container-title"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <group delimiter=" ">
      <choose>
        <if type="book">
          <text font-style="italic" variable="title"/>
        </if>
        <else-if match="none" type="article-journal">
          <text variable="title"/>
        </else-if>
      </choose>
      <choose>
        <if match="none" type="article-journal article-magazine">
          <choose>
            <if variable="URL">
              <text prefix="[" suffix="]" term="internet" text-case="capitalize-first"/>
            </if>
          </choose>
        </if>
      </choose>
      <choose>
        <if type="thesis">
          <text prefix="[" suffix="]" variable="genre"/>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter=", " vertical-align="sup">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography et-al-min="7" et-al-use-first="6" second-field-align="flush">
    <layout>
      <text prefix=" $citation-number$ " suffix=".  $citation-number$ " variable="citation-number"/>
      <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
      <text macro="title" prefix=" $title$ " suffix=".  $/title$ "/>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song thesis">
          <group delimiter=" " prefix=" ">
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
            <text prefix=" $number-of-pages$ " suffix=" p. $number-of-pages$ " variable="number-of-pages"/>
          </group>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <group prefix=" " suffix=". ">
            <text prefix=" $$ " suffix=":  $/$ " term="in" text-case="capitalize-first"/>
            <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
            <text prefix=" $container-title$ " suffix=" $container-title$ " variable="container-title"/>
          </group>
          <text macro="publisher" prefix=" $publisher$  " suffix=" $/publisher$ "/>
          <text prefix=" $page$  p. " suffix=". $page$ " variable="page"/>
        </else-if>
        <else>
          <text macro="journal-title" prefix=" $journal-title$ " suffix=" $/journal-title$ "/>
          <group suffix=";">
            <date variable="issued">
              <date-part name="year"/>
            </date>
            <text macro="accessed-date" prefix=" $accessed-date$  " suffix=" $/accessed-date$ "/>
          </group>
          <group delimiter=": " prefix=" " suffix=". ">
            <text font-weight="bold" prefix=" $volume$ " suffix=" $volume$ " variable="volume"/>
            <text prefix=" $page$ " suffix=" $page$ " variable="page"/>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=" $access$ " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>