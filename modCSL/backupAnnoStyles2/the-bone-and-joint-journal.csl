<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-GB" version="1.0">
  <info>
    <title>The Bone &amp; Joint Journal</title>
    <title-short>BJJ</title-short>
    <id>http://www.zotero.org/styles/the-bone-and-joint-journal</id>
    <link href="http://www.zotero.org/styles/the-bone-and-joint-journal" rel="self"/>
    <link href="http://www.zotero.org/styles/vancouver-superscript" rel="template"/>
    <link href="http://www.bjj.boneandjoint.org.uk/site/media/Authors/Paperstructure.pdf" rel="documentation"/>
    <author>
      <name>Max Gordon</name>
      <email>max@gforge.se</email>
    </author>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <issn>2049-4394</issn>
    <eissn>2049-4408</eissn>
    <summary>Modified version of Vancouver</summary>
    <updated>2012-09-09T21:58:08+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <date delimiter=" " form="text">
      <date-part name="day"/>
      <date-part form="short" name="month" strip-periods="true"/>
      <date-part name="year"/>
    </date>
    <terms>
      <term name="retrieved">available</term>
      <term form="short" name="section">sect.</term>
    </terms>
  </locale>
  <macro name="author">
    <names variable="author">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="short" prefix=", "/>
      <substitute>
        <names variable="editor"/>
        <text value="No authors listed."/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <group delimiter=": ">
      <choose>
        <if match="any" type="chapter paper-conference">
          <text term="in" text-case="capitalize-first"/>
        </if>
      </choose>
      <names suffix="." variable="editor">
        <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
        <label form="short" prefix=", "/>
      </names>
    </group>
  </macro>
  <macro name="publisher">
    <group delimiter=": " suffix=",">
      <choose>
        <if type="thesis">
          <text prefix="[" suffix="]" variable="publisher-place"/>
        </if>
        <else-if type="webpage">
          <text suffix=" " variable="URL"/>
        </else-if>
        <else>
          <text variable="publisher-place"/>
        </else>
      </choose>
      <text variable="publisher"/>
    </group>
  </macro>
  <macro name="accessed-date">
    <choose>
      <if variable="URL">
        <group delimiter=" " prefix="[" suffix="]">
          <text term="cited"/>
          <date form="text" variable="accessed"/>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="container-title">
    <choose>
      <if match="any" type="article-journal article-magazine paper-conference article-newspaper">
        <group delimiter=" " suffix=" ">
          <text font-style="italic" form="short" strip-periods="true" variable="container-title"/>
          <choose>
            <if variable="URL">
              <text prefix="[" suffix="]" term="internet" text-case="capitalize-first"/>
            </if>
          </choose>
        </group>
        <text macro="edition" prefix=" "/>
      </if>
      <else-if type="chapter">
        <group delimiter=" " suffix=".">
          <text font-style="italic" form="short" variable="container-title"/>
          <choose>
            <if variable="URL">
              <text prefix="[" suffix="]" term="internet" text-case="capitalize-first"/>
            </if>
          </choose>
        </group>
        <text macro="edition" prefix=" "/>
      </else-if>
      <!--add event-name and event-place once they become available-->
      <else-if match="any" type="bill legislation">
        <group delimiter=", ">
          <group delimiter=". ">
            <text form="short" variable="container-title"/>
            <group delimiter=" ">
              <text form="short" term="section" text-case="capitalize-first"/>
              <text variable="section"/>
            </group>
          </group>
          <text variable="number"/>
        </group>
      </else-if>
      <else>
        <text form="short" suffix="." variable="container-title"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if type="book">
        <text font-style="italic" variable="title"/>
      </if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
    <choose>
      <if match="none" type="article-journal article-magazine chapter paper-conference article-newspaper">
        <text macro="edition" prefix=". "/>
      </if>
    </choose>
    <choose>
      <if type="thesis">
        <text prefix=" [" suffix="]" variable="genre"/>
      </if>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="date">
    <choose>
      <if match="any" type="article-journal article-magazine article-newspaper">
        <group delimiter=" " suffix=";">
          <date date-parts="year" form="text" variable="issued"/>
          <text macro="accessed-date"/>
        </group>
      </if>
      <else-if match="any" type="bill legislation">
        <group delimiter=", ">
          <date delimiter=" " variable="issued">
            <date-part form="short" name="month" strip-periods="true"/>
            <date-part name="day"/>
          </date>
          <date variable="issued">
            <date-part name="year"/>
          </date>
        </group>
      </else-if>
      <else-if type="report">
        <date delimiter=" " variable="issued">
          <date-part name="year"/>
          <date-part form="short" name="month" strip-periods="true"/>
        </date>
      </else-if>
      <else-if type="webpage">
        <group delimiter=" " prefix="(" suffix=")">
          <text text-case="lowercase" value="date last accessed"/>
          <date form="text" variable="accessed">
            <date-part form="long" name="month"/>
          </date>
        </group>
      </else-if>
      <else>
        <group>
          <date variable="issued">
            <date-part name="year"/>
          </date>
          <text macro="accessed-date" prefix=". "/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="pages">
    <text prefix=":" variable="page"/>
  </macro>
  <macro name="journal-location">
    <choose>
      <if match="any" type="article-journal article-magazine">
        <text variable="volume"/>
        <text prefix="(" suffix=")" variable="issue"/>
      </if>
    </choose>
  </macro>
  <macro name="report-details">
    <choose>
      <if type="report">
        <text prefix="Report No.: " variable="number"/>
      </if>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="," vertical-align="sup">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography et-al-min="7" et-al-use-first="6" second-field-align="flush">
    <layout>
      <text prefix=" $citation-number$ " suffix=".  $citation-number$ " variable="citation-number"/>
      <group delimiter=". " suffix=". ">
        <text font-weight="bold" macro="author" prefix=" $author$ " suffix=" $/author$ "/>
        <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
      </group>
      <group delimiter=" " suffix=". ">
        <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
        <text macro="container-title" prefix=" $container-title$ " suffix=" $/container-title$ "/>
        <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
        <group>
          <text macro="date" prefix=" $date$ " suffix=" $/date$ "/>
          <text macro="journal-location" prefix=" $journal-location$ " suffix=" $/journal-location$ "/>
          <text macro="pages" prefix=" $pages$ " suffix=" $/pages$ "/>
        </group>
      </group>
      <text macro="report-details" prefix=" $report-details$ " suffix=".  $/report-details$ "/>
    </layout>
  </bibliography>
</style>