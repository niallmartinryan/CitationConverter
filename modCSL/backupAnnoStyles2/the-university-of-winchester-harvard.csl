<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" and="text" class="in-text" default-locale="en-GB" demote-non-dropping-particle="never" name-as-sort-order="all" version="1.0">
  <info>
    <title>The University of Winchester - Harvard</title>
    <id>http://www.zotero.org/styles/the-university-of-winchester-harvard</id>
    <link href="http://www.zotero.org/styles/the-university-of-winchester-harvard" rel="self"/>
    <link href="http://www.zotero.org/styles/harvard-university-of-exeter-geography" rel="template"/>
    <link href="http://mla.winchester.ac.uk/?attachment_id=2060" rel="documentation"/>
    <author>
      <name>Patrick O'Brien</name>
      <email>obrienpat86@gmail.com</email>
    </author>
    <category citation-format="author-date"/>
    <category field="generic-base"/>
    <summary>Harvard style for the University of Winchester, based on the revised guidelines from 7th September 2016.</summary>
    <updated>2017-10-12T13:16:59+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <terms>
      <term form="short" name="editortranslator">
        <single>ed. &amp; trans.</single>
        <multiple>eds. &amp; trans.</multiple>
      </term>
      <term form="short" name="translator">
        <single>trans.</single>
        <multiple>trans.</multiple>
      </term>
      <term form="short" name="edition">edn</term>
    </terms>
  </locale>
  <macro name="container-contributors">
    <choose>
      <if match="any" type="chapter paper-conference">
        <names delimiter=", " variable="editor translator">
          <name and="text" delimiter=", " initialize-with=". "/>
          <label form="short" prefix=" (" suffix=")"/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="secondary-contributors">
    <choose>
      <if match="none" type="chapter paper-conference">
        <names delimiter=", " prefix=" " variable="translator editor">
          <label form="verb-short" text-case="capitalize-first"/>
          <name and="text" initialize-with=". " prefix=" "/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="author">
    <names variable="author">
      <name and="text" delimiter=", " delimiter-precedes-last="always" initialize-with=". " name-as-sort-order="all" sort-separator=", "/>
      <label form="short" prefix=" "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if type="report">
            <text variable="publisher"/>
            <text macro="title"/>
          </if>
          <else>
            <text macro="title"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <group delimiter=" ">
      <choose>
        <if variable="DOI">
          <text prefix="https://doi.org/" variable="DOI"/>
        </if>
        <else-if match="all" variable="URL">
          <group delimiter=": ">
            <text term="available at" text-case="capitalize-first"/>
            <text suffix=". " variable="URL"/>
          </group>
        </else-if>
      </choose>
      <group delimiter=" " prefix="[" suffix="]">
        <text term="accessed" text-case="capitalize-first"/>
        <date form="text" variable="accessed"/>
      </group>
    </group>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="text" delimiter=", " form="short" initialize-with=". "/>
      <et-al font-style="italic"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if type="report">
            <text variable="publisher"/>
            <text font-style="italic" form="short" variable="title"/>
          </if>
          <else-if match="any" type="bill book graphic legal_case legislation motion_picture song">
            <text font-style="italic" form="short" variable="title"/>
          </else-if>
          <else>
            <text form="short" quotes="true" variable="title"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="title">
    <choose>
      <if match="any" type="report thesis">
        <text font-style="italic" variable="title"/>
        <group delimiter=" " prefix=", " suffix="">
          <text variable="genre"/>
          <text prefix="No. " variable="number"/>
        </group>
      </if>
      <else-if match="any" type="book graphic  motion_picture report song manuscript speech">
        <text font-style="italic" variable="title"/>
      </else-if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <choose>
      <if match="any" type="report">
        <group delimiter=": ">
          <text variable="publisher-place"/>
          <text variable="publisher"/>
        </group>
      </if>
      <else-if match="any" type="thesis">
        <group delimiter=", ">
          <text variable="publisher"/>
          <text variable="publisher-place"/>
        </group>
      </else-if>
      <else>
        <group delimiter=", ">
          <choose>
            <if match="none" variable="event">
              <text variable="genre"/>
            </if>
          </choose>
          <choose>
            <if match="none" type="article-journal article-magazine">
              <group delimiter=": ">
                <text variable="publisher-place"/>
                <text variable="publisher"/>
              </group>
            </if>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="event">
    <choose>
      <if variable="event">
        <choose>
          <if match="none" variable="genre">
            <text suffix=" " term="presented at" text-case="capitalize-first"/>
            <text variable="event"/>
          </if>
          <else>
            <group delimiter=" ">
              <text text-case="capitalize-first" variable="genre"/>
              <text term="presented at"/>
              <text variable="event"/>
            </group>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="issued">
    <choose>
      <if match="none" type="bill legal_case legislation">
        <choose>
          <if variable="issued">
            <group prefix=" (" suffix=")">
              <choose>
                <if match="any" type="webpage">
                  <date variable="issued">
                    <date-part form="numeric-leading-zeros" name="day"/>
                    <date-part form="numeric-leading-zeros" name="month" prefix="/"/>
                    <date-part name="year" prefix="/"/>
                  </date>
                </if>
                <else>
                  <date variable="issued">
                    <date-part name="year"/>
                  </date>
                </else>
              </choose>
              <text variable="year-suffix"/>
              <choose>
                <if match="none" type="article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song webpage thesis">
                  <date variable="issued">
                    <date-part name="day" prefix="/"/>
                    <date-part name="month" prefix="/"/>
                  </date>
                </if>
              </choose>
            </group>
          </if>
          <else>
            <group prefix=" (" suffix=")">
              <text form="short" term="no date"/>
              <text prefix="-" variable="year-suffix"/>
            </group>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="issued-sort">
    <choose>
      <if match="none" type="article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song">
        <date variable="issued">
          <date-part name="year"/>
          <date-part name="month"/>
          <date-part name="day"/>
        </date>
      </if>
      <else>
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </else>
    </choose>
  </macro>
  <macro name="issued-year">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year"/>
        </date>
        <text variable="year-suffix"/>
      </if>
      <else>
        <text form="short" term="no date"/>
        <text prefix="-" variable="year-suffix"/>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition" text-case="capitalize-first"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="locators">
    <choose>
      <if match="any" type="article-journal article-magazine">
        <group delimiter=" " prefix=" ">
          <group delimiter=", ">
            <text variable="volume"/>
            <text prefix="(" suffix=")" variable="issue"/>
          </group>
          <text variable="page"/>
        </group>
      </if>
      <else-if type="article-newspaper">
        <group delimiter=" " prefix=": ">
          <text variable="page"/>
        </group>
      </else-if>
      <else-if match="any" type="book graphic motion_picture report song chapter paper-conference">
        <group delimiter=", " prefix=" ">
          <text macro="edition"/>
          <group>
            <text form="short" plural="true" suffix=" " term="volume" text-case="capitalize-first"/>
            <number form="numeric" prefix="1-" variable="number-of-volumes"/>
          </group>
          <group>
            <text form="short" suffix=" " term="volume" text-case="capitalize-first"/>
            <number form="numeric" variable="volume"/>
          </group>
        </group>
      </else-if>
      <else-if type="legal_case">
        <group delimiter=" " prefix=" (" suffix=")">
          <text variable="authority"/>
          <date delimiter=" " variable="issued">
            <date-part form="short" name="month"/>
            <date-part name="day" suffix=","/>
            <date-part name="year"/>
          </date>
        </group>
      </else-if>
      <else-if match="any" type="bill legislation">
        <date prefix=" (" suffix=")" variable="issued">
          <date-part name="year"/>
        </date>
      </else-if>
    </choose>
  </macro>
  <macro name="citation-locator">
    <group delimiter=" ">
      <choose>
        <if locator="chapter">
          <label form="long" text-case="capitalize-first" variable="locator"/>
        </if>
      </choose>
      <text variable="locator"/>
    </group>
  </macro>
  <macro name="container">
    <group>
      <choose>
        <if match="any" type="chapter paper-conference entry-encyclopedia">
          <text suffix=": " term="in" text-case="capitalize-first"/>
        </if>
      </choose>
      <text macro="container-contributors" suffix=" "/>
      <text macro="secondary-contributors"/>
      <text macro="container-title"/>
    </group>
  </macro>
  <macro name="container-title">
    <choose>
      <if match="none" type="bill legal_case legislation">
        <choose>
          <if match="any" type="article-journal article-newspaper article-magazine">
            <text font-style="italic" variable="container-title"/>
          </if>
          <else>
            <text font-style="italic" text-case="title" variable="container-title"/>
          </else>
        </choose>
      </if>
      <else>
        <group delimiter=" " prefix=", ">
          <choose>
            <if variable="container-title">
              <text variable="volume"/>
              <text variable="container-title"/>
              <group delimiter=" ">
                <text form="symbol" term="section"/>
                <text variable="section"/>
              </group>
              <text variable="page"/>
            </if>
            <else>
              <choose>
                <if type="legal_case">
                  <text prefix="No. " variable="number"/>
                </if>
                <else>
                  <text prefix="Pub. L. No. " variable="number"/>
                  <group delimiter=" ">
                    <text form="symbol" term="section"/>
                    <text variable="section"/>
                  </group>
                </else>
              </choose>
            </else>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <citation collapse="year" disambiguate-add-givenname="true" disambiguate-add-names="true" disambiguate-add-year-suffix="true" et-al-min="3" et-al-use-first="1" givenname-disambiguation-rule="primary-name">
    <sort>
      <key macro="author"/>
      <key macro="issued-sort"/>
    </sort>
    <layout delimiter="; " prefix="(" suffix=")">
      <group delimiter=":">
        <group delimiter=", ">
          <text macro="author-short"/>
          <text macro="issued-year"/>
        </group>
        <text macro="citation-locator"/>
      </group>
    </layout>
  </citation>
  <bibliography entry-spacing="0" et-al-min="20" et-al-use-first="6" et-al-use-last="false" hanging-indent="false" line-spacing="2">
    <sort>
      <key macro="author"/>
      <key macro="issued-sort" sort="ascending"/>
    </sort>
    <layout>
      <group>
        <group delimiter=" ">
          <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
          <text macro="issued" prefix=" $issued$ " suffix=" $/issued$ "/>
          <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
          <text macro="container" prefix=" $container$ " suffix=" $/container$ "/>
        </group>
        <text macro="locators" prefix=" $locators$ " suffix=" $/locators$ "/>
        <group delimiter=" " prefix=". ">
          <text macro="event" prefix=" $event$ " suffix=" $/event$ "/>
          <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          <text macro="access" prefix=" $access$ " suffix=" $/access$ "/>
        </group>
      </group>
    </layout>
  </bibliography>
</style>