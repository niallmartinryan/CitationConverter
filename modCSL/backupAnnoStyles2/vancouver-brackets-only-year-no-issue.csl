<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" demote-non-dropping-particle="sort-only" page-range-format="minimal" version="1.0">
  <!-- This style was edited with the Visual CSL Editor (http://editor.citationstyles.org/visualEditor/) -->
  <info>
    <title>Vancouver (brackets, only year in date, no issue numbers)</title>
    <id>http://www.zotero.org/styles/vancouver-brackets-only-year-no-issue</id>
    <link href="http://www.zotero.org/styles/vancouver-brackets-only-year-no-issue" rel="self"/>
    <link href="http://www.zotero.org/styles/vancouver-superscript" rel="template"/>
    <link href="https://github.com/citation-style-language/styles/pull/645" rel="documentation"/>
    <author>
      <name>Charles Parnot</name>
      <email>charles.parnot@gmail.com</email>
      <uri>http://twitter.com/cparnot</uri>
    </author>
    <contributor>
      <name>Michael Berkowitz</name>
      <email>mberkowi@gmu.edu</email>
    </contributor>
    <contributor>
      <name>Sean Takats</name>
      <email>stakats@gmu.edu</email>
    </contributor>
    <contributor>
      <name>Petr Hlustik</name>
      <email>phlustik@upol.cz</email>
    </contributor>
    <contributor>
      <name>Sebastian Karcher</name>
    </contributor>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <summary>Vancouver style as outlined by International Committee of Medical Journal Editors Uniform Requirements for Manuscripts Submitted to Biomedical Journals: Sample References</summary>
    <updated>2013-07-23T08:11:53+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <names suffix=". " variable="author">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="long" prefix=", " text-case="lowercase"/>
      <substitute>
        <names variable="editor"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names suffix=". " variable="editor">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="long" prefix=", " text-case="lowercase"/>
    </names>
  </macro>
  <macro name="publisher">
    <text suffix=": " variable="publisher-place"/>
    <text suffix="; " variable="publisher"/>
    <group suffix=". ">
      <date variable="issued">
        <date-part name="year"/>
      </date>
      <text macro="accessed-date" prefix=" "/>
    </group>
  </macro>
  <macro name="access">
    <group>
      <text value="Available from: "/>
      <text variable="URL"/>
    </group>
  </macro>
  <macro name="accessed-date">
    <choose>
      <if variable="URL">
        <group delimiter=" " prefix="[" suffix="]">
          <text term="cited" text-case="lowercase"/>
          <date variable="accessed">
            <date-part name="year"/>
            <date-part form="short" name="month" prefix=" " strip-periods="true"/>
            <date-part name="day" prefix=" "/>
          </date>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="journal-title">
    <choose>
      <if match="any" type="article-journal article-magazine review review-book">
        <group suffix=" ">
          <text form="short" variable="container-title"/>
          <choose>
            <if variable="URL">
              <text prefix=" [" suffix="]" term="internet" text-case="capitalize-first"/>
            </if>
          </choose>
        </group>
      </if>
      <else>
        <text form="short" variable="container-title"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <group delimiter=" ">
      <text variable="title"/>
      <choose>
        <if match="none" type="article-journal article-magazine review review-book">
          <choose>
            <if variable="URL">
              <text prefix="[" suffix="]" term="internet" text-case="capitalize-first"/>
            </if>
          </choose>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="," prefix="[" suffix="]" vertical-align="baseline">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography et-al-min="7" et-al-use-first="6" second-field-align="flush">
    <layout>
      <text prefix=" $citation-number$ " suffix=".  $citation-number$ " variable="citation-number"/>
      <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
      <text macro="title" prefix=" $title$ " suffix=".  $/title$ "/>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=" " prefix=" ">
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </if>
        <else-if match="any" type="chapter paper-conference entry-dictionary entry-encyclopedia">
          <group prefix=" " suffix=". ">
            <text prefix=" $$ " suffix=":  $/$ " term="in" text-case="capitalize-first"/>
            <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
            <text prefix=" $container-title$ " suffix=" $container-title$ " variable="container-title"/>
          </group>
          <text macro="publisher" prefix=" $publisher$  " suffix=" $/publisher$ "/>
          <group delimiter=" " prefix=" " suffix=".">
            <label plural="never" variable="page"/>
            <text prefix=" $page$ " suffix=" $page$ " variable="page"/>
          </group>
        </else-if>
        <else>
          <text macro="journal-title" prefix=" $journal-title$ " suffix=" $/journal-title$ "/>
          <group suffix=";">
            <date variable="issued">
              <date-part name="year"/>
            </date>
            <text macro="accessed-date" prefix=" $accessed-date$  " suffix=" $/accessed-date$ "/>
          </group>
          <group suffix=". ">
            <text prefix=" $volume$ " suffix=" $volume$ " variable="volume"/>
            <text prefix=" $page$ :" suffix=" $page$ " variable="page"/>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=" $access$ " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>