<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="never" version="1.0">
  <!-- This style was initially created with the Visual CSL Editor (http://steveridout.com/csl/visualEditor/) -->
  <info>
    <title>Acta Pharmaceutica</title>
    <id>http://www.zotero.org/styles/acta-pharmaceutica</id>
    <link href="http://www.zotero.org/styles/acta-pharmaceutica" rel="self"/>
    <link href="http://www.zotero.org/styles/apa" rel="template"/>
    <link href="http://public.carnet.hr/acphee/Inst.html" rel="documentation"/>
    <author>
      <name>Charles Parnot</name>
      <email>charles.parnot@gmail.com</email>
      <uri>http://twitter.com/cparnot</uri>
    </author>
    <category citation-format="numeric"/>
    <category field="biology"/>
    <issn>1330-0075</issn>
    <eissn>1846-9558</eissn>
    <updated>2012-09-27T22:06:38+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="container-contributors">
    <choose>
      <if match="any" type="chapter paper-conference">
        <text suffix=" " term="in" text-case="capitalize-first"/>
        <names delimiter=", " suffix=", " variable="editor">
          <name and="symbol" delimiter=", " initialize-with=". "/>
          <label form="short" prefix=" (" suffix=")" text-case="capitalize-first"/>
          <substitute>
            <names variable="translator"/>
          </substitute>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="secondary-contributors">
    <choose>
      <if match="none" type="chapter paper-conference">
        <names delimiter=", " prefix=" (" suffix=")" variable="translator">
          <name and="symbol" delimiter=", " initialize-with=". "/>
          <label form="short" prefix=", " text-case="capitalize-first"/>
          <substitute>
            <names variable="editor"/>
          </substitute>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="author">
    <names variable="author">
      <name and="text" delimiter=", " delimiter-precedes-last="never" et-al-min="19" et-al-use-first="18" initialize-with=". "/>
      <label form="short" prefix=" (" suffix=")" text-case="capitalize-first"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if type="report">
            <text variable="publisher"/>
            <text macro="title"/>
          </if>
          <else>
            <text macro="title"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <choose>
      <if type="thesis">
        <choose>
          <if match="any" variable="archive">
            <group>
              <text suffix=" " term="retrieved" text-case="capitalize-first"/>
              <text suffix=" " term="from"/>
              <text suffix="." variable="archive"/>
              <text prefix=" (" suffix=")" variable="archive_location"/>
            </group>
          </if>
          <else>
            <group>
              <text suffix=" " term="retrieved" text-case="capitalize-first"/>
              <text suffix=" " term="from"/>
              <text variable="URL"/>
            </group>
          </else>
        </choose>
      </if>
      <else>
        <choose>
          <if variable="DOI">
            <text form="long" prefix="DOI:" variable="DOI"/>
          </if>
          <else>
            <choose>
              <if type="webpage">
                <group delimiter=" ">
                  <text suffix=" " term="retrieved" text-case="capitalize-first"/>
                  <group>
                    <date suffix=", " variable="accessed">
                      <date-part name="month" suffix=" "/>
                      <date-part name="day" suffix=", "/>
                      <date-part name="year"/>
                    </date>
                  </group>
                  <text term="from"/>
                  <text variable="URL"/>
                </group>
              </if>
              <else>
                <group>
                  <text suffix=" " term="retrieved" text-case="capitalize-first"/>
                  <text suffix=" " term="from"/>
                  <text variable="URL"/>
                </group>
              </else>
            </choose>
          </else>
        </choose>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if match="any" type="report thesis">
        <text font-style="italic" variable="title"/>
        <group delimiter=" " prefix=" (" suffix=")">
          <text variable="genre"/>
          <text prefix="No. " variable="number"/>
        </group>
      </if>
      <else-if match="any" type="book graphic  motion_picture report song manuscript speech">
        <text font-style="italic" variable="title"/>
      </else-if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <choose>
      <if match="any" type="report">
        <group delimiter=": ">
          <text variable="publisher-place"/>
          <text variable="publisher"/>
        </group>
      </if>
      <else-if match="any" type="thesis">
        <group delimiter=", ">
          <text variable="publisher"/>
          <text variable="publisher-place"/>
        </group>
      </else-if>
      <else>
        <group delimiter=", ">
          <choose>
            <if match="none" variable="event">
              <text variable="genre"/>
            </if>
          </choose>
          <choose>
            <if match="none" type="article-journal article-magazine">
              <group delimiter=": ">
                <text variable="publisher-place"/>
                <text variable="publisher"/>
              </group>
            </if>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="event">
    <choose>
      <if variable="event">
        <choose>
          <if match="none" variable="genre">
            <text suffix=" " term="presented at" text-case="capitalize-first"/>
            <text variable="event"/>
          </if>
          <else>
            <group delimiter=" ">
              <text text-case="capitalize-first" variable="genre"/>
              <text term="presented at"/>
              <text variable="event"/>
            </group>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="issued">
    <choose>
      <if match="none" type="bill legal_case legislation">
        <choose>
          <if variable="issued">
            <group prefix="(" suffix=")">
              <date variable="issued">
                <date-part name="year"/>
              </date>
              <text variable="year-suffix"/>
              <choose>
                <if match="none" type="article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song">
                  <date variable="issued">
                    <date-part name="month" prefix=", "/>
                    <date-part name="day" prefix=" "/>
                  </date>
                </if>
              </choose>
            </group>
          </if>
          <else>
            <group prefix="(" suffix=")">
              <text form="short" term="no date"/>
              <text prefix="-" variable="year-suffix"/>
            </group>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="locators">
    <choose>
      <if match="any" type="article-journal article-magazine">
        <group delimiter=" " prefix=" ">
          <text font-weight="bold" form="long" variable="volume"/>
          <text macro="issued"/>
          <text variable="page"/>
        </group>
      </if>
      <else-if type="article-newspaper">
        <group delimiter=" " prefix=", ">
          <label form="short" variable="page"/>
          <text variable="page"/>
        </group>
      </else-if>
      <else-if match="any" type="book graphic motion_picture report song chapter paper-conference">
        <group delimiter=", " prefix=" (" suffix=")">
          <text macro="edition"/>
          <group>
            <text form="short" plural="true" suffix=" " term="volume" text-case="capitalize-first"/>
            <number form="numeric" prefix="1-" variable="number-of-volumes"/>
          </group>
          <group>
            <text form="short" suffix=" " term="volume" text-case="capitalize-first"/>
            <number form="numeric" variable="volume"/>
          </group>
          <group>
            <label form="short" suffix=" " variable="page"/>
            <text variable="page"/>
          </group>
        </group>
      </else-if>
      <else-if type="legal_case">
        <group delimiter=" " prefix=" (" suffix=")">
          <text variable="authority"/>
          <date delimiter=" " variable="issued">
            <date-part form="short" name="month"/>
            <date-part name="day" suffix=","/>
            <date-part name="year"/>
          </date>
        </group>
      </else-if>
      <else-if match="any" type="bill legislation">
        <date prefix=" (" suffix=")" variable="issued">
          <date-part name="year"/>
        </date>
      </else-if>
    </choose>
  </macro>
  <macro name="container">
    <choose>
      <if match="none" type="bill legal_case legislation">
        <text font-style="italic" form="short" variable="container-title"/>
      </if>
      <else>
        <group delimiter=" " prefix=", ">
          <choose>
            <if variable="container-title">
              <text variable="volume"/>
              <text font-style="italic" variable="container-title"/>
              <group delimiter=" ">
                <text form="symbol" term="section"/>
                <text variable="section"/>
              </group>
              <text variable="page"/>
            </if>
            <else>
              <choose>
                <if type="legal_case">
                  <text prefix="No. " variable="number"/>
                </if>
                <else>
                  <text prefix="Pub. L. No. " variable="number"/>
                  <group delimiter=" ">
                    <text form="symbol" term="section"/>
                    <text variable="section"/>
                  </group>
                </else>
              </choose>
            </else>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="," prefix="(" suffix=")">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography entry-spacing="0" et-al-min="8" et-al-use-first="7" hanging-indent="true" line-spacing="2">
    <layout>
      <text prefix=" $citation-number$ " suffix=". $citation-number$ " variable="citation-number"/>
      <group suffix=".">
        <group delimiter=". " suffix=", ">
          <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
        </group>
        <group delimiter=", ">
          <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
          <group>
            <text macro="container-contributors" prefix=" $container-contributors$ " suffix=" $/container-contributors$ "/>
            <text macro="secondary-contributors" prefix=" $secondary-contributors$ " suffix=" $/secondary-contributors$ "/>
            <group delimiter=", ">
              <text macro="container" prefix=" $container$ " suffix=" $/container$ "/>
              <text prefix=" $collection-title$ " suffix=" $collection-title$ " variable="collection-title"/>
            </group>
          </group>
        </group>
        <text macro="locators" prefix=" $locators$ " suffix=" $/locators$ "/>
        <group delimiter=", " prefix=". ">
          <text macro="event" prefix=" $event$ " suffix=" $/event$ "/>
          <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
        </group>
      </group>
      <text macro="access" prefix=" $access$ ; " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>