<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>Annals of Neurology</title>
    <id>http://www.zotero.org/styles/annals-of-neurology</id>
    <link href="http://www.zotero.org/styles/annals-of-neurology" rel="self"/>
    <link href="http://www.zotero.org/styles/vancouver" rel="template"/>
    <link href="http://onlinelibrary.wiley.com/journal/10.1002/%28ISSN%291531-8249/homepage/ForAuthors.html" rel="documentation"/>
    <author>
      <name>Michael Berkowitz</name>
      <email>mberkowi@gmu.edu</email>
    </author>
    <contributor>
      <name>Sebastian Karcher</name>
    </contributor>
    <category citation-format="numeric"/>
    <category field="biology"/>
    <category field="medicine"/>
    <issn>0364-5134</issn>
    <eissn>1531-8249</eissn>
    <updated>2012-09-27T22:06:38+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <terms>
      <term form="long" name="no date">date unknown</term>
    </terms>
  </locale>
  <macro name="author">
    <names suffix=". " variable="author">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="long" prefix=", " suffix="."/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names variable="editor">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="long" prefix=", " suffix=". "/>
    </names>
  </macro>
  <macro name="publisher">
    <text suffix=": " variable="publisher-place"/>
    <text suffix="; " variable="publisher"/>
    <text macro="date"/>
  </macro>
  <macro name="date">
    <choose>
      <if variable="issued">
        <choose>
          <if type="article-journal">
            <date variable="issued">
              <date-part name="year"/>
            </date>
          </if>
          <else>
            <date variable="issued">
              <date-part name="year"/>
            </date>
          </else>
        </choose>
      </if>
      <else>
        <text prefix="[" suffix="]" term="no date"/>
      </else>
    </choose>
  </macro>
  <macro name="access">
    <group delimiter=" ">
      <group delimiter=" " prefix="[" suffix="]">
        <text term="cited"/>
        <date suffix=" " variable="accessed">
          <date-part name="year"/>
          <date-part form="short" name="month" prefix=" " strip-periods="true"/>
          <date-part name="day" prefix=" "/>
        </date>
      </group>
      <group>
        <text value="Available from: "/>
        <text variable="URL"/>
      </group>
    </group>
  </macro>
  <macro name="title">
    <group delimiter=" ">
      <text variable="title"/>
      <choose>
        <if variable="URL">
          <text prefix="[" suffix="]" term="internet" text-case="capitalize-first"/>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="," vertical-align="sup">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography et-al-min="5" et-al-use-first="3" second-field-align="flush">
    <layout>
      <text prefix=" $citation-number$ " suffix=".  $citation-number$ " variable="citation-number"/>
      <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
      <text macro="title" prefix=" $title$ " suffix=".  $/title$ "/>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=" " prefix=" " suffix=".">
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <group prefix=" " suffix=". ">
            <text prefix=" $$ " suffix=":  $/$ " term="in" text-case="capitalize-first"/>
            <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
            <text prefix=" $container-title$ " suffix=" $container-title$ " variable="container-title"/>
          </group>
          <text macro="publisher" prefix=" $publisher$  " suffix=" $/publisher$ "/>
          <text prefix=" $page$  p. " suffix=". $page$ " variable="page"/>
        </else-if>
        <else>
          <text form="short" prefix=" $container-title$ " suffix="  $container-title$ " variable="container-title"/>
          <text macro="date" prefix=" $date$ " suffix="; $/date$ "/>
          <text prefix=" $volume$ " suffix=" $volume$ " variable="volume"/>
          <text prefix=" $issue$ (" suffix=") $issue$ " variable="issue"/>
          <text prefix=" $page$ :" suffix=". $page$ " variable="page"/>
        </else>
      </choose>
      <text macro="access" prefix=" $access$ " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>