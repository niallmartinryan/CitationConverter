<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" page-range-format="minimal" version="1.0">
  <info>
    <title>Elsevier - Vancouver</title>
    <id>http://www.zotero.org/styles/elsevier-vancouver</id>
    <link href="http://www.zotero.org/styles/elsevier-vancouver" rel="self"/>
    <link href="http://www.elsevier.com/journals/energy/0360-5442/guide-for-authors#68000" rel="documentation"/>
    <author>
      <name>Richard Karnesky</name>
      <email>karnesky+zotero@gmail.com</email>
      <uri>http://arc.nucapt.northwestern.edu/Richard_Karnesky</uri>
    </author>
    <category citation-format="numeric"/>
    <category field="generic-base"/>
    <summary>A style for some Elsevier journals, resembles Vancouver style</summary>
    <updated>2012-09-27T22:06:38+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <names variable="author">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="long" prefix=", "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names variable="editor">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="long" prefix=", " suffix="."/>
    </names>
  </macro>
  <macro name="year-date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </if>
      <else>
        <text form="short" term="no date"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <text suffix=": " text-case="title" variable="publisher-place"/>
    <text suffix="; " variable="publisher"/>
    <text macro="year-date"/>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="access">
    <choose>
      <if variable="DOI">
        <text prefix="doi:" variable="DOI"/>
      </if>
      <else-if match="any" type="webpage post-weblog">
        <choose>
          <if variable="URL">
            <text variable="URL"/>
            <group delimiter=" " prefix=" (" suffix=")">
              <text term="accessed"/>
              <date form="text" variable="accessed"/>
            </group>
          </if>
        </choose>
      </else-if>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="," prefix="[" suffix="]">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography entry-spacing="0" et-al-min="7" et-al-use-first="6" second-field-align="flush">
    <layout suffix=".">
      <text prefix=" $citation-number$ [" suffix="] $citation-number$ " variable="citation-number"/>
      <text macro="author" prefix=" $author$ " suffix=".  $/author$ "/>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=". ">
            <text prefix=" $title$ " suffix=" $title$ " variable="title"/>
            <text prefix=" $volume$ vol. " suffix=" $volume$ " variable="volume"/>
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <group delimiter=", ">
            <group delimiter=". ">
              <text prefix=" $title$ " suffix=" $title$ " variable="title"/>
              <group>
                <text prefix=" $$ " suffix=":  $/$ " term="in" text-case="sentence"/>
                <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
              </group>
              <group delimiter=", ">
                <text form="short" prefix=" $container-title$ " suffix=" $container-title$ " variable="container-title"/>
                <text prefix=" $volume$ vol. " suffix=" $volume$ " variable="volume"/>
              </group>
              <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            </group>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
            <group delimiter=" ">
              <label form="short" plural="never" variable="page"/>
              <text prefix=" $page$ " suffix=" $page$ " variable="page"/>
            </group>
          </group>
        </else-if>
        <else-if type="patent">
          <group delimiter=", ">
            <group delimiter=". ">
              <text prefix=" $title$ " suffix=" $title$ " variable="title"/>
              <text prefix=" $number$ " suffix=" $number$ " variable="number"/>
            </group>
            <text macro="year-date" prefix=" $year-date$ " suffix=" $/year-date$ "/>
          </group>
        </else-if>
        <else-if type="thesis">
          <group delimiter=". ">
            <text prefix=" $title$ " suffix=" $title$ " variable="title"/>
            <text prefix=" $genre$ " suffix=" $genre$ " variable="genre"/>
            <group delimiter=", ">
              <text prefix=" $publisher$ " suffix=" $publisher$ " variable="publisher"/>
              <text macro="year-date" prefix=" $year-date$ " suffix=" $/year-date$ "/>
            </group>
          </group>
        </else-if>
        <else>
          <group delimiter=":">
            <group delimiter=" ">
              <group delimiter=". ">
                <text prefix=" $title$ " suffix=" $title$ " variable="title"/>
                <text form="short" prefix=" $container-title$ " strip-periods="true" suffix=" $container-title$ " text-case="title" variable="container-title"/>
              </group>
              <group delimiter=";">
                <text macro="year-date" prefix=" $year-date$ " suffix=" $/year-date$ "/>
                <text prefix=" $volume$ " suffix=" $volume$ " variable="volume"/>
              </group>
            </group>
            <text form="short" prefix=" $page$ " suffix=" $page$ " variable="page"/>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=" $access$ . " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>