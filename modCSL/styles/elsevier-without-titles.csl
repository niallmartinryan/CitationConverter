<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>Elsevier (numeric, without titles)</title>
    <id>http://www.zotero.org/styles/elsevier-without-titles</id>
    <link href="http://www.zotero.org/styles/elsevier-without-titles" rel="self"/>
    <link href="http://www.elsevier.com/journals/the-journal-of-chemical-thermodynamics/0021-9614/guide-for-authors#68000" rel="documentation"/>
    <author>
      <name>Richard Karnesky</name>
      <email>karnesky+zotero@gmail.com</email>
      <uri>http://arc.nucapt.northwestern.edu/Richard_Karnesky</uri>
    </author>
    <category citation-format="numeric"/>
    <category field="generic-base"/>
    <summary>A style for many of Elsevier's journals that does not include article titles in the reference list</summary>
    <updated>2012-09-27T22:06:38+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <names variable="author">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="."/>
      <label form="short" prefix=", "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names variable="editor">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="."/>
      <label form="short" prefix=" (" suffix=")" text-case="capitalize-first"/>
    </names>
  </macro>
  <macro name="year-date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </if>
      <else>
        <text form="short" term="no date"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <text suffix=", " variable="publisher"/>
    <text suffix=", " variable="publisher-place"/>
    <text macro="year-date"/>
  </macro>
  <macro name="edition">
    <!--TODO: CSL should have low numeric be text (e.g. '3'->'third')-->
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text variable="edition"/>
      </else>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="," prefix="[" suffix="]">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography entry-spacing="0" second-field-align="flush">
    <layout suffix=".">
      <text prefix=" $citation-number$ [" suffix="] $citation-number$ " variable="citation-number"/>
      <text macro="author" prefix=" $author$ " suffix=",  $/author$ "/>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=", ">
            <text prefix=" $title$ " suffix=" $title$ " text-case="title" variable="title"/>
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <group delimiter=", ">
            <text prefix=" $$ " suffix=": $/$ " term="in"/>
            <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
            <text form="short" prefix=" $container-title$ " suffix=" $container-title$ " text-case="title" variable="container-title"/>
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
            <group delimiter=" ">
              <label form="short" variable="page"/>
              <text prefix=" $page$ " suffix=" $page$ " variable="page"/>
            </group>
          </group>
        </else-if>
        <else-if type="patent">
          <group delimiter=", ">
            <text prefix=" $title$ " suffix=" $title$ " text-case="title" variable="title"/>
            <text prefix=" $number$ " suffix=" $number$ " variable="number"/>
            <text macro="year-date" prefix=" $year-date$ " suffix=" $/year-date$ "/>
          </group>
        </else-if>
        <else-if type="thesis">
          <group delimiter=", ">
            <text prefix=" $title$ " suffix=" $title$ " text-case="title" variable="title"/>
            <text prefix=" $genre$ " suffix=" $genre$ " variable="genre"/>
            <text prefix=" $publisher$ " suffix=" $publisher$ " variable="publisher"/>
            <text macro="year-date" prefix=" $year-date$ " suffix=" $/year-date$ "/>
          </group>
        </else-if>
        <else>
          <group delimiter=" ">
            <text form="short" prefix=" $container-title$ " suffix=" $container-title$ " text-case="title" variable="container-title"/>
            <text prefix=" $volume$ " suffix=" $volume$ " variable="volume"/>
            <text macro="year-date" prefix=" $year-date$ (" suffix=") $/year-date$ "/>
            <text prefix=" $page$ " suffix=" $page$ " variable="page"/>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>