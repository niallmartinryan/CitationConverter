<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>Journal of Atrial Fibrillation</title>
    <title-short>JAFIB</title-short>
    <id>http://www.zotero.org/styles/journal-of-atrial-fibrillation</id>
    <link href="http://www.zotero.org/styles/journal-of-atrial-fibrillation" rel="self"/>
    <link href="http://www.jafib.com/index.php?cont=author_submit" rel="documentation"/>
    <author>
      <name>Dennis Lau</name>
    </author>
    <contributor>
      <name>Pawel Kuklik</name>
      <email>pawel.kuklik@gmail.com</email>
    </contributor>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <issn>1941-6911</issn>
    <summary>Style for the Journal of Atrial Fibrillation, no et al. and no month/issue number</summary>
    <updated>2013-03-08T13:20:16+01:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter=", " prefix="[" suffix="]">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography>
    <layout>
      <choose>
        <if type="article-journal">
          <text prefix=" $citation-number$ " suffix=".  $citation-number$ " variable="citation-number"/>
          <names suffix=". " variable="author">
            <name delimiter=", " form="long" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
          </names>
          <text prefix=" $title$ " suffix=".  $title$ " variable="title"/>
          <text form="short" prefix=" $container-title$ " suffix="  $container-title$ " variable="container-title"/>
          <date suffix="; " variable="issued">
            <date-part form="long" name="year"/>
          </date>
          <text prefix=" $volume$ " suffix=":  $volume$ " variable="volume"/>
          <text prefix=" $page$ " suffix=".  $page$ " variable="page"/>
        </if>
        <else-if type="book">
          <text prefix=" $citation-number$ " suffix=".  $citation-number$ " variable="citation-number"/>
          <names suffix=". " variable="author">
            <name delimiter=", " form="long" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
          </names>
          <text prefix=" $title$ " suffix=".  $title$ " variable="title"/>
          <text prefix=" $edition$ " suffix=".,  $edition$ " variable="edition"/>
          <text prefix=" $publisher-place$ " suffix=",  $publisher-place$ " variable="publisher-place"/>
          <text prefix=" $publisher$ " suffix=".  $publisher$ " variable="publisher">
          </text>
          <date suffix="; " variable="issued">
            <date-part form="long" name="year"/>
          </date>
          <text prefix=" $page$ " suffix=".  $page$ " variable="page"/>
        </else-if>
        <else-if type="chapter">
          <text prefix=" $citation-number$ " suffix=".  $citation-number$ " variable="citation-number"/>
          <names suffix=". " variable="author">
            <name delimiter=", " form="long" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
          </names>
          <text prefix=" $title$ " suffix=".  $title$ " variable="title"/>
          <text prefix=" $$ " suffix="  $/$ " value="In:"/>
          <names suffix=", " variable="editor">
            <name delimiter="," form="long" initialize-with=""/>
          </names>
          <text prefix=" $$ " suffix=".  $/$ " value="eds"/>
          <text prefix=" $collection-title$ " suffix=".  $collection-title$ " variable="collection-title"/>
          <text prefix=" $edition$ " suffix=".,  $edition$ " variable="edition"/>
          <text prefix=" $publisher-place$ " suffix=",  $publisher-place$ " variable="publisher-place"/>
          <text prefix=" $publisher$ " suffix=".  $publisher$ " variable="publisher">
          </text>
          <date suffix="; " variable="issued">
            <date-part form="long" name="year"/>
          </date>
          <text prefix=" $page$ " suffix=".  $page$ " variable="page"/>
        </else-if>
        <else-if type="webpage">
          <text prefix=" $citation-number$ " suffix=".  $citation-number$ " variable="citation-number"/>
          <names suffix=". " variable="author">
            <name delimiter=", " form="long" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
          </names>
          <text prefix=" $title$ " suffix=".  $title$ " variable="title"/>
          <text prefix=" $URL$ " suffix="  $URL$ " variable="URL"/>
          <date prefix="(accessed " suffix="). " variable="accessed">
            <date-part form="long" name="year"/>
            <date-part form="long" name="month"/>
          </date>
        </else-if>
        <else>
          <text prefix=" $citation-number$ " suffix=".  $citation-number$ " variable="citation-number"/>
          <names suffix=". " variable="author">
            <name delimiter=", " form="long" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
          </names>
          <text prefix=" $title$ " suffix=".  $title$ " variable="title"/>
          <text prefix=" $container-title$ " suffix="  $container-title$ " variable="container-title"/>
          <date suffix="; " variable="issued">
            <date-part form="long" name="year"/>
          </date>
          <text prefix=" $volume$ " suffix=":  $volume$ " variable="volume"/>
          <text prefix=" $page$ " suffix=".  $page$ " variable="page"/>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>