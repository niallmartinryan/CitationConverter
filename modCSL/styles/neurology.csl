<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" page-range-format="expanded" version="1.0">
  <info>
    <title>Neurology</title>
    <id>http://www.zotero.org/styles/neurology</id>
    <link href="http://www.zotero.org/styles/neurology" rel="self"/>
    <link href="http://www.zotero.org/styles/vancouver-superscript" rel="template"/>
    <link href="http://www.neurology.org/site/misc/auth2.xhtml#REFERENCESTYLE" rel="documentation"/>
    <author>
      <name>Charles Parnot</name>
      <uri>http://twitter.com/cparnot</uri>
      <email>charles.parnot@gmail.com</email>
    </author>
    <category citation-format="numeric"/>
    <category field="biology"/>
    <category field="medicine"/>
    <issn>0028-3878</issn>
    <eissn>1526-632X</eissn>
    <updated>2016-02-01T08:00:00+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <names variable="author">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="long" prefix=", "/>
      <substitute>
        <names variable="editor"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <group delimiter=": ">
      <choose>
        <if match="any" type="chapter paper-conference">
          <text term="in" text-case="capitalize-first"/>
        </if>
      </choose>
      <names suffix="." variable="editor">
        <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
        <label form="long" prefix=", "/>
      </names>
    </group>
  </macro>
  <macro name="publisher">
    <group delimiter=": " suffix=";">
      <choose>
        <if type="thesis">
          <text prefix="[" suffix="]" variable="publisher-place"/>
        </if>
        <else>
          <text variable="publisher-place"/>
        </else>
      </choose>
      <text variable="publisher"/>
    </group>
  </macro>
  <macro name="access">
    <choose>
      <if variable="URL">
        <group delimiter=": " suffix=".">
          <group delimiter=" ">
            <text term="accessed" text-case="capitalize-first"/>
            <text term="at"/>
          </group>
          <text variable="URL"/>
        </group>
        <text macro="accessed-date" prefix=" "/>
      </if>
    </choose>
  </macro>
  <macro name="accessed-date">
    <choose>
      <if variable="URL">
        <group delimiter=" ">
          <text term="accessed" text-case="capitalize-first"/>
          <date delimiter=" " suffix="." variable="accessed">
            <date-part form="long" name="month"/>
            <date-part name="day" suffix=","/>
            <date-part name="year"/>
          </date>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="container-title">
    <choose>
      <if match="any" type="article-journal">
        <group delimiter=" " suffix=".">
          <text form="short" strip-periods="true" variable="container-title"/>
          <choose>
            <if variable="URL">
              <text prefix="[" suffix="]" value="online serial"/>
            </if>
          </choose>
        </group>
        <text macro="edition" prefix=" "/>
      </if>
      <else-if match="any" type="article-magazine chapter paper-conference article-newspaper">
        <group delimiter=" " suffix=".">
          <text form="short" strip-periods="true" variable="container-title"/>
          <choose>
            <if variable="URL">
              <text prefix="[" suffix="]" term="online"/>
            </if>
          </choose>
        </group>
        <text macro="edition" prefix=" "/>
      </else-if>
      <!--add event-name and event-place once they become available-->
      <else-if match="any" type="bill legislation">
        <group delimiter=", ">
          <group delimiter=". ">
            <text form="short" strip-periods="true" variable="container-title"/>
            <group delimiter=" ">
              <text form="short" term="section" text-case="capitalize-first"/>
              <text variable="section"/>
            </group>
          </group>
          <text variable="number"/>
        </group>
      </else-if>
      <else>
        <text form="short" variable="container-title"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <text variable="title"/>
    <choose>
      <if match="none" type="article-journal article-magazine chapter paper-conference article-newspaper">
        <choose>
          <if variable="URL">
            <text prefix=" [" suffix="]" term="online"/>
          </if>
        </choose>
        <text macro="edition" prefix=". "/>
      </if>
    </choose>
    <choose>
      <if type="thesis">
        <text prefix=" [" suffix="]" variable="genre"/>
      </if>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="date">
    <choose>
      <if match="any" type="article-journal">
        <choose>
          <if variable="volume">
            <group delimiter=" " suffix=";">
              <date delimiter=" " variable="issued">
                <date-part name="year"/>
              </date>
            </group>
          </if>
          <else>
            <group prefix="Epub " suffix=".">
              <date delimiter=" " variable="issued">
                <date-part name="year"/>
                <date-part form="short" name="month" strip-periods="true"/>
                <date-part name="day"/>
              </date>
            </group>
          </else>
        </choose>
      </if>
      <else-if match="any" type="bill legislation">
        <group delimiter=", ">
          <date delimiter=" " variable="issued">
            <date-part form="short" name="month" strip-periods="true"/>
            <date-part name="day"/>
          </date>
          <date variable="issued">
            <date-part name="year"/>
          </date>
        </group>
      </else-if>
      <else-if type="report">
        <date delimiter=" " variable="issued">
          <date-part name="year"/>
          <date-part form="short" name="month" strip-periods="true"/>
        </date>
      </else-if>
      <else>
        <group suffix=".">
          <date variable="issued">
            <date-part name="year"/>
          </date>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="pages">
    <choose>
      <if match="any" type="article-journal article-magazine article-newspaper">
        <text prefix=":" variable="page"/>
      </if>
      <else>
        <text prefix=" p. " variable="page"/>
      </else>
    </choose>
  </macro>
  <macro name="journal-location">
    <choose>
      <if match="any" type="article-journal article-magazine">
        <text variable="volume"/>
      </if>
    </choose>
  </macro>
  <macro name="report-details">
    <choose>
      <if type="report">
        <text prefix="Report No.: " variable="number"/>
      </if>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="," vertical-align="sup">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography et-al-min="7" et-al-use-first="3" second-field-align="flush">
    <layout>
      <text prefix=" $citation-number$ " suffix=".  $citation-number$ " variable="citation-number"/>
      <group delimiter=". " suffix=". ">
        <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
        <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
      </group>
      <group delimiter=" " suffix=". ">
        <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
        <text macro="container-title" prefix=" $container-title$ " suffix=" $/container-title$ "/>
        <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
        <group>
          <text macro="date" prefix=" $date$ " suffix=" $/date$ "/>
          <text macro="journal-location" prefix=" $journal-location$ " suffix=" $/journal-location$ "/>
          <text macro="pages" prefix=" $pages$ " suffix=" $/pages$ "/>
        </group>
      </group>
      <text macro="report-details" prefix=" $report-details$ " suffix=".  $/report-details$ "/>
      <text macro="access" prefix=" $access$ " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>