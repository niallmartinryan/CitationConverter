<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="note" delimiter-precedes-last="always" demote-non-dropping-particle="sort-only" et-al-min="4" et-al-use-first="1" initialize-with=". " initialize-with-hyphen="true" name-delimiter=" – " name-form="long" names-delimiter=" – " page-range-format="expanded" sort-separator=", " version="1.0">
  <!-- Polyglot -->
  <info>
    <title>Pontifical Biblical Institute</title>
    <id>http://www.zotero.org/styles/pontifical-biblical-institute</id>
    <link href="http://www.zotero.org/styles/pontifical-biblical-institute" rel="self"/>
    <link href="http://www.biblico.it/pubblicazioni/sb36_bazylinski_engl.htm" rel="documentation"/>
    <author>
      <name>Devin Roza LC</name>
      <email>devin (.) roza (@) legionaries {.} org  (remove spaces and parenthesis/brackets but leave periods to get email)</email>
    </author>
    <category citation-format="note"/>
    <category field="theology"/>
    <summary>The Pontifical Biblical Institute (Pontificio Istituto Biblico) style</summary>
    <updated>2012-09-27T22:06:38+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale>
    <style-options punctuation-in-quote="false"/>
    <terms>
      <term name="et-al">et al.</term>
      <term name="ibid">ibidem</term>
      <term form="short" name="editor">
        <single>ed.</single>
        <multiple>ed.</multiple>
      </term>
      <term form="short" name="collection-editor">
        <single>ed.</single>
        <multiple>ed.</multiple>
      </term>
      <term name="page-range-delimiter">-</term>
    </terms>
  </locale>
  <macro name="author">
    <names variable="author">
      <name font-variant="small-caps"/>
    </names>
  </macro>
  <macro name="author-bibliography">
    <names variable="author">
      <name font-variant="small-caps" name-as-sort-order="all"/>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name font-variant="small-caps" form="short"/>
    </names>
  </macro>
  <macro name="reviewer">
    <choose>
      <if type="paper-conference">
        <names variable="translator">
          <name font-variant="small-caps"/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="reviewer-bibliography">
    <choose>
      <if type="paper-conference">
        <names variable="translator">
          <name font-variant="small-caps" name-as-sort-order="all"/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="reviewer-short">
    <choose>
      <if type="paper-conference">
        <names variable="translator">
          <name font-variant="small-caps" form="short"/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="editor">
    <choose>
      <if match="any" type="chapter entry entry-dictionary entry-encyclopedia paper-conference">
      </if>
      <else-if variable="editor">
        <names variable="editor">
          <name font-variant="small-caps"/>
          <et-al font-variant="normal"/>
          <label form="short" prefix=" (" strip-periods="false" suffix=")" text-case="lowercase"/>
        </names>
      </else-if>
      <else>
        <names font-variant="small-caps" variable="collection-editor">
          <name font-variant="small-caps"/>
          <et-al font-variant="normal"/>
          <label form="short" prefix=" (" strip-periods="false" suffix=")" text-case="lowercase"/>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="editor-bibliography">
    <choose>
      <if match="any" type="chapter entry entry-dictionary entry-encyclopedia paper-conference"/>
      <else-if variable="editor">
        <names variable="editor">
          <name font-variant="small-caps" name-as-sort-order="all"/>
          <et-al font-variant="normal"/>
          <label form="short" prefix=" (" strip-periods="false" suffix=")" text-case="lowercase"/>
        </names>
      </else-if>
      <else>
        <names font-variant="small-caps" variable="collection-editor">
          <name font-variant="small-caps" name-as-sort-order="all"/>
          <et-al font-variant="normal"/>
          <label form="short" prefix=" (" strip-periods="false" suffix=")" text-case="lowercase"/>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="editor-short">
    <choose>
      <if match="any" type="chapter entry entry-dictionary entry-encyclopedia paper-conference">
      </if>
      <else-if variable="editor">
        <names variable="editor">
          <name font-variant="small-caps"/>
          <et-al font-variant="normal"/>
          <label form="short" prefix=" (" strip-periods="false" suffix=")" text-case="lowercase"/>
        </names>
      </else-if>
      <else>
        <names variable="collection-editor">
          <name font-variant="small-caps"/>
          <et-al font-variant="normal"/>
          <label form="short" prefix=" (" strip-periods="false" suffix=")" text-case="lowercase"/>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="editors-when-author">
    <choose>
      <if type="manuscript">
        <names variable="author">
          <name font-variant="small-caps"/>
          <et-al font-variant="normal"/>
        </names>
      </if>
      <else-if variable="editor">
        <names variable="editor">
          <label form="short" strip-periods="false" suffix=" " text-case="lowercase"/>
          <name font-variant="small-caps"/>
          <et-al font-variant="normal"/>
        </names>
      </else-if>
      <else-if variable="collection-editor">
        <names variable="collection-editor">
          <label form="short" strip-periods="false" suffix=" " text-case="lowercase"/>
          <name font-variant="small-caps"/>
          <et-al font-variant="normal"/>
        </names>
      </else-if>
    </choose>
  </macro>
  <macro name="contributors">
    <choose>
      <if match="none" type="manuscript">
        <choose>
          <if variable="author">
            <text macro="author"/>
          </if>
          <else>
            <text macro="editor"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="contributors-bibliography">
    <choose>
      <if match="none" type="manuscript">
        <choose>
          <if variable="author">
            <text macro="author-bibliography"/>
          </if>
          <else>
            <text macro="editor-bibliography"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="contributors-short">
    <choose>
      <if match="none" type="manuscript">
        <choose>
          <if variable="author">
            <text macro="author-short"/>
          </if>
          <else>
            <text macro="editor-short"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="contributors-reviewers">
    <group delimiter=", ">
      <choose>
        <if type="paper-conference">
          <text macro="reviewer"/>
          <text suffix=" " variable="title"/>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="contributors-reviewers-bibliography">
    <group delimiter=", ">
      <choose>
        <if type="paper-conference">
          <text macro="reviewer-bibliography"/>
          <text suffix=" " variable="title"/>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="contributors-reviewers-short">
    <group delimiter=", ">
      <choose>
        <if type="paper-conference">
          <text macro="reviewer-short"/>
          <text suffix=" " variable="title"/>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="title-subtitle">
    <group delimiter=". ">
      <text variable="title"/>
      <text variable="archive"/>
    </group>
  </macro>
  <macro name="title">
    <group delimiter=". ">
      <choose>
        <if match="any" type="book thesis">
          <choose>
            <if variable="archive_location">
              <group delimiter=" ">
                <text font-style="italic" variable="title"/>
                <text variable="archive_location"/>
              </group>
            </if>
            <else>
              <text font-style="italic" variable="title"/>
              <text variable="archive"/>
            </else>
          </choose>
        </if>
        <else-if type="paper-conference">
          <choose>
            <if variable="event">
              <text quotes="true" variable="container-title"/>
            </if>
            <else>
              <text font-style="italic" variable="container-title"/>
              <group delimiter=" ">
                <text variable="archive"/>
                <text macro="editors-when-author" prefix="(" suffix=")"/>
              </group>
            </else>
          </choose>
        </else-if>
        <else-if type="manuscript">
          <group delimiter=" ">
            <text variable="title"/>
            <text variable="locator"/>
            <text macro="editors-when-author" prefix="(ed. " suffix=")"/>
          </group>
        </else-if>
        <else>
          <text macro="title-subtitle" quotes="true"/>
        </else>
      </choose>
    </group>
    <choose>
      <if match="all" type="book" variable="author">
        <text macro="editors-when-author" prefix=" (" suffix=")"/>
      </if>
    </choose>
    <choose>
      <if locator="chapter">
        <text prefix=", " variable="locator"/>
      </if>
    </choose>
  </macro>
  <macro name="title-short">
    <choose>
      <if match="any" type="book thesis">
        <text font-style="italic" form="short" variable="title"/>
      </if>
      <else-if type="paper-conference">
        <choose>
          <if variable="event">
            <text form="short" quotes="true" variable="title"/>
          </if>
          <else>
            <text font-style="italic" form="short" variable="title"/>
          </else>
        </choose>
      </else-if>
      <else-if type="manuscript">
        <group delimiter=" ">
          <text variable="title"/>
          <text variable="locator"/>
          <text macro="editors-when-author" prefix="(ed. " suffix=")"/>
        </group>
      </else-if>
      <else>
        <text form="short" quotes="true" variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="container-title">
    <choose>
      <if match="any" type="entry entry-dictionary entry-encyclopedia">
        <text font-style="italic" form="short" variable="container-title"/>
        <choose>
          <if variable="volume">
            <text prefix=" " variable="volume"/>
          </if>
          <else-if variable="collection-number">
            <text prefix=" " variable="collection-number"/>
          </else-if>
        </choose>
        <text value=","/>
      </if>
      <else-if match="any" type="chapter">
        <group delimiter=". ">
          <text font-style="italic" variable="container-title"/>
          <text variable="archive_location"/>
        </group>
        <text macro="editors-when-author" prefix=" (" suffix=")"/>
      </else-if>
      <else-if type="paper-conference">
        <choose>
          <if variable="event">
            <group delimiter=". ">
              <text font-style="italic" variable="event"/>
              <text variable="archive"/>
            </group>
            <text macro="editors-when-author" prefix=" (" suffix=")"/>
          </if>
        </choose>
      </else-if>
      <else-if match="any" type="article article-magazine article-newspaper article-journal">
        <text font-style="italic" form="short" variable="container-title"/>
        <choose>
          <if match="none" variable="note">
            <group delimiter="/" prefix=" ">
              <number form="numeric" variable="volume"/>
              <text variable="issue"/>
            </group>
          </if>
        </choose>
      </else-if>
    </choose>
  </macro>
  <macro name="container-review">
    <choose>
      <if type="paper-conference">
        <text prefix=", " variable="archive_location"/>
      </if>
    </choose>
  </macro>
  <macro name="collection-title">
    <group delimiter=" ">
      <text variable="collection-title"/>
      <text variable="collection-number"/>
    </group>
  </macro>
  <macro name="volume-number">
    <choose>
      <if match="any" type="book chapter">
        <choose>
          <if is-numeric="volume">
            <number form="roman" text-case="uppercase" variable="volume"/>
          </if>
          <else>
            <text variable="volume"/>
          </else>
        </choose>
        <choose>
          <if match="all" variable="volume locator">
            <text value=","/>
          </if>
        </choose>
      </if>
      <else-if type="paper-conference">
        <choose>
          <if match="none" position="subsequent">
            <text variable="volume"/>
          </if>
        </choose>
      </else-if>
    </choose>
  </macro>
  <macro name="number-of-volumes">
    <choose>
      <if match="any" type="book chapter paper-conference">
        <choose>
          <if match="none" variable="locator volume">
            <choose>
              <if is-numeric="number-of-volumes">
                <number form="roman" prefix=" I-" text-case="uppercase" variable="number-of-volumes"/>
              </if>
              <else>
                <text prefix=" " variable="number-of-volumes"/>
              </else>
            </choose>
          </if>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="publisher-place">
    <group delimiter=", ">
      <text variable="publisher-place"/>
    </group>
  </macro>
  <macro name="edition-note">
    <choose>
      <if match="any" type="book chapter paper-conference">
        <choose>
          <if is-numeric="edition">
            <group delimiter=" ">
              <number variable="edition"/>
            </group>
          </if>
          <else>
            <text variable="edition"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="issued">
    <choose>
      <if variable="note">
        <text variable="note"/>
      </if>
      <else-if match="any" type="book chapter thesis paper-conference">
        <choose>
          <if variable="issued">
            <text macro="edition-note" vertical-align="sup"/>
            <date variable="issued">
              <date-part name="year"/>
            </date>
          </if>
        </choose>
      </else-if>
      <else-if match="any" type="graphic report article-newspaper">
        <date variable="issued">
          <date-part name="day" suffix=" "/>
          <date-part name="month" suffix=" "/>
          <date-part name="year"/>
        </date>
      </else-if>
      <else>
        <text macro="edition-note" vertical-align="sup"/>
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </else>
    </choose>
  </macro>
  <macro name="issue-note">
    <choose>
      <if type="article-journal">
        <choose>
          <if variable="note">
            <text macro="issued"/>
          </if>
          <else>
            <text macro="issued" prefix="(" suffix=")"/>
          </else>
        </choose>
      </if>
      <else-if match="any" type="entry entry-dictionary entry-encyclopedia">
      </else-if>
      <else-if type="thesis">
        <group delimiter=" " prefix="(" suffix=")">
          <text variable="genre"/>
          <text suffix=";" variable="publisher"/>
          <text macro="publisher-place"/>
          <text macro="issued"/>
        </group>
      </else-if>
      <else-if match="any" variable="publisher-place issued note collection-title collection-number">
        <group delimiter="; " prefix="(" suffix=")">
          <group>
            <text macro="collection-title"/>
          </group>
          <group delimiter=" ">
            <text macro="publisher-place"/>
            <text macro="issued"/>
          </group>
        </group>
      </else-if>
      <else>
        <text macro="issued" prefix="(" suffix=")"/>
      </else>
    </choose>
  </macro>
  <macro name="pages">
    <text variable="page"/>
  </macro>
  <macro name="locator">
    <choose>
      <if locator="chapter" match="any" type="manuscript">
      </if>
      <else-if match="none" variable="publisher-place note collection-title collection-number issued volume number-of-volumes">
        <choose>
          <if position="first">
            <text prefix=", " variable="locator"/>
          </if>
          <else>
            <text variable="locator"/>
          </else>
        </choose>
      </else-if>
      <else>
        <text prefix=" " variable="locator"/>
      </else>
    </choose>
  </macro>
  <macro name="access-note">
    <choose>
      <if type="webpage">
        <text prefix=" " variable="URL"/>
        <group prefix=" [" suffix="]">
          <text term="accessed" text-case="lowercase"/>
          <date variable="accessed">
            <date-part name="month" prefix=" "/>
            <date-part name="day" prefix=" " suffix=","/>
            <date-part name="year" prefix=" "/>
          </date>
        </group>
      </if>
      <else-if match="all" variable="URL accessed">
        <text prefix=" " variable="URL"/>
        <choose>
          <if match="none" variable="issued note">
            <group prefix=" [" suffix="]">
              <text term="accessed" text-case="lowercase"/>
              <date variable="accessed">
                <date-part name="month" prefix=" "/>
                <date-part name="day" prefix=" " suffix=","/>
                <date-part name="year" prefix=" "/>
              </date>
            </group>
          </if>
        </choose>
      </else-if>
    </choose>
  </macro>
  <citation disambiguate-add-givenname="true" disambiguate-add-names="true" disambiguate-add-year-suffix="false" near-note-distance="0">
    <layout delimiter="; " suffix=".">
      <choose>
        <if position="ibid">
          <choose>
            <if position="near-note">
              <text font-style="italic" term="ibid" text-case="capitalize-first"/>
              <text macro="locator" prefix=", "/>
            </if>
            <else>
              <group delimiter=" ">
                <text macro="contributors-reviewers-short"/>
                <text macro="contributors-short" suffix=", "/>
              </group>
              <group delimiter=", ">
                <text macro="title-short"/>
                <choose>
                  <if type="paper-conference">
                    <text macro="volume-number"/>
                  </if>
                </choose>
              </group>
              <choose>
                <if locator="volume">
                  <text macro="locator" prefix=" "/>
                </if>
                <else>
                  <text macro="locator" prefix=", "/>
                </else>
              </choose>
            </else>
          </choose>
        </if>
        <else-if position="subsequent">
          <group delimiter=" ">
            <text macro="contributors-reviewers-short"/>
            <text macro="contributors-short" suffix=", "/>
          </group>
          <group delimiter=", ">
            <text macro="title-short"/>
            <choose>
              <if type="paper-conference">
                <text macro="volume-number"/>
              </if>
            </choose>
          </group>
          <choose>
            <if locator="volume">
              <text macro="locator" prefix=" "/>
            </if>
            <else>
              <text macro="locator" prefix=", "/>
            </else>
          </choose>
        </else-if>
        <else>
          <group delimiter=" ">
            <text macro="contributors-reviewers"/>
            <text macro="contributors" suffix=", "/>
          </group>
          <group delimiter=", ">
            <text macro="title"/>
            <text macro="container-title"/>
          </group>
          <group delimiter=" ">
            <text macro="issue-note" prefix=" "/>
          </group>
          <group delimiter=", ">
            <text macro="number-of-volumes"/>
            <text macro="volume-number" prefix=" "/>
          </group>
          <text macro="container-review"/>
          <group delimiter=", ">
            <text macro="pages" prefix=" "/>
            <text macro="locator"/>
            <text macro="access-note"/>
          </group>
        </else>
      </choose>
    </layout>
  </citation>
  <bibliography subsequent-author-substitute="––––">
    <sort>
      <key macro="contributors-reviewers-bibliography"/>
      <key macro="contributors-bibliography"/>
      <key variable="title"/>
    </sort>
    <layout suffix=".">
      <group delimiter=" ">
        <text macro="contributors-reviewers-bibliography" prefix=" $contributors-reviewers-bibliography$ " suffix=" $/contributors-reviewers-bibliography$ "/>
        <text macro="contributors-bibliography" prefix=" $contributors-bibliography$ " suffix=",  $/contributors-bibliography$ "/>
      </group>
      <group delimiter=", ">
        <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
        <text macro="container-title" prefix=" $container-title$ " suffix=" $/container-title$ "/>
      </group>
      <group delimiter=" ">
        <text macro="issue-note" prefix=" $issue-note$  " suffix=" $/issue-note$ "/>
      </group>
      <group delimiter=", ">
        <text macro="number-of-volumes" prefix=" $number-of-volumes$  " suffix=" $/number-of-volumes$ "/>
        <text macro="volume-number" prefix=" $volume-number$  " suffix=" $/volume-number$ "/>
      </group>
      <text macro="container-review" prefix=" $container-review$ " suffix=" $/container-review$ "/>
      <group delimiter=", ">
        <text macro="pages" prefix=" $pages$  " suffix=" $/pages$ "/>
        <text macro="access-note" prefix=" $access-note$ " suffix=" $/access-note$ "/>
      </group>
    </layout>
  </bibliography>
</style>