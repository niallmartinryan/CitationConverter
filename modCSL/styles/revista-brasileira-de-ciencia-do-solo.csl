<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="pt-BR" demote-non-dropping-particle="never" version="1.0">
  <info>
    <title>Revista Brasileira de Ciência do Solo (Portuguese - Brazil)</title>
    <title-short>RBCS</title-short>
    <id>http://www.zotero.org/styles/revista-brasileira-de-ciencia-do-solo</id>
    <link href="http://www.zotero.org/styles/revista-brasileira-de-ciencia-do-solo" rel="self"/>
    <link href="http://www.zotero.org/styles/vancouver-author-date" rel="template"/>
    <link href="http://www.scielo.br/revistas/rbcs/iinstruc.htm" rel="documentation"/>
    <author>
      <name>Fábio Satoshi Higashikawa</name>
      <email>rioleste@gmail.com</email>
    </author>
    <category citation-format="author-date"/>
    <category field="science"/>
    <issn>0100-0683</issn>
    <eissn>1806-9657</eissn>
    <summary>Brazilian Soil Science Journal style based on Vancouver-author-date style</summary>
    <updated>2015-02-16T16:50:41+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="pt-BR">
    <terms>
      <term form="short" name="month-01">jan</term>
      <term form="short" name="month-02">fev</term>
      <term form="short" name="month-03">mar</term>
      <term form="short" name="month-04">abr</term>
      <term form="short" name="month-05">maio</term>
      <term form="short" name="month-06">jun</term>
      <term form="short" name="month-07">jul</term>
      <term form="short" name="month-08">ago</term>
      <term form="short" name="month-09">set</term>
      <term form="short" name="month-10">out</term>
      <term form="short" name="month-11">nov</term>
      <term form="short" name="month-12">dez</term>
      <term form="short" name="editor">
        <single>ed</single>
        <multiple>eds</multiple>
      </term>
      <term form="short" name="container-author">
        <single>ed</single>
        <multiple>eds</multiple>
      </term>
      <term form="short" name="collection-editor">
        <single>ed</single>
        <multiple>eds</multiple>
      </term>
    </terms>
  </locale>
  <macro name="author">
    <names variable="author">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="long" prefix=", "/>
      <substitute>
        <names variable="editor"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <group delimiter=": ">
      <choose>
        <if match="any" type="chapter paper-conference">
          <text term="in" text-case="capitalize-first"/>
        </if>
      </choose>
      <names suffix="." variable="editor">
        <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
        <label form="long" prefix=", "/>
      </names>
    </group>
  </macro>
  <macro name="publisher">
    <group delimiter=": " suffix=";">
      <choose>
        <if type="thesis">
          <text prefix="[" suffix="]" variable="publisher-place"/>
        </if>
        <else>
          <text variable="publisher-place"/>
        </else>
      </choose>
      <text variable="publisher"/>
    </group>
  </macro>
  <macro name="access">
    <choose>
      <if variable="URL"/>
    </choose>
  </macro>
  <macro name="accessed-date">
    <choose>
      <if variable="URL"/>
    </choose>
  </macro>
  <macro name="container-title">
    <choose>
      <if match="any" type="article-journal article-magazine chapter paper-conference article-newspaper">
        <group delimiter=" " suffix=".">
          <text form="short" strip-periods="true" variable="container-title"/>
          <choose>
            <if variable="URL"/>
          </choose>
        </group>
        <text macro="edition" prefix=" "/>
      </if>
      <else-if match="any" type="bill legislation">
        <group delimiter=", ">
          <group delimiter=". ">
            <text form="short" variable="container-title"/>
            <group delimiter=" ">
              <text form="short" term="section" text-case="capitalize-first"/>
              <text variable="section"/>
            </group>
          </group>
          <text variable="number"/>
        </group>
      </else-if>
      <else>
        <text form="short" suffix="." variable="container-title"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <text variable="title"/>
    <choose>
      <if match="none" type="article-journal article-magazine chapter paper-conference article-newspaper">
        <choose>
          <if variable="URL">
            <text prefix=" [" suffix="]" term="internet" text-case="capitalize-first"/>
          </if>
        </choose>
        <text macro="edition" prefix=". "/>
      </if>
    </choose>
    <choose>
      <if type="thesis">
        <text prefix=" [" suffix="]" variable="genre"/>
      </if>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="date">
    <choose>
      <if match="any" type="article-journal article-magazine article-newspaper">
        <group delimiter=" " suffix=";">
          <date date-parts="year" form="text" variable="issued"/>
          <text macro="accessed-date"/>
        </group>
      </if>
      <else-if match="any" type="bill legislation">
        <group delimiter=", ">
          <date delimiter=" " variable="issued">
            <date-part form="short" name="month" strip-periods="true"/>
            <date-part name="day"/>
          </date>
          <date variable="issued">
            <date-part name="year"/>
          </date>
        </group>
      </else-if>
      <else-if type="report">
        <date delimiter=" " variable="issued">
          <date-part name="year"/>
          <date-part form="short" name="month" strip-periods="true"/>
        </date>
      </else-if>
      <else>
        <group suffix=".">
          <date date-parts="year" form="numeric" variable="issued">
            <date-part name="year"/>
          </date>
          <text macro="accessed-date" prefix=" "/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="pages">
    <choose>
      <if match="any" type="article-journal article-magazine article-newspaper">
        <text prefix=":" variable="page"/>
      </if>
      <else>
        <group delimiter=" " prefix=" ">
          <label form="short" plural="never" variable="page"/>
          <text variable="page"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="journal-location">
    <choose>
      <if match="any" type="article-journal article-magazine">
        <text variable="volume"/>
      </if>
    </choose>
  </macro>
  <macro name="report-details">
    <choose>
      <if type="report">
        <text prefix="Report No.: " variable="number"/>
      </if>
    </choose>
  </macro>
  <macro name="author-short">
    <names suffix="," variable="author">
      <name delimiter=" e " form="short" sort-separator=","/>
    </names>
  </macro>
  <macro name="issued">
    <date variable="issued">
      <date-part name="year"/>
    </date>
  </macro>
  <citation collapse="year" disambiguate-add-year-suffix="true" et-al-min="3" et-al-use-first="1">
    <sort>
      <key macro="issued"/>
      <key macro="author-short"/>
    </sort>
    <layout delimiter="; " prefix="(" suffix=")">
      <group delimiter=" ">
        <text macro="author-short"/>
        <text macro="issued"/>
      </group>
    </layout>
  </citation>
  <bibliography entry-spacing="1" line-spacing="1">
    <sort>
      <key macro="author"/>
      <key macro="issued"/>
      <key variable="title"/>
    </sort>
    <layout>
      <group delimiter=". " suffix=". ">
        <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
        <text macro="title" prefix=" $title$ " suffix=" $/title$ "/>
      </group>
      <group delimiter=" " suffix=". ">
        <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
        <text macro="container-title" prefix=" $container-title$ " suffix=" $/container-title$ "/>
        <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
        <group>
          <text macro="date" prefix=" $date$ " suffix=" $/date$ "/>
          <text macro="journal-location" prefix=" $journal-location$ " suffix=" $/journal-location$ "/>
          <text macro="pages" prefix=" $pages$ " suffix=" $/pages$ "/>
        </group>
      </group>
      <text macro="report-details" prefix=" $report-details$ " suffix=".  $/report-details$ "/>
      <text macro="access" prefix=" $access$ " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>