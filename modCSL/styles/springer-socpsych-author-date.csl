<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" demote-non-dropping-particle="never" version="1.0">
  <info>
    <title>Springer - SocPsych (author-date)</title>
    <id>http://www.zotero.org/styles/springer-socpsych-author-date</id>
    <link href="http://www.zotero.org/styles/springer-socpsych-author-date" rel="self"/>
    <link href="http://www.zotero.org/styles/apa" rel="template"/>
    <link href="http://www.springer.com/cda/content/document/cda_downloaddocument/instruct-authors-e.pdf" rel="documentation"/>
    <link href="http://www.springer.com/cda/content/document/cda_downloaddocument/manuscript-guidelines-1.0.pdf" rel="documentation"/>
    <!-- This style corresponds to 'Springer SocPsych' in the pdf document 'Key Style Points' at this url -->
    <link href="http://www.springer.com/cda/content/document/cda_downloaddocument/Key_Style_Points_1.0.pdf" rel="documentation"/>
    <author>
      <name>Charles Parnot</name>
      <email>charles.parnot@gmail.com</email>
      <uri>http://twitter.com/cparnot</uri>
    </author>
    <category citation-format="author-date"/>
    <category field="psychology"/>
    <category field="generic-base"/>
    <summary>This style corresponds to 'Springer SocPsych' in the pdf document 'Key Style Points' at this url</summary>
    <updated>2015-07-09T20:48:24+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <date form="text">
      <date-part name="day" suffix=" "/>
      <date-part name="month" suffix=" "/>
      <date-part name="year"/>
    </date>
    <terms>
      <term form="short" name="editortranslator">
        <single>ed. &amp; trans.</single>
        <multiple>eds. &amp; trans.</multiple>
      </term>
      <term form="short" name="translator">
        <single>trans.</single>
        <multiple>trans.</multiple>
      </term>
    </terms>
  </locale>
  <locale xml:lang="de">
    <terms>
      <term name="et-al">et al</term>
      <term form="long" name="accessed">Zugegriffen:</term>
    </terms>
  </locale>
  <macro name="container-contributors">
    <choose>
      <if match="any" type="chapter paper-conference">
        <names delimiter=", " suffix=", " variable="editor translator">
          <name and="symbol" delimiter=", " initialize-with=". "/>
          <label form="short" prefix=" (" suffix=")" text-case="title"/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="secondary-contributors">
    <choose>
      <if match="none" type="article-journal chapter paper-conference">
        <names delimiter=", " prefix=" (" suffix=")" variable="translator editor">
          <name and="symbol" delimiter=", " initialize-with=". "/>
          <label form="short" prefix=", " text-case="title"/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="author">
    <names variable="author">
      <name and="symbol" delimiter=", " delimiter-precedes-last="always" initialize-with=". " name-as-sort-order="all" sort-separator=", "/>
      <label form="short" prefix=" (" suffix=")" text-case="capitalize-first"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if type="report">
            <text variable="publisher"/>
            <text macro="title"/>
          </if>
          <else>
            <text macro="title"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="text" delimiter=", " form="short" initialize-with=". "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if type="report">
            <text variable="publisher"/>
            <text font-style="italic" form="short" variable="title"/>
          </if>
          <else-if match="any" type="bill book graphic legal_case legislation motion_picture song">
            <text font-style="italic" form="short" variable="title"/>
          </else-if>
          <else>
            <text form="short" quotes="true" variable="title"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <choose>
      <if type="thesis">
        <choose>
          <if match="any" variable="archive">
            <group>
              <text suffix=" " term="retrieved" text-case="capitalize-first"/>
              <text suffix=" " term="from"/>
              <text suffix="." variable="archive"/>
              <text prefix=" (" suffix=")" variable="archive_location"/>
            </group>
          </if>
          <else>
            <group>
              <text suffix=" " term="retrieved" text-case="capitalize-first"/>
              <text suffix=" " term="from"/>
              <text variable="URL"/>
            </group>
          </else>
        </choose>
      </if>
      <else>
        <choose>
          <if variable="DOI">
            <text prefix="doi:" variable="DOI"/>
          </if>
          <else>
            <group delimiter=". ">
              <text variable="URL"/>
              <group delimiter=" ">
                <text term="accessed" text-case="capitalize-first"/>
                <date form="text" variable="accessed"/>
              </group>
            </group>
          </else>
        </choose>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if match="any" type="report thesis">
        <text font-style="italic" variable="title"/>
        <group delimiter=" " prefix=" (" suffix=")">
          <text variable="genre"/>
          <text prefix="No. " variable="number"/>
        </group>
      </if>
      <else-if match="any" type="book graphic  motion_picture report song manuscript speech">
        <text font-style="italic" variable="title"/>
      </else-if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <choose>
      <if match="any" type="report">
        <group delimiter=": ">
          <text variable="publisher-place"/>
          <text variable="publisher"/>
        </group>
      </if>
      <else-if match="any" type="thesis">
        <group delimiter=", ">
          <text variable="publisher"/>
          <text variable="publisher-place"/>
        </group>
      </else-if>
      <else>
        <group delimiter=", ">
          <choose>
            <if match="none" variable="event">
              <text variable="genre"/>
            </if>
          </choose>
          <choose>
            <if match="none" type="article-journal article-magazine">
              <group delimiter=": ">
                <text variable="publisher-place"/>
                <text variable="publisher"/>
              </group>
            </if>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="event">
    <choose>
      <if variable="event">
        <choose>
          <if match="none" variable="genre">
            <text suffix=" " term="presented at" text-case="capitalize-first"/>
            <text variable="event"/>
          </if>
          <else>
            <group delimiter=" ">
              <text text-case="capitalize-first" variable="genre"/>
              <text term="presented at"/>
              <text variable="event"/>
            </group>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="issued">
    <choose>
      <if match="none" type="bill legal_case legislation">
        <choose>
          <if variable="issued">
            <group prefix=" (" suffix=")">
              <date variable="issued">
                <date-part name="year"/>
              </date>
              <text variable="year-suffix"/>
              <choose>
                <if match="none" type="article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song">
                  <date variable="issued">
                    <date-part name="month" prefix=", "/>
                    <date-part name="day" prefix=" "/>
                  </date>
                </if>
              </choose>
            </group>
          </if>
          <else>
            <group prefix=" (" suffix=")">
              <text form="short" term="no date"/>
              <text prefix="-" variable="year-suffix"/>
            </group>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="issued-sort">
    <choose>
      <if match="none" type="article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song">
        <date variable="issued">
          <date-part name="year"/>
          <date-part name="month"/>
          <date-part name="day"/>
        </date>
      </if>
      <else>
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </else>
    </choose>
  </macro>
  <macro name="issued-year">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year"/>
        </date>
        <text variable="year-suffix"/>
      </if>
      <else>
        <text form="short" term="no date"/>
        <text prefix="-" variable="year-suffix"/>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="locators">
    <choose>
      <if match="any" type="article-journal article-magazine">
        <group delimiter=", " prefix=", ">
          <group>
            <text font-style="italic" variable="volume"/>
            <text prefix="(" suffix=")" variable="issue"/>
          </group>
          <text variable="page"/>
        </group>
      </if>
      <else-if type="article-newspaper">
        <group delimiter=" " prefix=", ">
          <label form="short" variable="page"/>
          <text variable="page"/>
        </group>
      </else-if>
      <else-if match="any" type="book graphic motion_picture report song chapter paper-conference">
        <group delimiter=", " prefix=" (" suffix=")">
          <text macro="edition"/>
          <group>
            <text form="short" plural="true" suffix=" " term="volume" text-case="capitalize-first"/>
            <number form="numeric" prefix="1-" variable="number-of-volumes"/>
          </group>
          <group>
            <text form="short" suffix=" " term="volume" text-case="capitalize-first"/>
            <number form="numeric" variable="volume"/>
          </group>
          <group>
            <label form="short" suffix=" " variable="page"/>
            <text variable="page"/>
          </group>
        </group>
      </else-if>
      <else-if type="legal_case">
        <group delimiter=" " prefix=" (" suffix=")">
          <text variable="authority"/>
          <date form="text" variable="issued"/>
        </group>
      </else-if>
      <else-if match="any" type="bill legislation">
        <date prefix=" (" suffix=")" variable="issued">
          <date-part name="year"/>
        </date>
      </else-if>
    </choose>
  </macro>
  <macro name="citation-locator">
    <group>
      <choose>
        <if locator="chapter">
          <label form="long" text-case="capitalize-first" variable="locator"/>
        </if>
        <else>
          <label form="short" variable="locator"/>
        </else>
      </choose>
      <text prefix=" " variable="locator"/>
    </group>
  </macro>
  <macro name="container">
    <group>
      <choose>
        <if match="any" type="chapter paper-conference entry-encyclopedia">
          <text suffix=" " term="in" text-case="capitalize-first"/>
        </if>
      </choose>
      <text macro="container-contributors"/>
      <text macro="secondary-contributors"/>
      <text macro="container-title"/>
    </group>
  </macro>
  <macro name="container-title">
    <choose>
      <if match="none" type="bill legal_case legislation">
        <text font-style="italic" variable="container-title"/>
      </if>
      <else>
        <group delimiter=" " prefix=", ">
          <choose>
            <if variable="container-title">
              <text variable="volume"/>
              <text variable="container-title"/>
              <group delimiter=" ">
                <!--change to label variable="section" as that becomes available -->
                <text form="symbol" term="section"/>
                <text variable="section"/>
              </group>
              <text variable="page"/>
            </if>
            <else>
              <choose>
                <if type="legal_case">
                  <text prefix="No. " variable="number"/>
                </if>
                <else>
                  <text prefix="Pub. L. No. " variable="number"/>
                  <group delimiter=" ">
                    <!--change to label variable="section" as that becomes available -->
                    <text form="symbol" term="section"/>
                    <text variable="section"/>
                  </group>
                </else>
              </choose>
            </else>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <citation collapse="year" disambiguate-add-givenname="true" disambiguate-add-names="true" disambiguate-add-year-suffix="true" et-al-min="3" et-al-use-first="1" givenname-disambiguation-rule="primary-name">
    <sort>
      <key macro="author"/>
      <key macro="issued-sort"/>
    </sort>
    <layout delimiter="; " prefix="(" suffix=")">
      <group delimiter=", ">
        <group delimiter=" ">
          <text macro="author-short"/>
          <text macro="issued-year"/>
        </group>
        <text macro="citation-locator"/>
      </group>
    </layout>
  </citation>
  <bibliography entry-spacing="0" et-al-min="8" et-al-use-first="6" hanging-indent="true" line-spacing="2">
    <sort>
      <key macro="author"/>
      <key macro="issued-sort" sort="ascending"/>
    </sort>
    <layout>
      <group suffix=".">
        <group delimiter=". ">
          <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
          <text macro="issued" prefix=" $issued$ " suffix=" $/issued$ "/>
          <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
          <text macro="container" prefix=" $container$ " suffix=" $/container$ "/>
        </group>
        <text macro="locators" prefix=" $locators$ " suffix=" $/locators$ "/>
        <group delimiter=", " prefix=". ">
          <text macro="event" prefix=" $event$ " suffix=" $/event$ "/>
          <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
        </group>
      </group>
      <text macro="access" prefix=" $access$  " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>