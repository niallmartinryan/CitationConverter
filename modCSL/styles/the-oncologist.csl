<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>The Oncologist</title>
    <id>http://www.zotero.org/styles/the-oncologist</id>
    <link href="http://www.zotero.org/styles/the-oncologist" rel="self"/>
    <link href="http://www.zotero.org/styles/the-lancet" rel="template"/>
    <link href="http://theoncologist.alphamedpress.org/site/misc/TO_references.pdf" rel="documentation"/>
    <author>
      <name>Sebastian Karcher</name>
    </author>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <issn>1083-7159</issn>
    <eissn>1549-490X</eissn>
    <summary>Vancouver style adapted for The Oncologist</summary>
    <updated>2012-09-27T22:06:38+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <names suffix=". " variable="author">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="short" prefix=", "/>
      <substitute>
        <names variable="editor"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names suffix=". " variable="editor">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="short" prefix=", "/>
    </names>
  </macro>
  <macro name="publisher">
    <text variable="publisher-place"/>
    <text prefix=": " variable="publisher"/>
    <group prefix=", ">
      <date variable="issued">
        <date-part name="year"/>
      </date>
    </group>
  </macro>
  <macro name="access">
    <choose>
      <if match="none" variable="page">
        <choose>
          <if variable="DOI">
            <text prefix=" doi:" variable="DOI"/>
          </if>
          <else-if variable="URL">
            <text prefix="Available at " variable="URL"/>
            <group delimiter=" " prefix=". " suffix=".">
              <text term="accessed" text-case="capitalize-first"/>
              <date variable="accessed">
                <date-part name="month" prefix=" "/>
                <date-part name="day" prefix=" "/>
                <date-part name="year" prefix=", "/>
              </date>
            </group>
          </else-if>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="journal-title">
    <choose>
      <if match="any" type="article-journal article-magazine">
        <text form="short" strip-periods="true" variable="container-title"/>
      </if>
      <else>
        <text form="short" suffix=". " variable="container-title"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if type="book">
        <group delimiter=", ">
          <text variable="title"/>
          <text macro="edition"/>
        </group>
      </if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="," vertical-align="sup">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography et-al-min="4" et-al-use-first="3" second-field-align="flush">
    <layout suffix=".">
      <text prefix=" $citation-number$ " suffix=" $citation-number$ " variable="citation-number"/>
      <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
      <text macro="title" prefix=" $title$ " suffix=".  $/title$ "/>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=" " prefix=" ">
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
          <text prefix=" $number-of-pages$ : 1-" suffix=" $number-of-pages$ " variable="number-of-pages"/>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <group prefix=" " suffix=". ">
            <text prefix=" $$ " suffix=":  $/$ " term="in" text-case="capitalize-first"/>
            <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
            <group delimiter=", ">
              <text prefix=" $container-title$ " suffix=" $container-title$ " variable="container-title"/>
              <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            </group>
          </group>
          <group delimiter=": " prefix=" " suffix=".">
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
            <text prefix=" $page$ " suffix=". $page$ " variable="page"/>
          </group>
        </else-if>
        <else>
          <text macro="journal-title" prefix=" $journal-title$ " suffix=" $/journal-title$ "/>
          <group suffix=".">
            <date prefix=" " variable="issued">
              <date-part name="year"/>
            </date>
            <group prefix=";">
              <text prefix=" $volume$ " suffix=" $volume$ " variable="volume"/>
              <text prefix=" $page$ :" suffix=" $page$ " variable="page"/>
            </group>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=" $access$ " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>