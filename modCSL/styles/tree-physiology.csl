<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-GB" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>Tree Physiology</title>
    <id>http://www.zotero.org/styles/tree-physiology</id>
    <link href="http://www.zotero.org/styles/tree-physiology" rel="self"/>
    <link href="http://www.zotero.org/styles/journal-of-pollination-ecology" rel="template"/>
    <link href="http://www.oxfordjournals.org.turing.library.northwestern.edu/our_journals/treephys/for_authors/" rel="documentation"/>
    <author>
      <name>Sebastian Karcher</name>
    </author>
    <category citation-format="author-date"/>
    <category field="biology"/>
    <issn>0829-318X</issn>
    <eissn>1758-4469</eissn>
    <updated>2012-09-27T22:06:38+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale>
    <terms>
      <term form="short" name="edition">edn.</term>
    </terms>
  </locale>
  <macro name="editor-translator">
    <names delimiter=", " variable="editor translator">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="short" prefix=" (" strip-periods="true" suffix=")"/>
    </names>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="author">
    <names variable="author">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="short" prefix=" (" strip-periods="true" suffix=")"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text macro="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="text" delimiter=", " form="short" initialize-with=""/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text variable="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="title">
    <text variable="title"/>
  </macro>
  <macro name="publisher">
    <group delimiter=", ">
      <text variable="publisher"/>
      <text variable="publisher-place"/>
    </group>
  </macro>
  <macro name="access">
    <choose>
      <if variable="URL">
        <text variable="URL"/>
        <group prefix=" (" suffix=").">
          <date variable="accessed">
            <date-part name="day" suffix=" "/>
            <date-part name="month" suffix=" "/>
            <date-part name="year"/>
          </date>
          <text prefix=", date last " suffix=" " term="accessed"/>
        </group>
      </if>
    </choose>
  </macro>
  <citation collapse="year" disambiguate-add-names="true" disambiguate-add-year-suffix="true" et-al-min="3" et-al-use-first="1">
    <sort>
      <key variable="issued"/>
      <key variable="author"/>
    </sort>
    <layout delimiter=", " prefix="(" suffix=")">
      <group delimiter=" ">
        <text macro="author-short"/>
        <date variable="issued">
          <date-part name="year"/>
        </date>
        <group>
          <text prefix=" " variable="locator"/>
        </group>
      </group>
    </layout>
  </citation>
  <bibliography hanging-indent="true">
    <sort>
      <key macro="author-short"/>
      <key variable="issued"/>
    </sort>
    <layout>
      <text macro="author" prefix=" $author$ " suffix=" $/author$ "/>
      <date prefix=" (" suffix=")" variable="issued">
        <date-part name="year"/>
      </date>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=", " suffix=".">
            <group delimiter=" ">
              <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
              <text macro="editor-translator" prefix=" $editor-translator$ " suffix=" $/editor-translator$ "/>
            </group>
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
          </group>
          <text macro="publisher" prefix=" $publisher$  " suffix=". $/publisher$ "/>
        </if>
        <else-if type="thesis">
          <group delimiter=". ">
            <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
            <group delimiter=", ">
              <text prefix=" $genre$ " suffix=" $genre$ " variable="genre"/>
              <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
            </group>
          </group>
        </else-if>
        <else-if match="any" type="chapter paper-conference">
          <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
          <group prefix=". ">
            <text prefix=" $$ " suffix=":  $/$ " term="in" text-case="capitalize-first"/>
            <text macro="editor-translator" prefix=" $editor-translator$ " suffix=" $/editor-translator$ "/>
            <group delimiter=", " prefix=" " suffix=".">
              <text prefix=" $container-title$ " suffix=" $container-title$ " variable="container-title"/>
              <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            </group>
            <group delimiter=", ">
              <text macro="publisher" prefix=" $publisher$  " suffix=" $/publisher$ "/>
              <group delimiter=" ">
                <label form="short" strip-periods="true" variable="page"/>
                <text prefix=" $page$ " suffix=". $page$ " variable="page"/>
              </group>
            </group>
          </group>
        </else-if>
        <else>
          <group suffix=".">
            <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
            <text macro="editor-translator" prefix=" $editor-translator$  " suffix=" $/editor-translator$ "/>
          </group>
          <group prefix=" ">
            <text form="short" prefix=" $container-title$ " strip-periods="true" suffix=" $container-title$ " variable="container-title"/>
            <group prefix=" ">
              <text prefix=" $volume$ " suffix=" $volume$ " variable="volume"/>
            </group>
            <text prefix=" $page$ :" suffix=". $page$ " variable="page"/>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=" $access$ . " suffix=" $/access$ "/>
    </layout>
  </bibliography>
</style>