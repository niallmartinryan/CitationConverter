<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>Water Science &amp; Technology</title>
    <id>http://www.zotero.org/styles/water-science-and-technology</id>
    <link href="http://www.zotero.org/styles/water-science-and-technology" rel="self"/>
    <link href="http://www.iwaponline.com/wst/i2a.htm" rel="documentation"/>
    <author>
      <name>Julian Onions</name>
      <email>julian.onions@gmail.com</email>
    </author>
    <contributor>
      <name>Bruce D'Arcus</name>
    </contributor>
    <category citation-format="author-date"/>
    <category field="engineering"/>
    <issn>0273-1223</issn>
    <summary>Style for Water Science and Technology.</summary>
    <updated>2012-09-27T22:06:38+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="editor">
    <names delimiter=", " prefix="(" suffix=")" variable="editor">
      <name and="text" delimiter=", " initialize-with=". "/>
      <label form="short" prefix=", "/>
    </names>
  </macro>
  <macro name="author">
    <names variable="author">
      <name and="text" delimiter=", " initialize-with=". " name-as-sort-order="all" sort-separator=", "/>
      <label form="short" prefix=" (" suffix=")"/>
      <substitute>
        <names variable="editor"/>
        <text macro="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="text" delimiter=", " form="short" initialize-with=". "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
            <text font-style="italic" form="short" variable="title"/>
          </if>
          <else>
            <text form="short" variable="title"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <choose>
      <if variable="URL">
        <text prefix="[" suffix="] " term="online"/>
        <text variable="URL"/>
        <group prefix=" (" suffix=")">
          <text suffix=" " term="accessed" text-case="capitalize-first"/>
          <date variable="accessed">
            <date-part name="month" suffix=" "/>
            <date-part name="day" suffix=", "/>
            <date-part name="year"/>
          </date>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
        <text font-style="italic" variable="title"/>
      </if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=", ">
      <text variable="publisher-place"/>
      <text variable="publisher"/>
    </group>
  </macro>
  <citation disambiguate-add-givenname="true" disambiguate-add-names="true" disambiguate-add-year-suffix="true" et-al-min="3" et-al-use-first="2">
    <layout delimiter="; " prefix="(" suffix=")">
      <group delimiter=", ">
        <text macro="author-short"/>
        <date variable="issued">
          <date-part name="year"/>
        </date>
        <group>
          <label form="short" variable="locator"/>
          <text prefix=" " variable="locator"/>
        </group>
      </group>
    </layout>
  </citation>
  <bibliography hanging-indent="true">
    <sort>
      <key macro="author"/>
      <key variable="title"/>
    </sort>
    <layout>
      <text macro="author" prefix=" $author$ " suffix="  $/author$ "/>
      <date prefix="(" suffix=")" variable="issued">
        <date-part name="year"/>
      </date>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group suffix=",">
            <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
            <text macro="editor" prefix=" $editor$  " suffix=" $/editor$ "/>
          </group>
          <text macro="publisher" prefix=" $publisher$  " suffix=". $/publisher$ "/>
        </if>
        <else-if match="any" type="chapter paper-conference">
          <text macro="title" prefix=" $title$  " quotes="true" suffix=" $/title$ "/>
          <group prefix=" ">
            <text prefix=" $$ " suffix=" $/$ " term="in"/>
            <names delimiter=", " prefix=" " suffix="," variable="editor">
              <name and="text" initialize-with=". " sort-separator=", "/>
              <label form="short" prefix=" (" suffix=")"/>
            </names>
            <text prefix=" $container-title$  " suffix=". $container-title$ " variable="container-title"/>
            <text prefix=" $collection-title$  " suffix=". $collection-title$ " variable="collection-title"/>
            <group suffix=".">
              <text macro="publisher" prefix=" $publisher$  " suffix=" $/publisher$ "/>
              <group prefix=", ">
                <text prefix=" $page$ " suffix=" $page$ " variable="page"/>
              </group>
            </group>
          </group>
        </else-if>
        <else>
          <group suffix=".">
            <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
            <text macro="editor" prefix=" $editor$  " suffix=" $/editor$ "/>
          </group>
          <group prefix=" " suffix=".">
            <text prefix=" $container-title$ " suffix=" $container-title$ " variable="container-title"/>
            <group prefix=", ">
              <text font-weight="bold" prefix=" $volume$ " suffix=" $volume$ " variable="volume"/>
              <text prefix=" $issue$ (" suffix=") $issue$ " variable="issue"/>
            </group>
            <group prefix=", ">
              <text prefix=" $page$ " suffix=" $page$ " variable="page"/>
            </group>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=" $access$  " suffix=". $/access$ "/>
    </layout>
  </bibliography>
</style>