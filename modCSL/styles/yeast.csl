<?xml version="1.0" encoding="utf-8" standalone="no"?><style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" demote-non-dropping-particle="sort-only" version="1.0">
  <info>
    <title>Yeast</title>
    <id>http://www.zotero.org/styles/yeast</id>
    <link href="http://www.zotero.org/styles/yeast" rel="self"/>
    <link href="http://onlinelibrary.wiley.com/journal/10.1002/%28ISSN%291097-0061/homepage/ForAuthors.html" rel="documentation"/>
    <author>
      <name>Julian Onions</name>
      <email>julian.onions@gmail.com</email>
    </author>
    <category citation-format="author-date"/>
    <category field="biology"/>
    <issn>0749-503X</issn>
    <eissn>1097-0061</eissn>
    <summary>Style for the journal Yeast</summary>
    <updated>2012-09-27T22:06:38+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="anon">
    <text form="short" strip-periods="true" term="anonymous" text-case="capitalize-first"/>
  </macro>
  <macro name="editor">
    <names variable="editor">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="short" prefix=". (" strip-periods="true" suffix=")"/>
    </names>
  </macro>
  <macro name="author">
    <names variable="author">
      <name delimiter=", " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="short" prefix=". (" strip-periods="true" suffix=")."/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name and="text" delimiter=", " form="short"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text macro="anon"/>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <choose>
      <if variable="URL">
        <text variable="URL"/>
        <group prefix=" (" suffix=")">
          <text suffix=" " term="accessed" text-case="capitalize-first"/>
          <date variable="accessed">
            <date-part name="month" suffix=" "/>
            <date-part name="day" suffix=", "/>
            <date-part name="year"/>
          </date>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if type="thesis">
        <text variable="title"/>
      </if>
      <else-if match="any" type="bill book graphic legal_case legislation motion_picture report song">
        <text font-style="italic" variable="title"/>
      </else-if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=": ">
      <text variable="publisher"/>
      <text variable="publisher-place"/>
    </group>
  </macro>
  <macro name="year-date">
    <date variable="issued">
      <date-part name="year"/>
    </date>
  </macro>
  <macro name="day-month">
    <date variable="issued">
      <date-part name="month"/>
      <date-part name="day" prefix=" "/>
    </date>
  </macro>
  <macro name="page">
    <text variable="page"/>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number form="ordinal" variable="edition"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text suffix="." variable="edition"/>
      </else>
    </choose>
  </macro>
  <citation collapse="year" disambiguate-add-givenname="true" disambiguate-add-names="true" disambiguate-add-year-suffix="true" et-al-min="10" et-al-subsequent-min="6" et-al-subsequent-use-first="1" et-al-use-first="1">
    <layout delimiter="; " prefix="[" suffix="]">
      <group delimiter=", ">
        <group delimiter=" ">
          <text macro="author-short"/>
          <text macro="year-date"/>
        </group>
        <text variable="locator"/>
      </group>
    </layout>
  </citation>
  <bibliography et-al-min="10" et-al-use-first="1" hanging-indent="true">
    <sort>
      <key macro="author"/>
      <key variable="title"/>
    </sort>
    <layout suffix=".">
      <group delimiter=" ">
        <text macro="author" prefix=" $author$ " suffix=". $/author$ "/>
        <text macro="year-date" prefix=" $year-date$ " suffix=". $/year-date$ "/>
      </group>
      <choose>
        <if match="any" type="article-newspaper article-magazine">
          <group delimiter=" ">
            <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
          </group>
          <group delimiter=", " prefix=" ">
            <text font-style="italic" prefix=" $container-title$ " suffix=" $container-title$ " variable="container-title"/>
            <text macro="day-month" prefix=" $day-month$ " suffix=" $/day-month$ "/>
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
          </group>
        </if>
        <else-if type="thesis">
          <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
          <group delimiter=", " prefix=" ">
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="editor" prefix=" $editor$ " suffix=". $/editor$ "/>
            <text prefix=" $genre$ " suffix=" $genre$ " variable="genre"/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </else-if>
        <else-if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=" ">
            <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
            <text macro="edition" prefix=" $edition$ " suffix=" $/edition$ "/>
            <text macro="editor" prefix=" $editor$ " suffix=". $/editor$ "/>
            <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
          </group>
        </else-if>
        <else-if match="any" type="chapter paper-conference">
          <group delimiter=" ">
            <text macro="title" prefix=" $title$  " suffix=". $/title$ "/>
            <group delimiter=" ">
              <group delimiter=" " suffix=".">
                <text prefix=" $$ " suffix=" $/$ " term="in" text-case="capitalize-first"/>
                <text font-style="italic" prefix=" $container-title$ " suffix=", $container-title$ " variable="container-title"/>
                <text macro="editor" prefix=" $editor$ " suffix=" $/editor$ "/>
              </group>
              <group delimiter="; ">
                <text macro="publisher" prefix=" $publisher$ " suffix=" $/publisher$ "/>
                <text macro="page" prefix=" $page$ " suffix=" $/page$ "/>
              </group>
            </group>
          </group>
        </else-if>
        <else>
          <group suffix=".">
            <text macro="title" prefix=" $title$  " suffix=" $/title$ "/>
            <text macro="editor" prefix=" $editor$  " suffix=" $/editor$ "/>
          </group>
          <group delimiter=", " prefix=" " suffix=".">
            <text font-style="italic" form="short" prefix=" $container-title$ " suffix=" $container-title$ " variable="container-title"/>
            <group delimiter=": ">
              <text font-weight="bold" prefix=" $volume$ " suffix=" $volume$ " variable="volume"/>
              <text prefix=" $page$ " suffix=" $page$ " variable="page"/>
            </group>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=" $access$  " suffix=". $/access$ "/>
    </layout>
  </bibliography>
</style>